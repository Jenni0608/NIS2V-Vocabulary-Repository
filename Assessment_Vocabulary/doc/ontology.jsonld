  "http://www.w3.org/2004/02/skos/core#definition" : [ {
    "@language" : "en",
    "@value" : "Information stored on, processed by, or accessible via user endpoint devices is fully protected, ensuring confidentiality, integrity, and availability through appropriate security measures and controls."
  } ],
  "http://www.w3.org/2004/02/skos/core#prefLabel" : [ {
    "@language" : "en",
    "@value" : "MCQ.94 - Fully Implemented"
  } ]
}, {
  "@id" : "http://JP_ontology.org/assessment/MCQ95AnswerA",
  "@type" : [ "http://www.w3.org/2002/07/owl#NamedIndividual" ],
  "http://www.w3.org/1999/02/22-rdf-syntax-ns#type" : [ {
