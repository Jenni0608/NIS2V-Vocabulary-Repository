  "http://www.w3.org/2004/02/skos/core#definition" : [ {
    "@language" : "en",
    "@value" : "The organisation has established and maintains regular contact with various special interest groups, security forums, and professional associations relevant to information security, fostering collaboration and knowledge sharing within the industry."
  } ],
  "http://www.w3.org/2004/02/skos/core#prefLabel" : [ {
    "@value" : "MCQ14. Fully Implemented"
  } ]
