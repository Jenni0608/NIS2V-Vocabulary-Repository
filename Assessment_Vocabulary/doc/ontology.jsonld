  } ],
  "http://www.w3.org/2004/02/skos/core#definition" : [ {
    "@language" : "en",
    "@value" : "There are some efforts to review the policy at planned intervals or if significant changes occur. However, these reviews may not be consistently conducted or may lack thoroughness in assessing the policy's relevance and effectiveness."
  } ],
  "http://www.w3.org/2004/02/skos/core#prefLabel" : [ {
    "@language" : "en",
    "@value" : "MCQ.10.2 - Partially Implemented"
  } ]
}, {
  "@id" : "http://JP_ontology.org/assessment/MCQ10.2AnswerC",
