    "@value" : "(i)"
  } ],
  "http://www.w3.org/2004/02/skos/core#definition" : [ {
    "@language" : "en",
    "@value" : "There is no management process in place to control the allocation and management of authentication information, and personnel are not advised on the appropriate handling of authentication information."
  } ],
  "http://www.w3.org/2004/02/skos/core#prefLabel" : [ {
    "@language" : "en",
    "@value" : "MCQ.98 - Not Implemented"
  } ]
}, {
  "@id" : "http://JP_ontology.org/assessment/MCQ98AnswerB",
  "@type" : [ "http://www.w3.org/2002/07/owl#NamedIndividual" ],
  "http://www.w3.org/1999/02/22-rdf-syntax-ns#type" : [ {
