  "http://www.w3.org/2004/02/skos/core#altLabel" : [ {
    "@language" : "en",
    "@value" : "(iii)"
  } ],
  "http://www.w3.org/2004/02/skos/core#definition" : [ {
    "@language" : "en",
    "@value" : "The organisation systematically identifies, documents, and keeps up to date with legal, statutory, regulatory, and contractual requirements relevant to information security, and its approach to meeting these requirements is well-documented and regularly updated, ensuring compliance with applicable laws and regulations."
