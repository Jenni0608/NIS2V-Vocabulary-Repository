  } ],
  "http://www.w3.org/2004/02/skos/core#altLabel" : [ {
    "@language" : "en",
    "@value" : "(ii)"
  } ],
  "http://www.w3.org/2004/02/skos/core#definition" : [ {
    "@language" : "en",
    "@value" : "There are some efforts to implement protection against malware, but user awareness measures may be lacking or insufficient to adequately support the protection against malware."
  } ],
  "http://www.w3.org/2004/02/skos/core#prefLabel" : [ {
    "@language" : "en",
    "@value" : "MCQ.95 - Partially Implemented"
  } ]
}, {
  "@id" : "http://JP_ontology.org/assessment/MCQ95AnswerC",
  "@type" : [ "http://www.w3.org/2002/07/owl#NamedIndividual" ],
  "http://www.w3.org/1999/02/22-rdf-syntax-ns#type" : [ {
    "@id" : "http://JP_ontology.org/assessment/MCQ.95"
