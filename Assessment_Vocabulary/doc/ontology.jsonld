  } ],
  "http://www.w3.org/2004/02/skos/core#altLabel" : [ {
    "@value" : "(iii)"
  } ],
  "http://www.w3.org/2004/02/skos/core#definition" : [ {
    "@language" : "en",
    "@value" : "The organisation consistently collects and analyses information relating to information security threats to produce comprehensive threat intelligence, enabling proactive measures to mitigate potential risks effectively."
  } ],
  "http://www.w3.org/2004/02/skos/core#prefLabel" : [ {
    "@value" : "MCQ15. Fully Implemented"
  } ]
}, {
