  } ],
  "http://www.w3.org/2004/02/skos/core#altLabel" : [ {
    "@language" : "en",
    "@value" : "(ii)"
  } ],
  "http://www.w3.org/2004/02/skos/core#definition" : [ {
    "@language" : "en",
    "@value" : "Some efforts are made to identify external and internal issues, but the process lacks thoroughness or consistency, resulting in gaps in understanding potential impacts on the information security management system."
  } ],
  "http://www.w3.org/2004/02/skos/core#prefLabel" : [ {
    "@language" : "en",
    "@value" : "MCQ.101 - Partially Implemented"
  } ]
}, {
  "@id" : "http://JP_ontology.org/assessment/MCQ101AnswerC",
  "@type" : [ "http://www.w3.org/2002/07/owl#NamedIndividual" ],
  "http://www.w3.org/1999/02/22-rdf-syntax-ns#type" : [ {
    "@id" : "http://JP_ontology.org/assessment/MCQ.101"
