  } ],
  "http://www.w3.org/2004/02/skos/core#altLabel" : [ {
    "@value" : "(i)"
  } ],
  "http://www.w3.org/2004/02/skos/core#definition" : [ {
    "@language" : "en",
    "@value" : "The organisation does not have established channels or mechanisms for contacting relevant authorities regarding information security matters."
  } ],
  "http://www.w3.org/2004/02/skos/core#prefLabel" : [ {
    "@value" : "MCQ13. Not Implemented"
  } ]
}, {
  "@id" : "http://JP_ontology.org/assessment/MCQ13AnswerB",
  "@type" : [ "http://www.w3.org/2002/07/owl#NamedIndividual" ],
  "http://www.w3.org/1999/02/22-rdf-syntax-ns#type" : [ {
    "@id" : "http://JP_ontology.org/assessment/MCQ.13"
  } ],
  "http://www.w3.org/2004/02/skos/core#altLabel" : [ {
    "@value" : "(ii)"
  } ],
  "http://www.w3.org/2004/02/skos/core#definition" : [ {
    "@language" : "en",
    "@value" : "There are ad-hoc or inconsistent efforts to establish and maintain contact with relevant authorities regarding information security matters, but it lacks a formalized or systematic approach."
  } ],
  "http://www.w3.org/2004/02/skos/core#prefLabel" : [ {
    "@value" : "MCQ13. Partially Implemented"
  } ]
}, {
