  "http://www.w3.org/2004/02/skos/core#altLabel" : [ {
    "@language" : "en",
    "@value" : "(i)"
  } ],
  "http://www.w3.org/2004/02/skos/core#definition" : [ {
    "@language" : "en",
    "@value" : "The organisation lacks proper maintenance procedures for equipment, leading to potential disruptions in availability and compromises to the integrity and confidentiality of information."
