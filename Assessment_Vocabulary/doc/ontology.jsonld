  } ],
  "http://www.w3.org/2004/02/skos/core#altLabel" : [ {
    "@value" : "(i)"
  } ],
  "http://www.w3.org/2004/02/skos/core#definition" : [ {
    "@language" : "en",
    "@value" : "The organisation does not collect or analyse information relating to information security threats to produce threat intelligence."
  } ],
  "http://www.w3.org/2004/02/skos/core#prefLabel" : [ {
    "@value" : "MCQ15. Not Implemented"
  } ]
