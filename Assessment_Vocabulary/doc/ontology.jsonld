  "http://www.w3.org/2004/02/skos/core#prefLabel" : [ {
    "@language" : "en",
    "@value" : "MCQ.12 - Partially Implemented"
  } ]
}, {
  "@id" : "http://JP_ontology.org/assessment/MCQ12AnswerC",
