  "http://www.w3.org/2004/02/skos/core#altLabel" : [ {
    "@value" : "(iii)"
  } ],
  "http://www.w3.org/2004/02/skos/core#definition" : [ {
    "@language" : "en",
    "@value" : "The organisation has well-defined processes and established channels for establishing and maintaining regular contact with relevant authorities regarding information security matters, ensuring timely communication and collaboration."
  } ],
  "http://www.w3.org/2004/02/skos/core#prefLabel" : [ {
    "@value" : "MCQ13. Fully Implemented"
  } ]
