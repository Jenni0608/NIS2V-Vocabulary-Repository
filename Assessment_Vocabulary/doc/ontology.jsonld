  } ],
  "http://www.w3.org/2004/02/skos/core#altLabel" : [ {
    "@language" : "en",
    "@value" : "(i)"
  } ],
  "http://www.w3.org/2004/02/skos/core#definition" : [ {
    "@language" : "en",
    "@value" : "Protection against malware is not implemented, and there are no appropriate user awareness measures in place, leaving the organisation vulnerable to malware attacks."
  } ],
  "http://www.w3.org/2004/02/skos/core#prefLabel" : [ {
    "@language" : "en",
    "@value" : "MCQ.95 - Not Implemented"
  } ]
}, {
  "@id" : "http://JP_ontology.org/assessment/MCQ95AnswerB",
