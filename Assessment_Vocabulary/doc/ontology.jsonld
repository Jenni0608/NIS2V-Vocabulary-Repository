  "http://www.w3.org/2004/02/skos/core#prefLabel" : [ {
    "@language" : "en",
    "@value" : "MCQ.130 - Not Implemented"
  } ]
}, {
  "@id" : "http://JP_ontology.org/assessment/MCQ130AnswerB",
