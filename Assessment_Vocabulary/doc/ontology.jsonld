  "http://www.w3.org/1999/02/22-rdf-syntax-ns#type" : [ {
    "@id" : "http://JP_ontology.org/assessment/MCQ.15"
  } ],
  "http://www.w3.org/2004/02/skos/core#altLabel" : [ {
    "@value" : "(ii)"
  } ],
  "http://www.w3.org/2004/02/skos/core#definition" : [ {
    "@language" : "en",
    "@value" : "There are some efforts to collect and analyse information relating to information security threats, but the process may be inconsistent or incomplete in producing comprehensive threat intelligence."
  } ],
  "http://www.w3.org/2004/02/skos/core#prefLabel" : [ {
    "@value" : "MCQ15. Partially Implemented"
  } ]
