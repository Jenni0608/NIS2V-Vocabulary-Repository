  } ],
  "http://www.w3.org/2004/02/skos/core#altLabel" : [ {
    "@language" : "en",
    "@value" : "(ii)"
  } ],
  "http://www.w3.org/2004/02/skos/core#definition" : [ {
    "@language" : "en",
    "@value" : "There are some efforts to control the allocation and management of authentication information through a management process, but personnel may not consistently receive advice on the appropriate handling of authentication information."
  } ],
  "http://www.w3.org/2004/02/skos/core#prefLabel" : [ {
    "@language" : "en",
    "@value" : "MCQ.98 - Partially Implemented"
  } ]
}, {
  "@id" : "http://JP_ontology.org/assessment/MCQ98AnswerC",
  "@type" : [ "http://www.w3.org/2002/07/owl#NamedIndividual" ],
  "http://www.w3.org/1999/02/22-rdf-syntax-ns#type" : [ {
    "@id" : "http://JP_ontology.org/assessment/MCQ.98"
  } ],
  "http://www.w3.org/2004/02/skos/core#altLabel" : [ {
    "@language" : "en",
    "@value" : "(iii)"
  } ],
  "http://www.w3.org/2004/02/skos/core#definition" : [ {
    "@language" : "en",
    "@value" : "The allocation and management of authentication information are controlled by a robust management process, which includes advising personnel on the appropriate handling of authentication information, ensuring secure and effective management of authentication credentials."
  } ],
  "http://www.w3.org/2004/02/skos/core#prefLabel" : [ {
    "@language" : "en",
    "@value" : "MCQ.98 - Fully Implemented"
  } ]
}, {
  "@id" : "http://JP_ontology.org/assessment/MCQ99AnswerA",
  "@type" : [ "http://www.w3.org/2002/07/owl#NamedIndividual" ],
  "http://www.w3.org/1999/02/22-rdf-syntax-ns#type" : [ {
    "@id" : "http://JP_ontology.org/assessment/MCQ.99"
  } ],
  "http://www.w3.org/2004/02/skos/core#altLabel" : [ {
