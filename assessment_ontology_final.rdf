    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <rdfs:subClassOf rdf:resource="https://jenni0608.github.io/NIS2V-Vocabulary-Repository/assessment_ontology_final/AssessmentQuestion"/>
    <skos:prefLabel xml:lang="en">Multiple Choice Question #104</skos:prefLabel>
    <skos:altLabel xml:lang="en">MCQ.104</skos:altLabel>
    <skos:definition xml:lang="en">How does the organisation ensure the establishment, implementation, maintenance, and continual improvement of its information security management system (ISMS) in alignment with the ISO27001 requirements?</skos:definition>
    <skos:broader rdf:resource="https://jenni0608.github.io/NIS2V-Vocabulary-Repository/assessment_ontology_final/AssessmentQuestion"/>
    <dct:created rdf:datatype="http://www.w3.org/2001/XMLSchema#date">2024-05-20</dct:created>
    <dct:contributor>Jenni Parry</dct:contributor>
    <rdfs:isDefinedBy rdf:resource="https://jenni0608.github.io/NIS2V-Vocabulary-Repository/assessment_ontology_final"/>
    <nis2v:hasAnswer rdf:resource="https://jenni0608.github.io/NIS2V-Vocabulary-Repository/assessment_ontology_final/MCQ.104AnswerA"/>
    <nis2v:hasAnswer rdf:resource="https://jenni0608.github.io/NIS2V-Vocabulary-Repository/assessment_ontology_final/MCQ.104AnswerB"/>
    <nis2v:hasAnswer rdf:resource="https://jenni0608.github.io/NIS2V-Vocabulary-Repository/assessment_ontology_final/MCQ.104AnswerC"/>
    <nis2v:relatedToControl rdf:resource="https://jenni0608.github.io/NIS2V-Vocabulary-Repository/ISO27001_ontology_final/Control4.4"/>
    <dct:subject rdf:resource="https://jenni0608.github.io/NIS2V-Vocabulary-Repository/nis2v_Ontology_Final/Article_21.2a"/>
  </rdf:Description>
  
  <rdf:Description rdf:about="https://jenni0608.github.io/NIS2V-Vocabulary-Repository/assessment_ontology_final/MCQ.99">
    <rdf:type rdf:resource="http://www.w3.org/2000/01/rdf-schema#Class"/>
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <rdfs:subClassOf rdf:resource="https://jenni0608.github.io/NIS2V-Vocabulary-Repository/assessment_ontology_final/AssessmentQuestion"/>
    <skos:prefLabel xml:lang="en">Multiple Choice Question #99</skos:prefLabel>
    <skos:altLabel xml:lang="en">MCQ.99</skos:altLabel>
    <skos:definition xml:lang="en">Are secure authentication technologies and procedures implemented based on information access restrictions and the topic-specific policy on access control?</skos:definition>
    <skos:broader rdf:resource="https://jenni0608.github.io/NIS2V-Vocabulary-Repository/assessment_ontology_final/AssessmentQuestion"/>
    <dct:created rdf:datatype="http://www.w3.org/2001/XMLSchema#date">2024-05-20</dct:created>
    <dct:contributor>Jenni Parry</dct:contributor>
    <rdfs:isDefinedBy rdf:resource="https://jenni0608.github.io/NIS2V-Vocabulary-Repository/assessment_ontology_final"/>
    <nis2v:hasAnswer rdf:resource="https://jenni0608.github.io/NIS2V-Vocabulary-Repository/assessment_ontology_final/MCQ.99AnswerA"/>
    <nis2v:hasAnswer rdf:resource="https://jenni0608.github.io/NIS2V-Vocabulary-Repository/assessment_ontology_final/MCQ.99AnswerB"/>
    <nis2v:hasAnswer rdf:resource="https://jenni0608.github.io/NIS2V-Vocabulary-Repository/assessment_ontology_final/MCQ.99AnswerC"/>
    <nis2v:relatedToControl rdf:resource="https://jenni0608.github.io/NIS2V-Vocabulary-Repository/ISO27001_ontology_final/ControlA.8.5"/>
    <dct:subject rdf:resource="https://jenni0608.github.io/NIS2V-Vocabulary-Repository/nis2v_Ontology_Final/Article_21.2j"/>
  </rdf:Description>
  
  

	 <rdf:Description rdf:about="https://jenni0608.github.io/NIS2V-Vocabulary-Repository/assessment_ontology_final/MCQ16AnswerB">
    <rdf:type rdf:resource="https://jenni0608.github.io/NIS2V-Vocabulary-Repository/assessment_ontology_final/MCQ.16"/>
    <skos:prefLabel xml:lang="en">MCQ.16 - Partially Implemented</skos:prefLabel>
    <skos:altLabel xml:lang="en">(ii)</skos:altLabel>
    <skos:definition xml:lang="en">There are some efforts to identify, document, and keep up to date with legal, statutory, regulatory, and contractual requirements relevant to information security, as well as the organisation’s approach to meeting these requirements, but the process may be inconsistent or incomplete.</skos:definition>
  </rdf:Description>
  
  <rdf:Description rdf:about="https://jenni0608.github.io/NIS2V-Vocabulary-Repository/assessment_ontology_final/MCQ56.1AnswerA">
    <rdf:type rdf:resource="https://jenni0608.github.io/NIS2V-Vocabulary-Repository/assessment_ontology_final/MCQ.56.1"/>
    <skos:prefLabel xml:lang="en">MCQ.56.1 - Not Implemented</skos:prefLabel>
    <skos:altLabel xml:lang="en">(i)</skos:altLabel>
    <skos:definition xml:lang="en">The organisation does not determine what information to communicate regarding the information security management system.</skos:definition>
  </rdf:Description>
  
  <rdf:Description rdf:about="https://jenni0608.github.io/NIS2V-Vocabulary-Repository/assessment_ontology_final/MCQ117AnswerB">
    <rdf:type rdf:resource="https://jenni0608.github.io/NIS2V-Vocabulary-Repository/assessment_ontology_final/MCQ.117"/>
    <skos:prefLabel xml:lang="en">MCQ.117 - Partially Implemented</skos:prefLabel>
    <skos:altLabel xml:lang="en">(ii)</skos:altLabel>
    <skos:definition xml:lang="en">Some efforts are made to improve the ISMS, but there are inconsistencies or gaps in ensuring continual improvement across all aspects of suitability, adequacy, and effectiveness.</skos:definition>
  </rdf:Description>
  
  <rdf:Description rdf:about="https://jenni0608.github.io/NIS2V-Vocabulary-Repository/assessment_ontology_final/MCQ84AnswerA">
    <rdf:type rdf:resource="https://jenni0608.github.io/NIS2V-Vocabulary-Repository/assessment_ontology_final/MCQ.84"/>
    <skos:prefLabel xml:lang="en">MCQ.84 - Not Implemented</skos:prefLabel>
    <skos:altLabel xml:lang="en">(i)</skos:altLabel>
    <skos:definition xml:lang="en">The allocation and use of privileged access rights are not restricted or managed, potentially leading to unauthorized access to sensitive systems and data.</skos:definition>
  </rdf:Description>
  
  <rdf:Description rdf:about="https://jenni0608.github.io/NIS2V-Vocabulary-Repository/assessment_ontology_final/MCQ86AnswerA">
    <rdf:type rdf:resource="https://jenni0608.github.io/NIS2V-Vocabulary-Repository/assessment_ontology_final/MCQ.86"/>
    <skos:prefLabel xml:lang="en">MCQ.86 - Not Implemented</skos:prefLabel>
    <skos:altLabel xml:lang="en">(i)</skos:altLabel>
    <skos:definition xml:lang="en">Read and write access to source code, development tools, and software libraries is not appropriately managed, potentially leading to unauthorized modifications, code leaks, or security breaches.</skos:definition>
  </rdf:Description>
  
  <rdf:Description rdf:about="https://jenni0608.github.io/NIS2V-Vocabulary-Repository/assessment_ontology_final/MCQ127AnswerA">
    <rdf:type rdf:resource="https://jenni0608.github.io/NIS2V-Vocabulary-Repository/assessment_ontology_final/MCQ.127"/>
    <skos:prefLabel xml:lang="en">MCQ.127 - Not Implemented</skos:prefLabel>
    <skos:altLabel xml:lang="en">(i)</skos:altLabel>
    <skos:definition xml:lang="en">The organisation lacks established principles for engineering secure systems and does not document or apply them to information system development activities, leading to potential security vulnerabilities.</skos:definition>
  </rdf:Description>
  
  <rdf:Description rdf:about="https://jenni0608.github.io/NIS2V-Vocabulary-Repository/assessment_ontology_final/MCQ107.2AnswerA">
    <rdf:type rdf:resource="https://jenni0608.github.io/NIS2V-Vocabulary-Repository/assessment_ontology_final/MCQ.107.2"/>
    <skos:prefLabel xml:lang="en">MCQ.107.2 - Not Implemented</skos:prefLabel>
    <skos:altLabel xml:lang="en">(i)</skos:altLabel>
    <skos:definition xml:lang="en">The organisation lacks a structured approach to planning for the achievement of information security objectives, leading to uncertainty regarding what needs to be done, required resources, responsibilities, timelines, and evaluation methods.</skos:definition>
  </rdf:Description>
  
  <rdf:Description rdf:about="https://jenni0608.github.io/NIS2V-Vocabulary-Repository/assessment_ontology_final/MCQ59AnswerA">
    <rdf:type rdf:resource="https://jenni0608.github.io/NIS2V-Vocabulary-Repository/assessment_ontology_final/MCQ.59"/>
    <skos:prefLabel xml:lang="en">MCQ.59 - Not Implemented</skos:prefLabel>
    <skos:altLabel xml:lang="en">(i)</skos:altLabel>
    <skos:definition xml:lang="en">Rules to control physical and logical access to information and associated assets are not established or implemented, lacking alignment with business and information security requirements.</skos:definition>
  </rdf:Description>
  
  <rdf:Description rdf:about="https://jenni0608.github.io/NIS2V-Vocabulary-Repository/assessment_ontology_final/MCQ86AnswerC">
    <rdf:type rdf:resource="https://jenni0608.github.io/NIS2V-Vocabulary-Repository/assessment_ontology_final/MCQ.86"/>
    <skos:prefLabel xml:lang="en">MCQ.86 - Fully Implemented</skos:prefLabel>
    <skos:altLabel xml:lang="en">(iii)</skos:altLabel>
    <skos:definition xml:lang="en">Read and write access to source code, development tools, and software libraries is fully and appropriately managed, ensuring that access is granted only to authorized personnel and activities are monitored and controlled to prevent unauthorized modifications or breaches.</skos:definition>
  </rdf:Description>
  
  <rdf:Description rdf:about="https://jenni0608.github.io/NIS2V-Vocabulary-Repository/assessment_ontology_final/MCQ22.1AnswerA">
    <rdf:type rdf:resource="https://jenni0608.github.io/NIS2V-Vocabulary-Repository/assessment_ontology_final/MCQ.22.1"/>
    <skos:prefLabel xml:lang="en">MCQ.22.1 - Not Implemented</skos:prefLabel>
    <skos:altLabel xml:lang="en">(i)</skos:altLabel>
    <skos:definition xml:lang="en">Top management does not establish an information security policy appropriate to the purpose of the organisation, potentially leading to a lack of clear direction and guidance in managing information security.</skos:definition>
  </rdf:Description>
  
  <rdf:Description rdf:about="https://jenni0608.github.io/NIS2V-Vocabulary-Repository/assessment_ontology_final/MCQ61AnswerB">
    <rdf:type rdf:resource="https://jenni0608.github.io/NIS2V-Vocabulary-Repository/assessment_ontology_final/MCQ.61"/>
    <skos:prefLabel xml:lang="en">MCQ.61 - Partially Implemented</skos:prefLabel>
    <skos:altLabel xml:lang="en">(ii)</skos:altLabel>
    <skos:definition xml:lang="en">There are some efforts to control the allocation and management of authentication information through a management process, but personnel may not consistently receive advice on the appropriate handling of authentication information.</skos:definition>
  </rdf:Description>
  
  <rdf:Description rdf:about="https://jenni0608.github.io/NIS2V-Vocabulary-Repository/assessment_ontology_final/MCQ100AnswerC">
    <rdf:type rdf:resource="https://jenni0608.github.io/NIS2V-Vocabulary-Repository/assessment_ontology_final/MCQ.100"/>
    <skos:prefLabel xml:lang="en">MCQ.100 - Fully Implemented</skos:prefLabel>
    <skos:altLabel xml:lang="en">(iii)</skos:altLabel>
    <skos:definition xml:lang="en">Data leakage prevention measures are fully applied to systems, networks, and other devices that process, store, or transmit sensitive information, ensuring protection against unauthorized disclosure or leakage of sensitive data.</skos:definition>
  </rdf:Description>
  
  <rdf:Description rdf:about="https://jenni0608.github.io/NIS2V-Vocabulary-Repository/assessment_ontology_final/MCQ47AnswerC">
    <rdf:type rdf:resource="https://jenni0608.github.io/NIS2V-Vocabulary-Repository/assessment_ontology_final/MCQ.47"/>
    <skos:prefLabel xml:lang="en">MCQ.47 - Fully Implemented</skos:prefLabel>
    <skos:altLabel xml:lang="en">(iii)</skos:altLabel>
    <skos:definition xml:lang="en">Segregation is fully implemented for groups of information services, users, and information systems within the organisation's networks, ensuring controlled access and minimizing the impact of security incidents.</skos:definition>
  </rdf:Description>
  
  <rdf:Description rdf:about="https://jenni0608.github.io/NIS2V-Vocabulary-Repository/assessment_ontology_final/MCQ83AnswerC">
    <rdf:type rdf:resource="https://jenni0608.github.io/NIS2V-Vocabulary-Repository/assessment_ontology_final/MCQ.83"/>
    <skos:prefLabel xml:lang="en">MCQ.83 - Fully Implemented</skos:prefLabel>
    <skos:altLabel xml:lang="en">(iii)</skos:altLabel>
    <skos:definition xml:lang="en">Access rights to information and associated assets are provisioned, reviewed, modified, and removed in strict accordance with the organisation’s topic-specific policy and rules for access control, ensuring effective access management and adherence to security requirements.</skos:definition>
  </rdf:Description>
  
  <rdf:Description rdf:about="https://jenni0608.github.io/NIS2V-Vocabulary-Repository/assessment_ontology_final/MCQ34.4AnswerC">
    <rdf:type rdf:resource="https://jenni0608.github.io/NIS2V-Vocabulary-Repository/assessment_ontology_final/MCQ.34.4"/>
    <skos:prefLabel xml:lang="en">MCQ.34.4 - Fully Implemented</skos:prefLabel>
    <skos:altLabel xml:lang="en">(iii)</skos:altLabel>
    <skos:definition xml:lang="en">The organisation fully ensures that externally provided processes, products, or services relevant to the information security management system are controlled, minimizing security risks associated with external dependencies and maintaining the integrity of the management system.</skos:definition>
  </rdf:Description>
  
  <rdf:Description rdf:about="https://jenni0608.github.io/NIS2V-Vocabulary-Repository/assessment_ontology_final/MCQ127AnswerB">
    <rdf:type rdf:resource="https://jenni0608.github.io/NIS2V-Vocabulary-Repository/assessment_ontology_final/MCQ.127"/>
    <skos:prefLabel xml:lang="en">MCQ.127 - Partially Implemented</skos:prefLabel>
    <skos:altLabel xml:lang="en">(ii)</skos:altLabel>
    <skos:definition xml:lang="en">Some principles for engineering secure systems are established, but there are inconsistencies or gaps in their documentation, maintenance, or application to information system development activities.</skos:definition>
  </rdf:Description>
  
  <rdf:Description rdf:about="https://jenni0608.github.io/NIS2V-Vocabulary-Repository/assessment_ontology_final/MCQ49.2AnswerA">
    <rdf:type rdf:resource="https://jenni0608.github.io/NIS2V-Vocabulary-Repository/assessment_ontology_final/MCQ.49.2"/>
    <skos:prefLabel xml:lang="en">MCQ.49.2 - Not Implemented</skos:prefLabel>
    <skos:altLabel xml:lang="en">(i)</skos:altLabel>
    <skos:definition xml:lang="en">The organisation has not established methods for monitoring, measurement, analysis, and evaluation to ensure valid results. The methods selected may not be capable of producing comparable and reproducible results, potentially leading to unreliable data for decision-making.</skos:definition>
  </rdf:Description>
  
  <rdf:Description rdf:about="https://jenni0608.github.io/NIS2V-Vocabulary-Repository/assessment_ontology_final/MCQ121AnswerC">
    <rdf:type rdf:resource="https://jenni0608.github.io/NIS2V-Vocabulary-Repository/assessment_ontology_final/MCQ.121"/>
    <skos:prefLabel xml:lang="en">MCQ.121 - Fully Implemented</skos:prefLabel>
    <skos:altLabel xml:lang="en">(iii)</skos:altLabel>
    <skos:definition xml:lang="en">The organisation has established change management procedures specifically for information processing facilities and information systems. These procedures ensure that changes are effectively managed, minimizing risks of disruptions and ensuring continuity of operations.</skos:definition>
  </rdf:Description>
  
  <rdf:Description rdf:about="https://jenni0608.github.io/NIS2V-Vocabulary-Repository/assessment_ontology_final/MCQ34.1AnswerB">
    <rdf:type rdf:resource="https://jenni0608.github.io/NIS2V-Vocabulary-Repository/assessment_ontology_final/MCQ.34.1"/>
    <skos:prefLabel xml:lang="en">MCQ.34.1 - Partially Implemented</skos:prefLabel>
    <skos:altLabel xml:lang="en">(ii)</skos:altLabel>
    <skos:definition xml:lang="en">There are some efforts by the organisation to plan, implement, and control processes to meet requirements and implement actions by establishing criteria for the processes and implementing control. However, these efforts may be inconsistent or not fully comprehensive.</skos:definition>
  </rdf:Description>
  
  <rdf:Description rdf:about="https://jenni0608.github.io/NIS2V-Vocabulary-Repository/assessment_ontology_final/MCQ82AnswerB">
    <rdf:type rdf:resource="https://jenni0608.github.io/NIS2V-Vocabulary-Repository/assessment_ontology_final/MCQ.82"/>
    <skos:prefLabel xml:lang="en">MCQ.82 - Partially Implemented</skos:prefLabel>
    <skos:altLabel xml:lang="en">(ii)</skos:altLabel>
    <skos:definition xml:lang="en">There are some efforts to control the allocation and management of authentication information through a management process, but personnel may not consistently receive advice on the appropriate handling of authentication information.</skos:definition>
  </rdf:Description>
  
  <rdf:Description rdf:about="https://jenni0608.github.io/NIS2V-Vocabulary-Repository/assessment_ontology_final/MCQ37AnswerA">
    <rdf:type rdf:resource="https://jenni0608.github.io/NIS2V-Vocabulary-Repository/assessment_ontology_final/MCQ.37"/>
    <skos:prefLabel xml:lang="en">MCQ.37 - Not Implemented</skos:prefLabel>
    <skos:altLabel xml:lang="en">(i)</skos:altLabel>
    <skos:definition xml:lang="en">Relevant information security requirements are not established or agreed upon with each supplier based on the type of supplier relationship, leaving the organisation exposed to potential security vulnerabilities from third-party sources.</skos:definition>
  </rdf:Description>
  
  <rdf:Description rdf:about="https://jenni0608.github.io/NIS2V-Vocabulary-Repository/assessment_ontology_final/MCQ56.1AnswerB">
    <rdf:type rdf:resource="https://jenni0608.github.io/NIS2V-Vocabulary-Repository/assessment_ontology_final/MCQ.56.1"/>
    <skos:prefLabel xml:lang="en">MCQ.56.1 - Partially Implemented</skos:prefLabel>
    <skos:altLabel xml:lang="en">(ii)</skos:altLabel>
    <skos:definition xml:lang="en">The organisation has some understanding of what information to communicate regarding the information security management system, but it may not be comprehensive.</skos:definition>
  </rdf:Description>
  
  <rdf:Description rdf:about="https://jenni0608.github.io/NIS2V-Vocabulary-Repository/assessment_ontology_final/MCQ87AnswerC">
    <rdf:type rdf:resource="https://jenni0608.github.io/NIS2V-Vocabulary-Repository/assessment_ontology_final/MCQ.87"/>
    <skos:prefLabel xml:lang="en">MCQ.87 - Fully Implemented</skos:prefLabel>
    <skos:altLabel xml:lang="en">(iii)</skos:altLabel>
    <skos:definition xml:lang="en">Secure authentication technologies and procedures are fully implemented based on information access restrictions and the topic-specific policy on access control, ensuring robust access security and compliance with organisational policies.</skos:definition>
  </rdf:Description>
  
  <rdf:Description rdf:about="https://jenni0608.github.io/NIS2V-Vocabulary-Repository/assessment_ontology_final/MCQ121AnswerA">
    <rdf:type rdf:resource="https://jenni0608.github.io/NIS2V-Vocabulary-Repository/assessment_ontology_final/MCQ.121"/>
    <skos:prefLabel xml:lang="en">MCQ.121 - Not Implemented</skos:prefLabel>
    <skos:altLabel xml:lang="en">(i)</skos:altLabel>
    <skos:definition xml:lang="en">The organisation lacks change management procedures for information processing facilities and information systems, leading to uncontrolled changes and potential disruptions.</skos:definition>
  </rdf:Description>
  
  <rdf:Description rdf:about="https://jenni0608.github.io/NIS2V-Vocabulary-Repository/assessment_ontology_final/MCQ114AnswerA">
    <rdf:type rdf:resource="https://jenni0608.github.io/NIS2V-Vocabulary-Repository/assessment_ontology_final/MCQ.114"/>
    <skos:prefLabel xml:lang="en">MCQ.114 - Not Implemented</skos:prefLabel>
    <skos:altLabel xml:lang="en">(i)</skos:altLabel>
    <skos:definition xml:lang="en">The organisation lacks a planned, established, implemented, and maintained audit program, including defined frequency, methods, responsibilities, planning requirements, and reporting. Additionally, there is no consideration given to the importance of processes or results of previous audits.</skos:definition>
  </rdf:Description>
  
  <rdf:Description rdf:about="https://jenni0608.github.io/NIS2V-Vocabulary-Repository/assessment_ontology_final/MCQ96AnswerA">
    <rdf:type rdf:resource="https://jenni0608.github.io/NIS2V-Vocabulary-Repository/assessment_ontology_final/MCQ.96"/>
    <skos:prefLabel xml:lang="en">MCQ.96 - Not Implemented</skos:prefLabel>
    <skos:altLabel xml:lang="en">(i)</skos:altLabel>
    <skos:definition xml:lang="en">Information transfer rules, procedures, or agreements are not in place for all types of transfer facilities within the organisation and between the organisation and other parties, potentially leading to unsecure or unauthorized transfer of sensitive information.</skos:definition>
  </rdf:Description>
  
  <rdf:Description rdf:about="https://jenni0608.github.io/NIS2V-Vocabulary-Repository/assessment_ontology_final/MCQ24AnswerC">
    <rdf:type rdf:resource="https://jenni0608.github.io/NIS2V-Vocabulary-Repository/assessment_ontology_final/MCQ.24"/>
    <skos:prefLabel xml:lang="en">MCQ.24 - Fully Implemented</skos:prefLabel>
    <skos:altLabel xml:lang="en">(iii)</skos:altLabel>
    <skos:definition xml:lang="en">Logs that record activities, exceptions, faults, and other relevant events are fully produced, stored, protected, and analysed, providing comprehensive visibility into the organisation's security posture and enabling proactive management of security events.</skos:definition>
  </rdf:Description>
  
  <rdf:Description rdf:about="https://jenni0608.github.io/NIS2V-Vocabulary-Repository/assessment_ontology_final/MCQ106.1AnswerA">
    <rdf:type rdf:resource="https://jenni0608.github.io/NIS2V-Vocabulary-Repository/assessment_ontology_final/MCQ.106.1"/>
    <skos:prefLabel xml:lang="en">MCQ.106.1 - Not Implemented</skos:prefLabel>
    <skos:altLabel xml:lang="en">(i)</skos:altLabel>
    <skos:definition xml:lang="en">The organisation does not consider the broader organisational context or the needs and expectations of interested parties when planning for the ISMS, leading to potential disconnects between security measures and organisational objectives.</skos:definition>
  </rdf:Description>
  
  <rdf:Description rdf:about="https://jenni0608.github.io/NIS2V-Vocabulary-Repository/assessment_ontology_final/MCQ108AnswerA">
    <rdf:type rdf:resource="https://jenni0608.github.io/NIS2V-Vocabulary-Repository/assessment_ontology_final/MCQ.108"/>
    <skos:prefLabel xml:lang="en">MCQ.108 - Not Implemented</skos:prefLabel>
    <skos:altLabel xml:lang="en">(i)</skos:altLabel>
    <skos:definition xml:lang="en">The organisation does not systematically determine or provide the resources needed for the ISMS, resulting in resource shortages that hinder effective security management.</skos:definition>
  </rdf:Description>
  
  <rdf:Description rdf:about="https://jenni0608.github.io/NIS2V-Vocabulary-Repository/assessment_ontology_final/MCQ67AnswerB">
    <rdf:type rdf:resource="https://jenni0608.github.io/NIS2V-Vocabulary-Repository/assessment_ontology_final/MCQ.67"/>
    <skos:prefLabel xml:lang="en">MCQ.67 - Partially Implemented</skos:prefLabel>
    <skos:altLabel xml:lang="en">(ii)</skos:altLabel>
    <skos:definition xml:lang="en">There are some efforts to implement segregation for groups of information services, users, and information systems within the organisation's networks, but the implementation may be incomplete or inconsistent.</skos:definition>
  </rdf:Description>
  
  <rdf:Description rdf:about="https://jenni0608.github.io/NIS2V-Vocabulary-Repository/assessment_ontology_final/MCQ66AnswerB">
    <rdf:type rdf:resource="https://jenni0608.github.io/NIS2V-Vocabulary-Repository/assessment_ontology_final/MCQ.66"/>
    <skos:prefLabel xml:lang="en">MCQ.66 - Partially Implemented</skos:prefLabel>
    <skos:altLabel xml:lang="en">(ii)</skos:altLabel>
    <skos:definition xml:lang="en">There are some procedures and measures in place to securely manage software installation on operational systems, but they may be incomplete or inconsistently applied.</skos:definition>
  </rdf:Description>
  
  <rdf:Description rdf:about="https://jenni0608.github.io/NIS2V-Vocabulary-Repository/assessment_ontology_final/MCQ78AnswerC">
    <rdf:type rdf:resource="https://jenni0608.github.io/NIS2V-Vocabulary-Repository/assessment_ontology_final/MCQ.78"/>
    <skos:prefLabel xml:lang="en">MCQ.78 - Fully Implemented</skos:prefLabel>
    <skos:altLabel xml:lang="en">(iii)</skos:altLabel>
    <skos:definition xml:lang="en">Confidentiality or non-disclosure agreements reflecting the organisation's needs for the protection of information are fully identified, documented, regularly reviewed, and signed by personnel and other relevant interested parties, ensuring legal protection and safeguarding of sensitive information.</skos:definition>
  </rdf:Description>
  
  <rdf:Description rdf:about="https://jenni0608.github.io/NIS2V-Vocabulary-Repository/assessment_ontology_final/MCQ115AnswerB">
    <rdf:type rdf:resource="https://jenni0608.github.io/NIS2V-Vocabulary-Repository/assessment_ontology_final/MCQ.115"/>
    <skos:prefLabel xml:lang="en">MCQ.115 - Partially Implemented</skos:prefLabel>
    <skos:altLabel xml:lang="en">(ii)</skos:altLabel>
    <skos:definition xml:lang="en">Some effort is made by senior management to review the information security management system at planned intervals, but there are inconsistencies or gaps in ensuring its continuing suitability, adequacy, and effectiveness.</skos:definition>
  </rdf:Description>
  
  <rdf:Description rdf:about="https://jenni0608.github.io/NIS2V-Vocabulary-Repository/assessment_ontology_final/MCQ97AnswerA">
    <rdf:type rdf:resource="https://jenni0608.github.io/NIS2V-Vocabulary-Repository/assessment_ontology_final/MCQ.97"/>
    <skos:prefLabel xml:lang="en">MCQ.97 - Not Implemented</skos:prefLabel>
    <skos:altLabel xml:lang="en">(i)</skos:altLabel>
    <skos:definition xml:lang="en">The organisation lacks a structured approach to managing the complete life cycle of identities, leading to inconsistencies and gaps in identity-related processes such as creation, authentication, and termination.</skos:definition>
  </rdf:Description>
  
  <rdf:Description rdf:about="https://jenni0608.github.io/NIS2V-Vocabulary-Repository/assessment_ontology_final/MCQ80AnswerC">
    <rdf:type rdf:resource="https://jenni0608.github.io/NIS2V-Vocabulary-Repository/assessment_ontology_final/MCQ.80"/>
    <skos:prefLabel xml:lang="en">MCQ.80 - Fully Implemented</skos:prefLabel>
    <skos:altLabel xml:lang="en">(iii)</skos:altLabel>
    <skos:definition xml:lang="en">Comprehensive rules are established and rigorously implemented to control physical and logical access to information and associated assets, ensuring alignment with both business and information security requirements for effective access control.</skos:definition>
  </rdf:Description>
  
  <rdf:Description rdf:about="https://jenni0608.github.io/NIS2V-Vocabulary-Repository/assessment_ontology_final/MCQ110AnswerC">
    <rdf:type rdf:resource="https://jenni0608.github.io/NIS2V-Vocabulary-Repository/assessment_ontology_final/MCQ.110"/>
    <skos:prefLabel xml:lang="en">MCQ.110 - Fully Implemented</skos:prefLabel>
    <skos:altLabel xml:lang="en">(iii)</skos:altLabel>
    <skos:definition xml:lang="en">The organisation establishes criteria for processes and ensures their control. Documented information is available to ensure confidence in process execution. Planned changes are controlled, and consequences of unintended changes are reviewed, with necessary actions taken to mitigate adverse effects. Additionally, externally provided processes, products, or services relevant to the ISMS are controlled.</skos:definition>
  </rdf:Description>
  
  <rdf:Description rdf:about="https://jenni0608.github.io/NIS2V-Vocabulary-Repository/assessment_ontology_final/MCQ.10.1AnswerB">
    <rdf:type rdf:resource="https://jenni0608.github.io/NIS2V-Vocabulary-Repository/assessment_ontology_final/MCQ.10.1"/>
    <skos:prefLabel xml:lang="en">MCQ.10.1 - Partially Implemented</skos:prefLabel>
    <skos:altLabel xml:lang="en">(ii)</skos:altLabel>
    <skos:definition xml:lang="en">There are some efforts to define, approve, publish, communicate, and acknowledge the information security policy and topic-specific policies. However, these efforts may be inconsistent or not fully comprehensive across all relevant parties.</skos:definition>
  </rdf:Description>
  
  <rdf:Description rdf:about="https://jenni0608.github.io/NIS2V-Vocabulary-Repository/assessment_ontology_final/MCQ36AnswerB">
    <rdf:type rdf:resource="https://jenni0608.github.io/NIS2V-Vocabulary-Repository/assessment_ontology_final/MCQ.36"/>
    <skos:prefLabel xml:lang="en">MCQ.36 - Partially Implemented</skos:prefLabel>
    <skos:altLabel xml:lang="en">(ii)</skos:altLabel>
    <skos:definition xml:lang="en">There are some processes and procedures in place to manage information security risks associated with the use of supplier’s products or services, but they may be incomplete or inconsistently applied</skos:definition>
  </rdf:Description>
  
  <rdf:Description rdf:about="https://jenni0608.github.io/NIS2V-Vocabulary-Repository/assessment_ontology_final/MCQ55.3AnswerB">
    <rdf:type rdf:resource="https://jenni0608.github.io/NIS2V-Vocabulary-Repository/assessment_ontology_final/MCQ.55.3"/>
    <skos:prefLabel xml:lang="en">MCQ.55.3 - Partially Implemented</skos:prefLabel>
    <skos:altLabel xml:lang="en">(ii)</skos:altLabel>
    <skos:definition xml:lang="en">There are some efforts to make persons doing work under the organisation’s control aware of the implications of not conforming with the information security management system requirements. However, awareness may be inconsistent or not fully comprehensive.</skos:definition>
  </rdf:Description>
  
  <rdf:Description rdf:about="https://jenni0608.github.io/NIS2V-Vocabulary-Repository/assessment_ontology_final/MCQ126AnswerC">
    <rdf:type rdf:resource="https://jenni0608.github.io/NIS2V-Vocabulary-Repository/assessment_ontology_final/MCQ.126"/>
    <skos:prefLabel xml:lang="en">MCQ.126 - Fully Implemented</skos:prefLabel>
    <skos:altLabel xml:lang="en">(iii)</skos:altLabel>
    <skos:definition xml:lang="en">The organisation has established and consistently applies a process to identify, specify, and approve information security requirements during application development or acquisition. This ensures that security considerations are integrated into the development or acquisition process to mitigate potential vulnerabilities effectively.

</skos:definition>
  </rdf:Description>
  
  <rdf:Description rdf:about="https://jenni0608.github.io/NIS2V-Vocabulary-Repository/assessment_ontology_final/MCQ8.2AnswerA">
    <rdf:type rdf:resource="https://jenni0608.github.io/NIS2V-Vocabulary-Repository/assessment_ontology_final/MCQ.8.2"/>
    <skos:prefLabel xml:lang="en">MCQ.8.2 - Not Implemented</skos:prefLabel>
    <skos:altLabel xml:lang="en">(i)</skos:altLabel>
    <skos:definition xml:lang="en">The organisation does not retain documented information of the results of these assessments, potentially leading to a lack of historical data for analysis and decision-making in future assessments.</skos:definition>
  </rdf:Description>
  
  <rdf:Description rdf:about="https://jenni0608.github.io/NIS2V-Vocabulary-Repository/assessment_ontology_final/MCQ79AnswerB">
    <rdf:type rdf:resource="https://jenni0608.github.io/NIS2V-Vocabulary-Repository/assessment_ontology_final/MCQ.79"/>
    <skos:prefLabel xml:lang="en">MCQ.79 - Partially Implemented</skos:prefLabel>
    <skos:altLabel xml:lang="en">(ii)</skos:altLabel>
    <skos:definition xml:lang="en">There are some efforts to implement security measures when personnel are working remotely, but the measures may be inconsistent or not fully comprehensive in protecting information accessed, processed, or stored outside the organisation’s premises.</skos:definition>
  </rdf:Description>
  
  <rdf:Description rdf:about="https://jenni0608.github.io/NIS2V-Vocabulary-Repository/assessment_ontology_final/MCQ120AnswerC">
    <rdf:type rdf:resource="https://jenni0608.github.io/NIS2V-Vocabulary-Repository/assessment_ontology_final/MCQ.120"/>
    <skos:prefLabel xml:lang="en">MCQ.120 - Fully Implemented</skos:prefLabel>
    <skos:altLabel xml:lang="en">(iii)</skos:altLabel>
    <skos:definition xml:lang="en">The organisation systematically develops and implements procedures for information labeling, ensuring alignment with the adopted information classification scheme. This ensures consistent and effective labeling practices across the organisation.</skos:definition>
  </rdf:Description>
  
  <rdf:Description rdf:about="https://jenni0608.github.io/NIS2V-Vocabulary-Repository/assessment_ontology_final/MCQ6.3AnswerC">
    <rdf:type rdf:resource="https://jenni0608.github.io/NIS2V-Vocabulary-Repository/assessment_ontology_final/MCQ.6.3"/>
    <skos:prefLabel xml:lang="en">MCQ.6.3 - Fully Implemented</skos:prefLabel>
    <skos:altLabel xml:lang="en">(iii)</skos:altLabel>
    <skos:definition xml:lang="en">The organisation has identified all relevant stakeholders with whom to communicate regarding the information security management system, ensuring comprehensive coverage.</skos:definition>
  </rdf:Description>
  
  <rdf:Description rdf:about="https://jenni0608.github.io/NIS2V-Vocabulary-Repository/assessment_ontology_final/MCQ107.1AnswerB">
    <rdf:type rdf:resource="https://jenni0608.github.io/NIS2V-Vocabulary-Repository/assessment_ontology_final/MCQ.107.1"/>
    <skos:prefLabel xml:lang="en">MCQ.107.1 - Partially Implemented</skos:prefLabel>
    <skos:altLabel xml:lang="en">(ii)</skos:altLabel>
    <skos:definition xml:lang="en">Some effort is made to establish information security objectives, but there are inconsistencies or gaps in their alignment with the information security policy, measurability, and consideration of applicable requirements and risk assessment results.</skos:definition>
  </rdf:Description>
  
  <rdf:Description rdf:about="https://jenni0608.github.io/NIS2V-Vocabulary-Repository/assessment_ontology_final/MCQ63AnswerC">
    <rdf:type rdf:resource="https://jenni0608.github.io/NIS2V-Vocabulary-Repository/assessment_ontology_final/MCQ.63"/>
    <skos:prefLabel xml:lang="en">MCQ.63 - Fully Implemented</skos:prefLabel>
    <skos:altLabel xml:lang="en">(iii)</skos:altLabel>
    <skos:definition xml:lang="en">The organisation consistently acquires information regarding technical vulnerabilities of the information systems in use, evaluates its exposure to such vulnerabilities, and implements appropriate measures accordingly, ensuring proactive management of security risks.</skos:definition>
  </rdf:Description>
  
  <rdf:Description rdf:about="https://jenni0608.github.io/NIS2V-Vocabulary-Repository/assessment_ontology_final/MCQ85AnswerA">
    <rdf:type rdf:resource="https://jenni0608.github.io/NIS2V-Vocabulary-Repository/assessment_ontology_final/MCQ.85"/>
    <skos:prefLabel xml:lang="en">MCQ.85 - Not Implemented</skos:prefLabel>
    <skos:altLabel xml:lang="en">(i)</skos:altLabel>
    <skos:definition xml:lang="en">Access to information and other associated assets is not restricted in accordance with the established topic-specific policy on access control, potentially leading to unauthorized access and compromise of sensitive assets.</skos:definition>
  </rdf:Description>
  
  <rdf:Description rdf:about="https://jenni0608.github.io/NIS2V-Vocabulary-Repository/assessment_ontology_final/MCQ97AnswerC">
    <rdf:type rdf:resource="https://jenni0608.github.io/NIS2V-Vocabulary-Repository/assessment_ontology_final/MCQ.97"/>
    <skos:prefLabel xml:lang="en">MCQ.97 - Fully Implemented</skos:prefLabel>
    <skos:altLabel xml:lang="en">(iii)</skos:altLabel>
    <skos:definition xml:lang="en">The organisation has implemented a comprehensive approach to managing the complete life cycle of identities, covering all activities from creation to termination, and ensuring robust oversight and control over identity-related processes.</skos:definition>
  </rdf:Description>
  
  <rdf:Description rdf:about="https://jenni0608.github.io/NIS2V-Vocabulary-Repository/assessment_ontology_final/MCQ106.2AnswerB">
    <rdf:type rdf:resource="https://jenni0608.github.io/NIS2V-Vocabulary-Repository/assessment_ontology_final/MCQ.106.2"/>
    <skos:prefLabel xml:lang="en">MCQ.106.2 - Partially Implemented</skos:prefLabel>
    <skos:altLabel xml:lang="en">(ii)</skos:altLabel>
    <skos:definition xml:lang="en">Some actions are planned to address identified risks and opportunities, but there are inconsistencies or gaps in implementation and integration into ISMS processes, hindering effectiveness.</skos:definition>
  </rdf:Description>
  
  <rdf:Description rdf:about="https://jenni0608.github.io/NIS2V-Vocabulary-Repository/assessment_ontology_final/MCQ93AnswerB">
    <rdf:type rdf:resource="https://jenni0608.github.io/NIS2V-Vocabulary-Repository/assessment_ontology_final/MCQ.93"/>
    <skos:prefLabel xml:lang="en">MCQ.93 - Partially Implemented</skos:prefLabel>
    <skos:altLabel xml:lang="en">(ii)</skos:altLabel>
    <skos:definition xml:lang="en">There are some efforts to verify items of equipment containing storage media, but the verification process may be inconsistent or not fully comprehensive in ensuring the removal of sensitive data and licensed software prior to disposal or re-use.</skos:definition>
  </rdf:Description>
  
  <rdf:Description rdf:about="https://jenni0608.github.io/NIS2V-Vocabulary-Repository/assessment_ontology_final/MCQ35AnswerC">
    <rdf:type rdf:resource="https://jenni0608.github.io/NIS2V-Vocabulary-Repository/assessment_ontology_final/MCQ.35"/>
    <skos:prefLabel xml:lang="en">MCQ.35 - Fully Implemented</skos:prefLabel>
    <skos:altLabel xml:lang="en">(iii)</skos:altLabel>
    <skos:definition xml:lang="en">Information security is fully integrated into project management processes, ensuring that security considerations are addressed throughout the project lifecycle, from initiation to completion.</skos:definition>
  </rdf:Description>
  
  <rdf:Description rdf:about="https://jenni0608.github.io/NIS2V-Vocabulary-Repository/assessment_ontology_final/MCQ60AnswerB">
    <rdf:type rdf:resource="https://jenni0608.github.io/NIS2V-Vocabulary-Repository/assessment_ontology_final/MCQ.60"/>
    <skos:prefLabel xml:lang="en">MCQ.60 - Partially Implemented</skos:prefLabel>
    <skos:altLabel xml:lang="en">(ii)</skos:altLabel>
    <skos:definition xml:lang="en">There are some efforts to manage the complete life cycle of identities, including activities such as creation, authentication, and periodic reviews, but the process may be inconsistent or lacking in certain areas.</skos:definition>
  </rdf:Description>
  
  <rdf:Description rdf:about="https://jenni0608.github.io/NIS2V-Vocabulary-Repository/assessment_ontology_final/MCQ53AnswerB">
    <rdf:type rdf:resource="https://jenni0608.github.io/NIS2V-Vocabulary-Repository/assessment_ontology_final/MCQ.53"/>
    <skos:prefLabel xml:lang="en">MCQ.53 - Partially Implemented</skos:prefLabel>
    <skos:altLabel xml:lang="en">(ii)</skos:altLabel>
    <skos:definition xml:lang="en">There are some efforts to review compliance with the organisation’s information security policy, topic-specific policies, rules, and standards, but the reviews may be inconsistent or not conducted regularly.</skos:definition>
  </rdf:Description>
  
  <rdf:Description rdf:about="https://jenni0608.github.io/NIS2V-Vocabulary-Repository/assessment_ontology_final/MCQ105.3AnswerB">
    <rdf:type rdf:resource="https://jenni0608.github.io/NIS2V-Vocabulary-Repository/assessment_ontology_final/MCQ.105.3"/>
    <skos:prefLabel xml:lang="en">MCQ.105.3 - Partially Implemented</skos:prefLabel>
    <skos:altLabel xml:lang="en">(ii)</skos:altLabel>
    <skos:definition xml:lang="en">Some efforts are made to ensure resource availability for the ISMS, but there are inconsistencies or gaps in resource allocation.</skos:definition>
  </rdf:Description>
  
  <rdf:Description rdf:about="https://jenni0608.github.io/NIS2V-Vocabulary-Repository/assessment_ontology_final/MCQ109AnswerB">
    <rdf:type rdf:resource="https://jenni0608.github.io/NIS2V-Vocabulary-Repository/assessment_ontology_final/MCQ.109"/>
    <skos:prefLabel xml:lang="en">MCQ.109 - Partially Implemented</skos:prefLabel>
    <skos:altLabel xml:lang="en">(ii)</skos:altLabel>
    <skos:definition xml:lang="en">Some efforts are made to control documented information, but there are inconsistencies or gaps in activities such as distribution, access, retrieval, use, storage, preservation, change control, retention, and disposition, resulting in potential risks to information security.</skos:definition>
  </rdf:Description>
  
  <rdf:Description rdf:about="https://jenni0608.github.io/NIS2V-Vocabulary-Repository/assessment_ontology_final/MCQ88AnswerA">
    <rdf:type rdf:resource="https://jenni0608.github.io/NIS2V-Vocabulary-Repository/assessment_ontology_final/MCQ.88"/>
    <skos:prefLabel xml:lang="en">MCQ.88 - Not Implemented</skos:prefLabel>
    <skos:altLabel xml:lang="en">(i)</skos:altLabel>
    <skos:definition xml:lang="en">The use of utility programs capable of overriding system and application controls is not restricted or tightly controlled, posing risks of unauthorized system modifications or security breaches.</skos:definition>
  </rdf:Description>
  
  <rdf:Description rdf:about="https://jenni0608.github.io/NIS2V-Vocabulary-Repository/assessment_ontology_final/MCQ116AnswerA">
    <rdf:type rdf:resource="https://jenni0608.github.io/NIS2V-Vocabulary-Repository/assessment_ontology_final/MCQ.116"/>
    <skos:prefLabel xml:lang="en">MCQ.116 - Not Implemented</skos:prefLabel>
    <skos:altLabel xml:lang="en">(i)</skos:altLabel>
    <skos:definition xml:lang="en">The results of the management review do not include decisions related to continual improvement opportunities or changes to the ISMS, leading to missed opportunities for enhancing information security.</skos:definition>
  </rdf:Description>
  
  <rdf:Description rdf:about="https://jenni0608.github.io/NIS2V-Vocabulary-Repository/assessment_ontology_final/MCQ48AnswerB">
    <rdf:type rdf:resource="https://jenni0608.github.io/NIS2V-Vocabulary-Repository/assessment_ontology_final/MCQ.48"/>
    <skos:prefLabel xml:lang="en">MCQ.48 - Partially Implemented</skos:prefLabel>
    <skos:altLabel xml:lang="en">(ii)</skos:altLabel>
    <skos:definition xml:lang="en">There are some efforts to manage access to external websites to reduce exposure to malicious content, but the management process may be inconsistent or not fully comprehensive.</skos:definition>
  </rdf:Description>
  
  <rdf:Description rdf:about="https://jenni0608.github.io/NIS2V-Vocabulary-Repository/assessment_ontology_final/MCQ98AnswerA">
    <rdf:type rdf:resource="https://jenni0608.github.io/NIS2V-Vocabulary-Repository/assessment_ontology_final/MCQ.98"/>
    <skos:prefLabel xml:lang="en">MCQ.98 - Not Implemented</skos:prefLabel>
    <skos:altLabel xml:lang="en">(i)</skos:altLabel>
    <skos:definition xml:lang="en">There is no management process in place to control the allocation and management of authentication information, and personnel are not advised on the appropriate handling of authentication information.</skos:definition>
  </rdf:Description>
  
  <rdf:Description rdf:about="https://jenni0608.github.io/NIS2V-Vocabulary-Repository/assessment_ontology_final/MCQ6.4AnswerB">
    <rdf:type rdf:resource="https://jenni0608.github.io/NIS2V-Vocabulary-Repository/assessment_ontology_final/MCQ.6.4"/>
    <skos:prefLabel xml:lang="en">MCQ.6.4 - Partially Implemented</skos:prefLabel>
    <skos:altLabel xml:lang="en">(ii)</skos:altLabel>
    <skos:definition xml:lang="en">There are some established methods or protocols for how to communicate information about the information security management system, but they may not be consistently applied.</skos:definition>
  </rdf:Description>
  
  <rdf:Description rdf:about="https://jenni0608.github.io/NIS2V-Vocabulary-Repository/assessment_ontology_final/MCQ21AnswerB">
    <rdf:type rdf:resource="https://jenni0608.github.io/NIS2V-Vocabulary-Repository/assessment_ontology_final/MCQ.21"/>
    <skos:prefLabel xml:lang="en">MCQ.21 - Partially Implemented</skos:prefLabel>
    <skos:altLabel xml:lang="en">(ii)</skos:altLabel>
    <skos:definition xml:lang="en">There are some efforts to utilize knowledge gained from information security incidents to strengthen and improve information security controls, but the process may be inconsistent or not fully comprehensive.</skos:definition>
  </rdf:Description>
  
  <rdf:Description rdf:about="https://jenni0608.github.io/NIS2V-Vocabulary-Repository/assessment_ontology_final/MCQ5.5AnswerA">
    <rdf:type rdf:resource="https://jenni0608.github.io/NIS2V-Vocabulary-Repository/assessment_ontology_final/MCQ.5.5"/>
    <skos:prefLabel xml:lang="en">MCQ.5.5 - Not Implemented</skos:prefLabel>
    <skos:altLabel xml:lang="en">(i)</skos:altLabel>
    <skos:definition xml:lang="en">The organisation does not obtain approval from risk owners for the information security risk treatment plan and acceptance of residual information security risks, potentially leading to misunderstandings and lack of accountability in managing security risks.</skos:definition>
  </rdf:Description>
  
  <rdf:Description rdf:about="https://jenni0608.github.io/NIS2V-Vocabulary-Repository/assessment_ontology_final/MCQ45AnswerB">
    <rdf:type rdf:resource="https://jenni0608.github.io/NIS2V-Vocabulary-Repository/assessment_ontology_final/MCQ.45"/>
    <skos:prefLabel xml:lang="en">MCQ.45 - Partially Implemented</skos:prefLabel>
    <skos:altLabel xml:lang="en">(ii)</skos:altLabel>
    <skos:definition xml:lang="en">There are some efforts to secure, manage, and control networks and network devices to protect information in systems and applications, but the measures may be inconsistent or insufficient in certain areas.</skos:definition>
  </rdf:Description>
  
  <rdf:Description rdf:about="https://jenni0608.github.io/NIS2V-Vocabulary-Repository/assessment_ontology_final/MCQ114AnswerB">
    <rdf:type rdf:resource="https://jenni0608.github.io/NIS2V-Vocabulary-Repository/assessment_ontology_final/MCQ.114"/>
    <skos:prefLabel xml:lang="en">MCQ.114 - Partially Implemented</skos:prefLabel>
    <skos:altLabel xml:lang="en">(ii)</skos:altLabel>
    <skos:definition xml:lang="en">Some efforts are made to establish an audit program, but there are inconsistencies or gaps in defining frequency, methods, responsibilities, planning requirements, and reporting. There is partial consideration of the importance of processes and results of previous audits.</skos:definition>
  </rdf:Description>
  
  <rdf:Description rdf:about="https://jenni0608.github.io/NIS2V-Vocabulary-Repository/assessment_ontology_final/MCQ110AnswerB">
    <rdf:type rdf:resource="https://jenni0608.github.io/NIS2V-Vocabulary-Repository/assessment_ontology_final/MCQ.110"/>
    <skos:prefLabel xml:lang="en">MCQ.110 - Partially Implemented</skos:prefLabel>
    <skos:altLabel xml:lang="en">(ii)</skos:altLabel>
    <skos:definition xml:lang="en">Some criteria are established for processes, and there is partial control over them. However, there are inconsistencies or gaps in controlling planned changes and reviewing unintended changes, resulting in potential issues in meeting requirements.</skos:definition>
  </rdf:Description>
  
  <rdf:Description rdf:about="https://jenni0608.github.io/NIS2V-Vocabulary-Repository/assessment_ontology_final/MCQ111AnswerB">
    <rdf:type rdf:resource="https://jenni0608.github.io/NIS2V-Vocabulary-Repository/assessment_ontology_final/MCQ.111"/>
    <skos:prefLabel xml:lang="en">MCQ.111 - Partially Implemented</skos:prefLabel>
    <skos:altLabel xml:lang="en">(ii)</skos:altLabel>
    <skos:definition xml:lang="en">Some efforts are made to separate and secure development, testing, and production environments, but there are inconsistencies or gaps in implementation, resulting in potential vulnerabilities.</skos:definition>
  </rdf:Description>
  
  <rdf:Description rdf:about="https://jenni0608.github.io/NIS2V-Vocabulary-Repository/assessment_ontology_final/MCQ22.2AnswerB">
    <rdf:type rdf:resource="https://jenni0608.github.io/NIS2V-Vocabulary-Repository/assessment_ontology_final/MCQ.22.2"/>
    <skos:prefLabel xml:lang="en">MCQ.22.2 - Partially Implemented</skos:prefLabel>
    <skos:altLabel xml:lang="en">(ii)</skos:altLabel>
    <skos:definition xml:lang="en">There are some efforts to include information security objectives in the information security policy or provide a framework for setting them. However, objectives may not be clearly defined or may lack alignment with organisational goals.</skos:definition>
  </rdf:Description>
  
  <rdf:Description rdf:about="https://jenni0608.github.io/NIS2V-Vocabulary-Repository/assessment_ontology_final/MCQ2.1AnswerC">
    <rdf:type rdf:resource="https://jenni0608.github.io/NIS2V-Vocabulary-Repository/assessment_ontology_final/MCQ.2.1"/>
    <skos:prefLabel xml:lang="en">MCQ.2.1 - Fully Implemented</skos:prefLabel>
    <skos:altLabel xml:lang="en">(iii)</skos:altLabel>
    <skos:definition xml:lang="en">Top management fully establishes an information security policy appropriate to the organisation's purpose, which includes information security objectives or provides a framework for setting them. Moreover, there is a clear commitment to satisfying applicable requirements related to information security and continually improving the information security management system.</skos:definition>
  </rdf:Description>
  
  <rdf:Description rdf:about="https://jenni0608.github.io/NIS2V-Vocabulary-Repository/assessment_ontology_final/MCQ88AnswerB">
    <rdf:type rdf:resource="https://jenni0608.github.io/NIS2V-Vocabulary-Repository/assessment_ontology_final/MCQ.88"/>
    <skos:prefLabel xml:lang="en">MCQ.88 - Partially Implemented</skos:prefLabel>
    <skos:altLabel xml:lang="en">(ii)</skos:altLabel>
    <skos:definition xml:lang="en">There are some efforts to restrict and tightly control the use of utility programs capable of overriding system and application controls, but the controls may be inconsistent or not fully comprehensive.</skos:definition>
  </rdf:Description>
  
  <rdf:Description rdf:about="https://jenni0608.github.io/NIS2V-Vocabulary-Repository/assessment_ontology_final/MCQ18AnswerA">
    <rdf:type rdf:resource="https://jenni0608.github.io/NIS2V-Vocabulary-Repository/assessment_ontology_final/MCQ.18"/>
    <skos:prefLabel xml:lang="en">MCQ.18 - Not Implemented</skos:prefLabel>
    <skos:altLabel xml:lang="en">(i)</skos:altLabel>
    <skos:definition xml:lang="en">The organisation lacks defined information security incident management processes, roles, and responsibilities, and there is no communication regarding incident management preparedness.</skos:definition>
  </rdf:Description>
  
  <rdf:Description rdf:about="https://jenni0608.github.io/NIS2V-Vocabulary-Repository/assessment_ontology_final/MCQ120AnswerB">
    <rdf:type rdf:resource="https://jenni0608.github.io/NIS2V-Vocabulary-Repository/assessment_ontology_final/MCQ.120"/>
    <skos:prefLabel xml:lang="en">MCQ.120 - Partially Implemented</skos:prefLabel>
    <skos:altLabel xml:lang="en">(ii)</skos:altLabel>
    <skos:definition xml:lang="en">Some procedures for information labeling are developed, but there are inconsistencies or gaps in alignment with the adopted information classification scheme, resulting in limited effectiveness.</skos:definition>
  </rdf:Description>
  
  <rdf:Description rdf:about="https://jenni0608.github.io/NIS2V-Vocabulary-Repository/assessment_ontology_final/MCQ130AnswerB">
    <rdf:type rdf:resource="https://jenni0608.github.io/NIS2V-Vocabulary-Repository/assessment_ontology_final/MCQ.130"/>
    <skos:prefLabel xml:lang="en">MCQ.130 - Partially Implemented</skos:prefLabel>
    <skos:altLabel xml:lang="en">(ii)</skos:altLabel>
    <skos:definition xml:lang="en">Some maintenance procedures are in place, but there are inconsistencies or gaps in their implementation, potentially resulting in reduced reliability and security of equipment.</skos:definition>
  </rdf:Description>
  
  <rdf:Description rdf:about="https://jenni0608.github.io/NIS2V-Vocabulary-Repository/assessment_ontology_final/MCQ107.1AnswerC">
    <rdf:type rdf:resource="https://jenni0608.github.io/NIS2V-Vocabulary-Repository/assessment_ontology_final/MCQ.107.1"/>
    <skos:prefLabel xml:lang="en">MCQ.107.1 - Fully Implemented</skos:prefLabel>
    <skos:altLabel xml:lang="en">(iii)</skos:altLabel>
    <skos:definition xml:lang="en">The organisation ensures that information security objectives are established consistently across relevant functions and levels. These objectives are aligned with the information security policy, measurable (where practicable), consider applicable requirements and risk assessment results, and are monitored, communicated, updated as needed, and documented.</skos:definition>
  </rdf:Description>
  
  <rdf:Description rdf:about="https://jenni0608.github.io/NIS2V-Vocabulary-Repository/assessment_ontology_final/MCQ43AnswerB">
    <rdf:type rdf:resource="https://jenni0608.github.io/NIS2V-Vocabulary-Repository/assessment_ontology_final/MCQ.43"/>
    <skos:prefLabel xml:lang="en">MCQ.43 - Partially Implemented</skos:prefLabel>
    <skos:altLabel xml:lang="en">(ii)</skos:altLabel>
    <skos:definition xml:lang="en">There are some efforts to establish, document, implement, monitor, and review configurations, including security configurations, of hardware, software, services, and networks, but the process may be inconsistent or incomplete.</skos:definition>
  </rdf:Description>
  
  <rdf:Description rdf:about="https://jenni0608.github.io/NIS2V-Vocabulary-Repository/assessment_ontology_final/MCQ117AnswerA">
    <rdf:type rdf:resource="https://jenni0608.github.io/NIS2V-Vocabulary-Repository/assessment_ontology_final/MCQ.117"/>
    <skos:prefLabel xml:lang="en">MCQ.117 - Not Implemented</skos:prefLabel>
    <skos:altLabel xml:lang="en">(i)</skos:altLabel>
    <skos:definition xml:lang="en">The organisation does not have processes in place to continually improve the ISMS, resulting in stagnation and potential vulnerabilities.</skos:definition>
  </rdf:Description>
  
  <rdf:Description rdf:about="https://jenni0608.github.io/NIS2V-Vocabulary-Repository/assessment_ontology_final/MCQ9.1AnswerC">
    <rdf:type rdf:resource="https://jenni0608.github.io/NIS2V-Vocabulary-Repository/assessment_ontology_final/MCQ.9.1"/>
    <skos:prefLabel xml:lang="en">MCQ.9.1 - Fully Implemented</skos:prefLabel>
    <skos:altLabel xml:lang="en">(iii)</skos:altLabel>
    <skos:definition xml:lang="en">The organisation fully implements the information security risk treatment plan, ensuring that identified risks are addressed effectively and appropriate measures are put in place to mitigate or manage those risks.</skos:definition>
  </rdf:Description>
  
  <rdf:Description rdf:about="https://jenni0608.github.io/NIS2V-Vocabulary-Repository/assessment_ontology_final/MCQ22.4AnswerA">
    <rdf:type rdf:resource="https://jenni0608.github.io/NIS2V-Vocabulary-Repository/assessment_ontology_final/MCQ.22.4"/>
    <skos:prefLabel xml:lang="en">MCQ.22.4 - Not Implemented</skos:prefLabel>
    <skos:altLabel xml:lang="en">(i)</skos:altLabel>
    <skos:definition xml:lang="en">The information security policy does not include a commitment to continual improvement of the information security management system, potentially hindering the organisation's ability to adapt to evolving security threats and challenges.</skos:definition>
  </rdf:Description>
  
  <rdf:Description rdf:about="https://jenni0608.github.io/NIS2V-Vocabulary-Repository/assessment_ontology_final/MCQ6.3AnswerA">
    <rdf:type rdf:resource="https://jenni0608.github.io/NIS2V-Vocabulary-Repository/assessment_ontology_final/MCQ.6.3"/>
    <skos:prefLabel xml:lang="en">MCQ.6.3 - Not Implemented</skos:prefLabel>
    <skos:altLabel xml:lang="en">(i)</skos:altLabel>
    <skos:definition xml:lang="en">The organisation lacks clarity on whom to communicate with regarding the information security management system.</skos:definition>
  </rdf:Description>
  
  <rdf:Description rdf:about="https://jenni0608.github.io/NIS2V-Vocabulary-Repository/assessment_ontology_final/MCQ3.2AnswerB">
    <rdf:type rdf:resource="https://jenni0608.github.io/NIS2V-Vocabulary-Repository/assessment_ontology_final/MCQ.3.2"/>
    <skos:prefLabel xml:lang="en">MCQ.3.2 - Partially Implemented</skos:prefLabel>
    <skos:altLabel xml:lang="en">(ii)</skos:altLabel>
    <skos:definition xml:lang="en">There are some efforts by top management to assign the responsibility and authority for ensuring that the information security management system conforms to its requirements, and reporting on its performance to top management. However, the assignment of responsibilities and reporting mechanisms may be inconsistent or not fully comprehensive.</skos:definition>
  </rdf:Description>
  
  <rdf:Description rdf:about="https://jenni0608.github.io/NIS2V-Vocabulary-Repository/assessment_ontology_final/MCQ4.1AnswerA">
    <rdf:type rdf:resource="https://jenni0608.github.io/NIS2V-Vocabulary-Repository/assessment_ontology_final/MCQ.4.1"/>
    <skos:prefLabel xml:lang="en">MCQ.4.1 - Not Implemented</skos:prefLabel>
    <skos:altLabel xml:lang="en">(i)</skos:altLabel>
    <skos:definition xml:lang="en">The organisation does not establish and maintain information security risk criteria, including risk acceptance criteria and criteria for performing information security risk assessments, potentially leading to inconsistent or inadequate risk management practices.</skos:definition>
  </rdf:Description>
  
  <rdf:Description rdf:about="https://jenni0608.github.io/NIS2V-Vocabulary-Repository/assessment_ontology_final/MCQ55.1AnswerC">
    <rdf:type rdf:resource="https://jenni0608.github.io/NIS2V-Vocabulary-Repository/assessment_ontology_final/MCQ.55.1"/>
    <skos:prefLabel xml:lang="en">MCQ.55.1 - Fully Implemented</skos:prefLabel>
    <skos:altLabel xml:lang="en">(iii)</skos:altLabel>
    <skos:definition xml:lang="en">Persons doing work under the organisation’s control are fully aware of the information security policy, ensuring widespread understanding and adherence to security protocols and procedures.</skos:definition>
  </rdf:Description>
  
  <rdf:Description rdf:about="https://jenni0608.github.io/NIS2V-Vocabulary-Repository/assessment_ontology_final/MCQ60AnswerA">
    <rdf:type rdf:resource="https://jenni0608.github.io/NIS2V-Vocabulary-Repository/assessment_ontology_final/MCQ.60"/>
    <skos:prefLabel xml:lang="en">MCQ.60 - Not Implemented</skos:prefLabel>
    <skos:altLabel xml:lang="en">(i)</skos:altLabel>
    <skos:definition xml:lang="en">The organisation lacks a structured approach to managing the complete life cycle of identities, leading to inconsistencies and gaps in identity-related processes such as creation, authentication, and termination.</skos:definition>
  </rdf:Description>
  
  <rdf:Description rdf:about="https://jenni0608.github.io/NIS2V-Vocabulary-Repository/assessment_ontology_final/MCQ104AnswerC">
    <rdf:type rdf:resource="https://jenni0608.github.io/NIS2V-Vocabulary-Repository/assessment_ontology_final/MCQ.104"/>
    <skos:prefLabel xml:lang="en">MCQ.104 - Fully Implemented</skos:prefLabel>
    <skos:altLabel xml:lang="en">(iii)</skos:altLabel>
    <skos:definition xml:lang="en">The organisation has robust processes in place to systematically establish, implement, maintain, and continually improve its ISMS in strict accordance with the requirements outlined in ISO27001, ensuring comprehensive security measures are in place and effectively managed.</skos:definition>
  </rdf:Description>
  
  <rdf:Description rdf:about="https://jenni0608.github.io/NIS2V-Vocabulary-Repository/assessment_ontology_final/MCQ108AnswerB">
    <rdf:type rdf:resource="https://jenni0608.github.io/NIS2V-Vocabulary-Repository/assessment_ontology_final/MCQ.108"/>
    <skos:prefLabel xml:lang="en">MCQ.108 - Partially Implemented</skos:prefLabel>
    <skos:altLabel xml:lang="en">(ii)</skos:altLabel>
    <skos:definition xml:lang="en">Some effort is made to determine and provide resources for the ISMS, but there are inconsistencies or gaps in resource allocation, leading to challenges in maintaining and improving the system.</skos:definition>
  </rdf:Description>
  
  <rdf:Description rdf:about="https://jenni0608.github.io/NIS2V-Vocabulary-Repository/assessment_ontology_final/MCQ126AnswerB">
    <rdf:type rdf:resource="https://jenni0608.github.io/NIS2V-Vocabulary-Repository/assessment_ontology_final/MCQ.126"/>
    <skos:prefLabel xml:lang="en">MCQ.126 - Partially Implemented</skos:prefLabel>
    <skos:altLabel xml:lang="en">(ii)</skos:altLabel>
    <skos:definition xml:lang="en">Some efforts are made to identify, specify, and approve information security requirements, but there are inconsistencies or gaps in the process, potentially resulting in inadequate security measures.</skos:definition>
  </rdf:Description>
  
  <rdf:Description rdf:about="https://jenni0608.github.io/NIS2V-Vocabulary-Repository/assessment_ontology_final/MCQ124AnswerA">
    <rdf:type rdf:resource="https://jenni0608.github.io/NIS2V-Vocabulary-Repository/assessment_ontology_final/MCQ.124"/>
    <skos:prefLabel xml:lang="en">MCQ.124 - Not Implemented</skos:prefLabel>
    <skos:altLabel xml:lang="en">(i)</skos:altLabel>
    <skos:definition xml:lang="en">The organisation lacks measures to protect records, leading to risks such as loss, destruction, falsification, unauthorized access, and unauthorized release.</skos:definition>
  </rdf:Description>
  
  <rdf:Description rdf:about="https://jenni0608.github.io/NIS2V-Vocabulary-Repository/assessment_ontology_final/MCQ1AnswerB">
    <rdf:type rdf:resource="https://jenni0608.github.io/NIS2V-Vocabulary-Repository/assessment_ontology_final/MCQ.1"/>
    <skos:prefLabel xml:lang="en">MCQ.1 - Partially Implemented</skos:prefLabel>
    <skos:altLabel xml:lang="en">(ii)</skos:altLabel>
    <skos:definition xml:lang="en">There are some efforts to determine external and internal issues relevant to the organisation's purpose and their impact on achieving the intended outcomes of its information security management system, but the process may be inconsistent or incomplete.</skos:definition>
  </rdf:Description>
  
  <rdf:Description rdf:about="https://jenni0608.github.io/NIS2V-Vocabulary-Repository/assessment_ontology_final/MCQ55.3AnswerA">
    <rdf:type rdf:resource="https://jenni0608.github.io/NIS2V-Vocabulary-Repository/assessment_ontology_final/MCQ.55.3"/>
    <skos:prefLabel xml:lang="en">MCQ.55.3 - Not Implemented</skos:prefLabel>
    <skos:altLabel xml:lang="en">(i)</skos:altLabel>
    <skos:definition xml:lang="en">Persons doing work under the organisation’s control are not aware of the implications of not conforming with the information security management system requirements, potentially leading to inadvertent non-compliance and increased security risks.</skos:definition>
  </rdf:Description>
  
  <rdf:Description rdf:about="https://jenni0608.github.io/NIS2V-Vocabulary-Repository/assessment_ontology_final/MCQ22.2AnswerA">
    <rdf:type rdf:resource="https://jenni0608.github.io/NIS2V-Vocabulary-Repository/assessment_ontology_final/MCQ.22.2"/>
    <skos:prefLabel xml:lang="en">MCQ.22.2 - Not Implemented</skos:prefLabel>
    <skos:altLabel xml:lang="en">(i)</skos:altLabel>
    <skos:definition xml:lang="en">The information security policy does not include information security objectives or provide a framework for setting them, potentially leading to ambiguity and lack of direction in achieving security goals.</skos:definition>
  </rdf:Description>
  
  <rdf:Description rdf:about="https://jenni0608.github.io/NIS2V-Vocabulary-Repository/assessment_ontology_final/MCQ8.1AnswerC">
    <rdf:type rdf:resource="https://jenni0608.github.io/NIS2V-Vocabulary-Repository/assessment_ontology_final/MCQ.8.1"/>
    <skos:prefLabel xml:lang="en">MCQ.8.1 - Fully Implemented</skos:prefLabel>
    <skos:altLabel xml:lang="en">(iii)</skos:altLabel>
    <skos:definition xml:lang="en">The organisation fully conducts information security risk assessments at planned intervals or when significant changes are proposed or occur, ensuring proactive risk management and mitigation strategies are in place.</skos:definition>
  </rdf:Description>
  
  <rdf:Description rdf:about="https://jenni0608.github.io/NIS2V-Vocabulary-Repository/assessment_ontology_final/MCQ55.2AnswerB">
    <rdf:type rdf:resource="https://jenni0608.github.io/NIS2V-Vocabulary-Repository/assessment_ontology_final/MCQ.55.2"/>
    <skos:prefLabel xml:lang="en">MCQ.55.2 - Partially Implemented</skos:prefLabel>
    <skos:altLabel xml:lang="en">(ii)</skos:altLabel>
    <skos:definition xml:lang="en">There are some efforts to make persons doing work under the organisation’s control aware of their contribution to the effectiveness of the information security management system and the benefits of improved information security performance. However, awareness may be inconsistent or not fully comprehensive.</skos:definition>
  </rdf:Description>
  
  <rdf:Description rdf:about="https://jenni0608.github.io/NIS2V-Vocabulary-Repository/assessment_ontology_final/MCQ22.1AnswerC">
    <rdf:type rdf:resource="https://jenni0608.github.io/NIS2V-Vocabulary-Repository/assessment_ontology_final/MCQ.22.1"/>
    <skos:prefLabel xml:lang="en">MCQ.22.1 - Fully Implemented</skos:prefLabel>
    <skos:altLabel xml:lang="en">(iii)</skos:altLabel>
    <skos:definition xml:lang="en">Top management has fully established an information security policy that is appropriate to the purpose of the organisation, providing clear direction and guidance in managing information security in alignment with organisational objectives.</skos:definition>
  </rdf:Description>
  
  <rdf:Description rdf:about="https://jenni0608.github.io/NIS2V-Vocabulary-Repository/assessment_ontology_final/MCQ53AnswerA">
    <rdf:type rdf:resource="https://jenni0608.github.io/NIS2V-Vocabulary-Repository/assessment_ontology_final/MCQ.53"/>
    <skos:prefLabel xml:lang="en">MCQ.53 - Not Implemented</skos:prefLabel>
    <skos:altLabel xml:lang="en">(i)</skos:altLabel>
    <skos:definition xml:lang="en">Compliance with the organisation’s information security policy, topic-specific policies, rules, and standards is not regularly reviewed, potentially leading to non-compliance and increased security risks.</skos:definition>
  </rdf:Description>
  
  <rdf:Description rdf:about="https://jenni0608.github.io/NIS2V-Vocabulary-Repository/assessment_ontology_final/MCQ56.3AnswerA">
    <rdf:type rdf:resource="https://jenni0608.github.io/NIS2V-Vocabulary-Repository/assessment_ontology_final/MCQ.56.3"/>
    <skos:prefLabel xml:lang="en">MCQ.56.3 - Not Implemented</skos:prefLabel>
    <skos:altLabel xml:lang="en">(i)</skos:altLabel>
    <skos:definition xml:lang="en">The organisation lacks clarity on whom to communicate with regarding the information security management system.</skos:definition>
  </rdf:Description>
  
  <rdf:Description rdf:about="https://jenni0608.github.io/NIS2V-Vocabulary-Repository/assessment_ontology_final/MCQ4.4AnswerB">
    <rdf:type rdf:resource="https://jenni0608.github.io/NIS2V-Vocabulary-Repository/assessment_ontology_final/MCQ.4.4"/>
    <skos:prefLabel xml:lang="en">MCQ.4.4 - Partially Implemented</skos:prefLabel>
    <skos:altLabel xml:lang="en">(ii)</skos:altLabel>
    <skos:definition xml:lang="en">There are some efforts by the organisation to analyse information security risks, including assessing the potential consequences and realistic likelihood of the occurrence of identified risks. However, the process may be inconsistent or not fully comprehensive in determining the levels of risk.</skos:definition>
  </rdf:Description>
  
  <rdf:Description rdf:about="https://jenni0608.github.io/NIS2V-Vocabulary-Repository/assessment_ontology_final/MCQ106.2AnswerA">
    <rdf:type rdf:resource="https://jenni0608.github.io/NIS2V-Vocabulary-Repository/assessment_ontology_final/MCQ.106.2"/>
    <skos:prefLabel xml:lang="en">MCQ.106.2 - Not Implemented</skos:prefLabel>
    <skos:altLabel xml:lang="en">(i)</skos:altLabel>
    <skos:definition xml:lang="en">The organisation does not plan actions to address identified risks and opportunities for the ISMS, leading to a lack of proactive measures to mitigate potential threats or leverage opportunities for improvement.</skos:definition>
  </rdf:Description>
  
  <rdf:Description rdf:about="https://jenni0608.github.io/NIS2V-Vocabulary-Repository/assessment_ontology_final/MCQ42AnswerA">
    <rdf:type rdf:resource="https://jenni0608.github.io/NIS2V-Vocabulary-Repository/assessment_ontology_final/MCQ.42"/>
    <skos:prefLabel xml:lang="en">MCQ.42 - Not Implemented</skos:prefLabel>
    <skos:altLabel xml:lang="en">(i)</skos:altLabel>
    <skos:definition xml:lang="en">The organisation does not acquire information regarding technical vulnerabilities of the information systems in use, evaluate its exposure to such vulnerabilities, or implement appropriate measures, leaving it susceptible to potential security breaches.</skos:definition>
  </rdf:Description>
  
  <rdf:Description rdf:about="https://jenni0608.github.io/NIS2V-Vocabulary-Repository/assessment_ontology_final/MCQ111AnswerC">
    <rdf:type rdf:resource="https://jenni0608.github.io/NIS2V-Vocabulary-Repository/assessment_ontology_final/MCQ.111"/>
    <skos:prefLabel xml:lang="en">MCQ.111 - Fully Implemented</skos:prefLabel>
    <skos:altLabel xml:lang="en">(iii)</skos:altLabel>
    <skos:definition xml:lang="en">The organisation ensures the strict separation and robust security measures for development, testing, and production environments, minimizing risks and ensuring the integrity, confidentiality, and availability of data and systems.</skos:definition>
  </rdf:Description>
  
  <rdf:Description rdf:about="https://jenni0608.github.io/NIS2V-Vocabulary-Repository/assessment_ontology_final/MCQ21AnswerA">
    <rdf:type rdf:resource="https://jenni0608.github.io/NIS2V-Vocabulary-Repository/assessment_ontology_final/MCQ.21"/>
    <skos:prefLabel xml:lang="en">MCQ.21 - Not Implemented</skos:prefLabel>
    <skos:altLabel xml:lang="en">(i)</skos:altLabel>
    <skos:definition xml:lang="en">Knowledge gained from information security incidents is not systematically utilized to strengthen or improve information security controls, resulting in missed opportunities for enhancement.</skos:definition>
  </rdf:Description>
  
  <rdf:Description rdf:about="https://jenni0608.github.io/NIS2V-Vocabulary-Repository/assessment_ontology_final/MCQ106.1AnswerC">
    <rdf:type rdf:resource="https://jenni0608.github.io/NIS2V-Vocabulary-Repository/assessment_ontology_final/MCQ.106.1"/>
    <skos:prefLabel xml:lang="en">MCQ.106.1 - Fully Implemented</skos:prefLabel>
    <skos:altLabel xml:lang="en">(iii)</skos:altLabel>
    <skos:definition xml:lang="en">The organisation systematically considers its context and the needs and expectations of interested parties when planning for the ISMS, ensuring that risks and opportunities are identified and addressed to achieve intended outcomes, prevent undesired effects, and promote continual improvement.</skos:definition>
  </rdf:Description>
  
  <rdf:Description rdf:about="https://jenni0608.github.io/NIS2V-Vocabulary-Repository/assessment_ontology_final/MCQ10.2AnswerA">
    <rdf:type rdf:resource="https://jenni0608.github.io/NIS2V-Vocabulary-Repository/assessment_ontology_final/MCQ.10.2"/>
    <skos:prefLabel xml:lang="en">MCQ.10.2 - Not Implemented</skos:prefLabel>
    <skos:altLabel xml:lang="en">(i)</skos:altLabel>
    <skos:definition xml:lang="en">The policy is not reviewed at planned intervals or if significant changes occur, potentially leading to outdated or ineffective security measures that do not align with evolving threats and organisational needs.</skos:definition>
  </rdf:Description>
  
  <rdf:Description rdf:about="https://jenni0608.github.io/NIS2V-Vocabulary-Repository/assessment_ontology_final/MCQ71AnswerB">
    <rdf:type rdf:resource="https://jenni0608.github.io/NIS2V-Vocabulary-Repository/assessment_ontology_final/MCQ.71"/>
    <skos:prefLabel xml:lang="en">MCQ.71 - Partially Implemented</skos:prefLabel>
    <skos:altLabel xml:lang="en">(ii)</skos:altLabel>
    <skos:definition xml:lang="en">There is a general understanding of information security roles and responsibilities, but they are not consistently defined and allocated according to the organisation's needs. Some areas may lack clarity or consistency.</skos:definition>
  </rdf:Description>
  
  <rdf:Description rdf:about="https://jenni0608.github.io/NIS2V-Vocabulary-Repository/assessment_ontology_final/MCQ6.2AnswerB">
    <rdf:type rdf:resource="https://jenni0608.github.io/NIS2V-Vocabulary-Repository/assessment_ontology_final/MCQ.6.2"/>
    <skos:prefLabel xml:lang="en">MCQ.6.2 - Partially Implemented</skos:prefLabel>
    <skos:altLabel xml:lang="en">(ii)</skos:altLabel>
    <skos:definition xml:lang="en">There are some established timings for communication related to the information security management system, but they may not cover all necessary instances.</skos:definition>
  </rdf:Description>
  
  <rdf:Description rdf:about="https://jenni0608.github.io/NIS2V-Vocabulary-Repository/assessment_ontology_final/MCQ31AnswerB">
    <rdf:type rdf:resource="https://jenni0608.github.io/NIS2V-Vocabulary-Repository/assessment_ontology_final/MCQ.31"/>
    <skos:prefLabel xml:lang="en">MCQ.31 - Partially Implemented</skos:prefLabel>
    <skos:altLabel xml:lang="en">(ii)</skos:altLabel>
    <skos:definition xml:lang="en">There are some efforts to monitor the use of resources and make adjustments based on current and expected capacity requirements, but the process may be inconsistent or not fully comprehensive.</skos:definition>
  </rdf:Description>
  
  <rdf:Description rdf:about="https://jenni0608.github.io/NIS2V-Vocabulary-Repository/assessment_ontology_final/MCQ100AnswerA">
    <rdf:type rdf:resource="https://jenni0608.github.io/NIS2V-Vocabulary-Repository/assessment_ontology_final/MCQ.100"/>
    <skos:prefLabel xml:lang="en">MCQ.100 - Not Implemented</skos:prefLabel>
    <skos:altLabel xml:lang="en">(i)</skos:altLabel>
    <skos:definition xml:lang="en">Data leakage prevention measures are not applied to systems, networks, and other devices that process, store, or transmit sensitive information, leaving the data vulnerable to unauthorized disclosure or leakage.</skos:definition>
  </rdf:Description>
  
  <rdf:Description rdf:about="https://jenni0608.github.io/NIS2V-Vocabulary-Repository/assessment_ontology_final/MCQ59AnswerC">
    <rdf:type rdf:resource="https://jenni0608.github.io/NIS2V-Vocabulary-Repository/assessment_ontology_final/MCQ.59"/>
    <skos:prefLabel xml:lang="en">MCQ.59 - Fully Implemented</skos:prefLabel>
    <skos:altLabel xml:lang="en">(iii)</skos:altLabel>
    <skos:definition xml:lang="en">Comprehensive rules are established and rigorously implemented to control physical and logical access to information and associated assets, ensuring alignment with both business and information security requirements for effective access control.</skos:definition>
  </rdf:Description>
  
  <rdf:Description rdf:about="https://jenni0608.github.io/NIS2V-Vocabulary-Repository/assessment_ontology_final/MCQ92AnswerB">
    <rdf:type rdf:resource="https://jenni0608.github.io/NIS2V-Vocabulary-Repository/assessment_ontology_final/MCQ.92"/>
    <skos:prefLabel xml:lang="en">MCQ.92 - Partially Implemented</skos:prefLabel>
    <skos:altLabel xml:lang="en">(ii)</skos:altLabel>
    <skos:definition xml:lang="en">There are some efforts to manage storage media through their life cycle, but the management may be inconsistent or not fully aligned with the organisation’s classification scheme and handling requirements.</skos:definition>
  </rdf:Description>
  
  <rdf:Description rdf:about="https://jenni0608.github.io/NIS2V-Vocabulary-Repository/assessment_ontology_final/MCQ23AnswerB">
    <rdf:type rdf:resource="https://jenni0608.github.io/NIS2V-Vocabulary-Repository/assessment_ontology_final/MCQ.23"/>
    <skos:prefLabel xml:lang="en">MCQ.23 - Partially Implemented</skos:prefLabel>
    <skos:altLabel xml:lang="en">(ii)</skos:altLabel>
    <skos:definition xml:lang="en">There are some efforts to provide a mechanism for personnel to report observed or suspected information security events through appropriate channels, but the process may be inconsistent or not fully comprehensive.</skos:definition>
  </rdf:Description>
  
  <rdf:Description rdf:about="https://jenni0608.github.io/NIS2V-Vocabulary-Repository/assessment_ontology_final/MCQ6.4AnswerC">
    <rdf:type rdf:resource="https://jenni0608.github.io/NIS2V-Vocabulary-Repository/assessment_ontology_final/MCQ.6.4"/>
    <skos:prefLabel xml:lang="en">MCQ.6.4 - Fully Implemented</skos:prefLabel>
    <skos:altLabel xml:lang="en">(iii)</skos:altLabel>
    <skos:definition xml:lang="en">There are established methods and protocols for how to communicate information about the information security management system, ensuring effective and secure communication channels.</skos:definition>
  </rdf:Description>
  
  <rdf:Description rdf:about="https://jenni0608.github.io/NIS2V-Vocabulary-Repository/assessment_ontology_final/MCQ126AnswerA">
    <rdf:type rdf:resource="https://jenni0608.github.io/NIS2V-Vocabulary-Repository/assessment_ontology_final/MCQ.126"/>
    <skos:prefLabel xml:lang="en">MCQ.126 - Not Implemented</skos:prefLabel>
    <skos:altLabel xml:lang="en">(i)</skos:altLabel>
    <skos:definition xml:lang="en">The organisation does not have a process to identify, specify, and approve information security requirements during application development or acquisition, leading to potential security vulnerabilities.</skos:definition>
  </rdf:Description>
  
  <rdf:Description rdf:about="https://jenni0608.github.io/NIS2V-Vocabulary-Repository/assessment_ontology_final/MCQ112.2AnswerB">
    <rdf:type rdf:resource="https://jenni0608.github.io/NIS2V-Vocabulary-Repository/assessment_ontology_final/MCQ.112.2"/>
    <skos:prefLabel xml:lang="en">MCQ.112.2 - Partially Implemented</skos:prefLabel>
    <skos:altLabel xml:lang="en">(ii)</skos:altLabel>
    <skos:definition xml:lang="en">Some effort is made to evaluate information security performance and ISMS effectiveness, but there are inconsistencies or gaps in the evaluation process, leading to limited insights into the system's performance and effectiveness.</skos:definition>
  </rdf:Description>
  
  <rdf:Description rdf:about="https://jenni0608.github.io/NIS2V-Vocabulary-Repository/assessment_ontology_final/MCQ10.2AnswerC">
    <rdf:type rdf:resource="https://jenni0608.github.io/NIS2V-Vocabulary-Repository/assessment_ontology_final/MCQ.10.2"/>
    <skos:prefLabel xml:lang="en">MCQ.10.2 - Fully Implemented</skos:prefLabel>
    <skos:altLabel xml:lang="en">(iii)</skos:altLabel>
    <skos:definition xml:lang="en">The policy is fully reviewed at planned intervals and if significant changes occur, ensuring that it remains up-to-date, relevant, and aligned with the organisation's objectives and evolving security landscape.</skos:definition>
  </rdf:Description>
  
  <rdf:Description rdf:about="https://jenni0608.github.io/NIS2V-Vocabulary-Repository/assessment_ontology_final/MCQ4.3AnswerC">
    <rdf:type rdf:resource="https://jenni0608.github.io/NIS2V-Vocabulary-Repository/assessment_ontology_final/MCQ.4.3"/>
    <skos:prefLabel xml:lang="en">MCQ.4.3 - Fully Implemented</skos:prefLabel>
    <skos:altLabel xml:lang="en">(iii)</skos:altLabel>
    <skos:definition xml:lang="en">The organisation fully identifies information security risks, applying the information security risk assessment process effectively to identify risks associated with the loss of confidentiality, integrity, and availability for information within the scope of the information security management system, and consistently identifies risk owners.</skos:definition>
  </rdf:Description>
  
  <rdf:Description rdf:about="https://jenni0608.github.io/NIS2V-Vocabulary-Repository/assessment_ontology_final/MCQ114AnswerC">
    <rdf:type rdf:resource="https://jenni0608.github.io/NIS2V-Vocabulary-Repository/assessment_ontology_final/MCQ.114"/>
    <skos:prefLabel xml:lang="en">MCQ.114 - Fully Implemented</skos:prefLabel>
    <skos:altLabel xml:lang="en">(iii)</skos:altLabel>
    <skos:definition xml:lang="en">The organisation plans, establishes, implements, and maintains an audit program, including defined frequency, methods, responsibilities, planning requirements, and reporting. Consideration is given to the importance of processes and results of previous audits. Auditors are selected to ensure objectivity and impartiality, and audit results are reported to relevant management. Documented information is available as evidence of the implementation of the audit program and the audit results.</skos:definition>
  </rdf:Description>
  
  <rdf:Description rdf:about="https://jenni0608.github.io/NIS2V-Vocabulary-Repository/assessment_ontology_final/MCQ78AnswerB">
    <rdf:type rdf:resource="https://jenni0608.github.io/NIS2V-Vocabulary-Repository/assessment_ontology_final/MCQ.78"/>
    <skos:prefLabel xml:lang="en">MCQ.78 - Partially Implemented</skos:prefLabel>
    <skos:altLabel xml:lang="en">(ii)</skos:altLabel>
    <skos:definition xml:lang="en">There are some efforts to identify, document, regularly review, and sign confidentiality or non-disclosure agreements reflecting the organisation's needs for the protection of information, but the process may be inconsistent or not fully comprehensive.</skos:definition>
  </rdf:Description>
  
  <rdf:Description rdf:about="https://jenni0608.github.io/NIS2V-Vocabulary-Repository/assessment_ontology_final/MCQ94AnswerC">
    <rdf:type rdf:resource="https://jenni0608.github.io/NIS2V-Vocabulary-Repository/assessment_ontology_final/MCQ.94"/>
    <skos:prefLabel xml:lang="en">MCQ.94 - Fully Implemented</skos:prefLabel>
    <skos:altLabel xml:lang="en">(iii)</skos:altLabel>
    <skos:definition xml:lang="en">Information stored on, processed by, or accessible via user endpoint devices is fully protected, ensuring confidentiality, integrity, and availability through appropriate security measures and controls.</skos:definition>
  </rdf:Description>
  
  <rdf:Description rdf:about="https://jenni0608.github.io/NIS2V-Vocabulary-Repository/assessment_ontology_final/MCQ116AnswerC">
    <rdf:type rdf:resource="https://jenni0608.github.io/NIS2V-Vocabulary-Repository/assessment_ontology_final/MCQ.116"/>
    <skos:prefLabel xml:lang="en">MCQ.116 - Fully Implemented</skos:prefLabel>
    <skos:altLabel xml:lang="en">(iii)</skos:altLabel>
    <skos:definition xml:lang="en">The results of the management review include decisions related to continual improvement opportunities and changes to the ISMS. These decisions are systematically considered and documented, ensuring that the ISMS evolves to meet current and future needs effectively.</skos:definition>
  </rdf:Description>
  
  <rdf:Description rdf:about="https://jenni0608.github.io/NIS2V-Vocabulary-Repository/assessment_ontology_final/MCQ98AnswerC">
    <rdf:type rdf:resource="https://jenni0608.github.io/NIS2V-Vocabulary-Repository/assessment_ontology_final/MCQ.98"/>
    <skos:prefLabel xml:lang="en">MCQ.98 - Fully Implemented</skos:prefLabel>
    <skos:altLabel xml:lang="en">(iii)</skos:altLabel>
    <skos:definition xml:lang="en">The allocation and management of authentication information are controlled by a robust management process, which includes advising personnel on the appropriate handling of authentication information, ensuring secure and effective management of authentication credentials.</skos:definition>
  </rdf:Description>
  
  <rdf:Description rdf:about="https://jenni0608.github.io/NIS2V-Vocabulary-Repository/assessment_ontology_final/MCQ125AnswerB">
    <rdf:type rdf:resource="https://jenni0608.github.io/NIS2V-Vocabulary-Repository/assessment_ontology_final/MCQ.125"/>
    <skos:prefLabel xml:lang="en">MCQ.125 - Partially Implemented</skos:prefLabel>
    <skos:altLabel xml:lang="en">(ii)</skos:altLabel>
    <skos:definition xml:lang="en">Some rules for secure development are established, but there are inconsistencies or gaps in their application, potentially resulting in security risks.</skos:definition>
  </rdf:Description>
  
  <rdf:Description rdf:about="https://jenni0608.github.io/NIS2V-Vocabulary-Repository/assessment_ontology_final/MCQ8.1AnswerB">
    <rdf:type rdf:resource="https://jenni0608.github.io/NIS2V-Vocabulary-Repository/assessment_ontology_final/MCQ.8.1"/>
    <skos:prefLabel xml:lang="en">MCQ.8.1 - Partially Implemented</skos:prefLabel>
    <skos:altLabel xml:lang="en">(ii)</skos:altLabel>
    <skos:definition xml:lang="en">There are some efforts by the organisation to conduct information security risk assessments at planned intervals or when significant changes are proposed or occur. However, the process may be inconsistent or not fully comprehensive.</skos:definition>
  </rdf:Description>
  
  <rdf:Description rdf:about="https://jenni0608.github.io/NIS2V-Vocabulary-Repository/assessment_ontology_final/MCQ47AnswerB">
    <rdf:type rdf:resource="https://jenni0608.github.io/NIS2V-Vocabulary-Repository/assessment_ontology_final/MCQ.47"/>
    <skos:prefLabel xml:lang="en">MCQ.47 - Partially Implemented</skos:prefLabel>
    <skos:altLabel xml:lang="en">(ii)</skos:altLabel>
    <skos:definition xml:lang="en">There are some efforts to implement segregation for groups of information services, users, and information systems within the organisation's networks, but the implementation may be incomplete or inconsistent.</skos:definition>
  </rdf:Description>
  
  <rdf:Description rdf:about="https://jenni0608.github.io/NIS2V-Vocabulary-Repository/assessment_ontology_final/MCQ56.2AnswerC">
    <rdf:type rdf:resource="https://jenni0608.github.io/NIS2V-Vocabulary-Repository/assessment_ontology_final/MCQ.56.2"/>
    <skos:prefLabel xml:lang="en">MCQ.56.2 - Fully Implemented</skos:prefLabel>
    <skos:altLabel xml:lang="en">(iii)</skos:altLabel>
    <skos:definition xml:lang="en">There are well-established timings for communication related to the information security management system, ensuring timely dissemination of information.</skos:definition>
  </rdf:Description>
  
  <rdf:Description rdf:about="https://jenni0608.github.io/NIS2V-Vocabulary-Repository/assessment_ontology_final/MCQ107.1AnswerA">
    <rdf:type rdf:resource="https://jenni0608.github.io/NIS2V-Vocabulary-Repository/assessment_ontology_final/MCQ.107.1"/>
    <skos:prefLabel xml:lang="en">MCQ.107.1 - Not Implemented</skos:prefLabel>
    <skos:altLabel xml:lang="en">(i)</skos:altLabel>
    <skos:definition xml:lang="en">The organisation does not establish information security objectives consistently across relevant functions and levels, resulting in ambiguity and inconsistency in security goals.</skos:definition>
  </rdf:Description>
  
  <rdf:Description rdf:about="https://jenni0608.github.io/NIS2V-Vocabulary-Repository/assessment_ontology_final/MCQ76AnswerA">
    <rdf:type rdf:resource="https://jenni0608.github.io/NIS2V-Vocabulary-Repository/assessment_ontology_final/MCQ.76"/>
    <skos:prefLabel xml:lang="en">MCQ.76 - Not Implemented</skos:prefLabel>
    <skos:altLabel xml:lang="en">(i)</skos:altLabel>
    <skos:definition xml:lang="en">A disciplinary process to take actions against personnel and other relevant interested parties who have committed an information security policy violation is not formalized or communicated, potentially leading to inconsistent responses and inadequate enforcement of security policies.</skos:definition>
  </rdf:Description>
  
  <rdf:Description rdf:about="https://jenni0608.github.io/NIS2V-Vocabulary-Repository/assessment_ontology_final/MCQ43AnswerA">
    <rdf:type rdf:resource="https://jenni0608.github.io/NIS2V-Vocabulary-Repository/assessment_ontology_final/MCQ.43"/>
    <skos:prefLabel xml:lang="en">MCQ.43 - Not Implemented</skos:prefLabel>
    <skos:altLabel xml:lang="en">(i)</skos:altLabel>
    <skos:definition xml:lang="en">Configurations, including security configurations, of hardware, software, services, and networks are not established, documented, implemented, monitored, or reviewed, leading to potential vulnerabilities and inconsistent security measures.</skos:definition>
  </rdf:Description>
  
  <rdf:Description rdf:about="https://jenni0608.github.io/NIS2V-Vocabulary-Repository/assessment_ontology_final/MCQ110AnswerA">
    <rdf:type rdf:resource="https://jenni0608.github.io/NIS2V-Vocabulary-Repository/assessment_ontology_final/MCQ.110"/>
    <skos:prefLabel xml:lang="en">MCQ.110 - Not Implemented</skos:prefLabel>
    <skos:altLabel xml:lang="en">(i)</skos:altLabel>
    <skos:definition xml:lang="en">The organisation lacks criteria for processes and control over them, leading to uncertainty in meeting requirements and implementing actions.</skos:definition>
  </rdf:Description>
  
  <rdf:Description rdf:about="https://jenni0608.github.io/NIS2V-Vocabulary-Repository/assessment_ontology_final/MCQ57.1AnswerA">
    <rdf:type rdf:resource="https://jenni0608.github.io/NIS2V-Vocabulary-Repository/assessment_ontology_final/MCQ.57.1"/>
    <skos:prefLabel xml:lang="en">MCQ.57.1 - Not Implemented</skos:prefLabel>
    <skos:altLabel xml:lang="en">(i)</skos:altLabel>
    <skos:definition xml:lang="en">Personnel of the organisation and relevant interested parties do not receive appropriate information security awareness, education, and training, leaving them ill-prepared to address security risks and threats.</skos:definition>
  </rdf:Description>
  
  <rdf:Description rdf:about="https://jenni0608.github.io/NIS2V-Vocabulary-Repository/assessment_ontology_final/MCQ99AnswerA">
    <rdf:type rdf:resource="https://jenni0608.github.io/NIS2V-Vocabulary-Repository/assessment_ontology_final/MCQ.99"/>
    <skos:prefLabel xml:lang="en">MCQ.99 - Not Implemented</skos:prefLabel>
    <skos:altLabel xml:lang="en">(i)</skos:altLabel>
    <skos:definition xml:lang="en">Secure authentication technologies and procedures are not implemented based on information access restrictions and the topic-specific policy on access control, resulting in potential vulnerabilities and unauthorized access.</skos:definition>
  </rdf:Description>
  
  <rdf:Description rdf:about="https://jenni0608.github.io/NIS2V-Vocabulary-Repository/assessment_ontology_final/MCQ19AnswerC">
    <rdf:type rdf:resource="https://jenni0608.github.io/NIS2V-Vocabulary-Repository/assessment_ontology_final/MCQ.19"/>
    <skos:prefLabel xml:lang="en">MCQ.19 - Fully Implemented</skos:prefLabel>
    <skos:altLabel xml:lang="en">(iii)</skos:altLabel>
    <skos:definition xml:lang="en">The organisation has a well-defined and systematic process for assessing information security events to determine if they should be categorized as incidents, ensuring prompt and accurate incident identification and response.</skos:definition>
  </rdf:Description>
  
  <rdf:Description rdf:about="https://jenni0608.github.io/NIS2V-Vocabulary-Repository/assessment_ontology_final/MCQ128AnswerB">
    <rdf:type rdf:resource="https://jenni0608.github.io/NIS2V-Vocabulary-Repository/assessment_ontology_final/MCQ.128"/>
    <skos:prefLabel xml:lang="en">MCQ.128 - Partially Implemented</skos:prefLabel>
    <skos:altLabel xml:lang="en">(ii)</skos:altLabel>
    <skos:definition xml:lang="en">Some efforts are made to direct, monitor, and review outsourced system development activities, but there are inconsistencies or gaps in the process, potentially resulting in inadequate oversight.</skos:definition>
  </rdf:Description>
  
  <rdf:Description rdf:about="https://jenni0608.github.io/NIS2V-Vocabulary-Repository/assessment_ontology_final/MCQ40AnswerB">
    <rdf:type rdf:resource="https://jenni0608.github.io/NIS2V-Vocabulary-Repository/assessment_ontology_final/MCQ.40"/>
    <skos:prefLabel xml:lang="en">MCQ.40 - Partially Implemented</skos:prefLabel>
    <skos:altLabel xml:lang="en">(ii)</skos:altLabel>
    <skos:definition xml:lang="en">There are some processes established for the acquisition, use, management, and exit from cloud services, but they may not fully align with the organisation’s information security requirements or be consistently applied.</skos:definition>
  </rdf:Description>
  
  <rdf:Description rdf:about="https://jenni0608.github.io/NIS2V-Vocabulary-Repository/assessment_ontology_final/MCQ63AnswerB">
    <rdf:type rdf:resource="https://jenni0608.github.io/NIS2V-Vocabulary-Repository/assessment_ontology_final/MCQ.63"/>
    <skos:prefLabel xml:lang="en">MCQ.63 - Partially Implemented</skos:prefLabel>
    <skos:altLabel xml:lang="en">(ii)</skos:altLabel>
    <skos:definition xml:lang="en">There are some efforts to acquire information regarding technical vulnerabilities and evaluate the organisation’s exposure to such vulnerabilities, but the implementation of appropriate measures may be inconsistent or incomplete.</skos:definition>
  </rdf:Description>
  
  <rdf:Description rdf:about="https://jenni0608.github.io/NIS2V-Vocabulary-Repository/assessment_ontology_final/MCQ123AnswerA">
    <rdf:type rdf:resource="https://jenni0608.github.io/NIS2V-Vocabulary-Repository/assessment_ontology_final/MCQ.123"/>
    <skos:prefLabel xml:lang="en">MCQ.123 - Not Implemented</skos:prefLabel>
    <skos:altLabel xml:lang="en">(i)</skos:altLabel>
    <skos:definition xml:lang="en">The organisation lacks procedures to protect intellectual property rights, leading to potential infringement and loss of valuable assets.</skos:definition>
  </rdf:Description>
  
  <rdf:Description rdf:about="https://jenni0608.github.io/NIS2V-Vocabulary-Repository/assessment_ontology_final/MCQ4.2AnswerC">
    <rdf:type rdf:resource="https://jenni0608.github.io/NIS2V-Vocabulary-Repository/assessment_ontology_final/MCQ.4.2"/>
    <skos:prefLabel xml:lang="en">MCQ.4.2 - Fully Implemented</skos:prefLabel>
    <skos:altLabel xml:lang="en">(iii)</skos:altLabel>
    <skos:definition xml:lang="en">The organisation fully ensures that repeated information security risk assessments produce consistent, valid, and comparable results, ensuring reliability and effectiveness in risk management decisions.</skos:definition>
  </rdf:Description>
  
  <rdf:Description rdf:about="https://jenni0608.github.io/NIS2V-Vocabulary-Repository/assessment_ontology_final/MCQ5.5AnswerB">
    <rdf:type rdf:resource="https://jenni0608.github.io/NIS2V-Vocabulary-Repository/assessment_ontology_final/MCQ.5.5"/>
    <skos:prefLabel xml:lang="en">MCQ.5.5 - Partially Implemented</skos:prefLabel>
    <skos:altLabel xml:lang="en">(ii)</skos:altLabel>
    <skos:definition xml:lang="en">There are some efforts by the organisation to obtain approval from risk owners for the information security risk treatment plan and acceptance of residual information security risks. However, these efforts may not be consistently applied or documented.</skos:definition>
  </rdf:Description>
  
  <rdf:Description rdf:about="https://jenni0608.github.io/NIS2V-Vocabulary-Repository/assessment_ontology_final/MCQ4.3AnswerB">
    <rdf:type rdf:resource="https://jenni0608.github.io/NIS2V-Vocabulary-Repository/assessment_ontology_final/MCQ.4.3"/>
    <skos:prefLabel xml:lang="en">MCQ.4.3 - Partially Implemented</skos:prefLabel>
    <skos:altLabel xml:lang="en">(ii)</skos:altLabel>
    <skos:definition xml:lang="en">There are some efforts by the organisation to identify information security risks, including applying the information security risk assessment process to identify risks associated with the loss of confidentiality, integrity, and availability for information within the scope of the information security management system. However, identification of risk owners may be inconsistent or not fully comprehensive.</skos:definition>
  </rdf:Description>
  
  <rdf:Description rdf:about="https://jenni0608.github.io/NIS2V-Vocabulary-Repository/assessment_ontology_final/MCQ11AnswerA">
    <rdf:type rdf:resource="https://jenni0608.github.io/NIS2V-Vocabulary-Repository/assessment_ontology_final/MCQ.11"/>
    <skos:prefLabel xml:lang="en">MCQ.11 - Not Implemented</skos:prefLabel>
    <skos:altLabel xml:lang="en">(i)</skos:altLabel>
    <skos:definition xml:lang="en">Roles and responsibilities related to information security are not clearly defined, and there is no formal allocation process based on organisational needs.</skos:definition>
  </rdf:Description>
  
  <rdf:Description rdf:about="https://jenni0608.github.io/NIS2V-Vocabulary-Repository/assessment_ontology_final/MCQ10.2AnswerB">
    <rdf:type rdf:resource="https://jenni0608.github.io/NIS2V-Vocabulary-Repository/assessment_ontology_final/MCQ.10.2"/>
    <skos:prefLabel xml:lang="en">MCQ.10.2 - Partially Implemented</skos:prefLabel>
    <skos:altLabel xml:lang="en">(ii)</skos:altLabel>
    <skos:definition xml:lang="en">There are some efforts to review the policy at planned intervals or if significant changes occur. However, these reviews may not be consistently conducted or may lack thoroughness in assessing the policy's relevance and effectiveness.</skos:definition>
  </rdf:Description>
  
  <rdf:Description rdf:about="https://jenni0608.github.io/NIS2V-Vocabulary-Repository/assessment_ontology_final/MCQ115AnswerC">
    <rdf:type rdf:resource="https://jenni0608.github.io/NIS2V-Vocabulary-Repository/assessment_ontology_final/MCQ.115"/>
    <skos:prefLabel xml:lang="en">MCQ.115 - Fully Implemented</skos:prefLabel>
    <skos:altLabel xml:lang="en">(iii)</skos:altLabel>
    <skos:definition xml:lang="en">Senior management systematically conducts reviews of the information security management system at planned intervals, ensuring its continuing suitability, adequacy, and effectiveness. This ensures that the ISMS remains aligned with organisational goals and adapts to changes in the internal and external environment.</skos:definition>
  </rdf:Description>
  
  <rdf:Description rdf:about="https://jenni0608.github.io/NIS2V-Vocabulary-Repository/assessment_ontology_final/MCQ6.2AnswerC">
    <rdf:type rdf:resource="https://jenni0608.github.io/NIS2V-Vocabulary-Repository/assessment_ontology_final/MCQ.6.2"/>
    <skos:prefLabel xml:lang="en">MCQ.6.2 - Fully Implemented</skos:prefLabel>
    <skos:altLabel xml:lang="en">(iii)</skos:altLabel>
    <skos:definition xml:lang="en">There are well-established timings for communication related to the information security management system, ensuring timely dissemination of information.</skos:definition>
  </rdf:Description>
  
  <rdf:Description rdf:about="https://jenni0608.github.io/NIS2V-Vocabulary-Repository/assessment_ontology_final/MCQ22.2AnswerC">
    <rdf:type rdf:resource="https://jenni0608.github.io/NIS2V-Vocabulary-Repository/assessment_ontology_final/MCQ.22.2"/>
    <skos:prefLabel xml:lang="en">MCQ.22.2 - Fully Implemented</skos:prefLabel>
    <skos:altLabel xml:lang="en">(iii)</skos:altLabel>
    <skos:definition xml:lang="en">The information security policy fully includes information security objectives or provides a framework for setting them, ensuring alignment with organisational goals and providing clear direction in achieving security objectives.</skos:definition>
  </rdf:Description>
  
  <rdf:Description rdf:about="https://jenni0608.github.io/NIS2V-Vocabulary-Repository/assessment_ontology_final/MCQ42AnswerB">
    <rdf:type rdf:resource="https://jenni0608.github.io/NIS2V-Vocabulary-Repository/assessment_ontology_final/MCQ.42"/>
    <skos:prefLabel xml:lang="en">MCQ.42 - Partially Implemented</skos:prefLabel>
    <skos:altLabel xml:lang="en">(ii)</skos:altLabel>
    <skos:definition xml:lang="en">There are some efforts to acquire information regarding technical vulnerabilities and evaluate the organisation’s exposure to such vulnerabilities, but the implementation of appropriate measures may be inconsistent or incomplete.</skos:definition>
  </rdf:Description>
  
  <rdf:Description rdf:about="https://jenni0608.github.io/NIS2V-Vocabulary-Repository/assessment_ontology_final/MCQ105.3AnswerA">
    <rdf:type rdf:resource="https://jenni0608.github.io/NIS2V-Vocabulary-Repository/assessment_ontology_final/MCQ.105.3"/>
    <skos:prefLabel xml:lang="en">MCQ.105.3 - Not Implemented</skos:prefLabel>
    <skos:altLabel xml:lang="en">(i)</skos:altLabel>
    <skos:definition xml:lang="en">Senior management does not ensure the availability of resources for the ISMS, resulting in resource shortages that hinder effective security management.</skos:definition>
  </rdf:Description>
  
  <rdf:Description rdf:about="https://jenni0608.github.io/NIS2V-Vocabulary-Repository/assessment_ontology_final/MCQ22.3AnswerB">
    <rdf:type rdf:resource="https://jenni0608.github.io/NIS2V-Vocabulary-Repository/assessment_ontology_final/MCQ.22.3"/>
    <skos:prefLabel xml:lang="en">MCQ.22.3 - Partially Implemented</skos:prefLabel>
    <skos:altLabel xml:lang="en">(ii)</skos:altLabel>
    <skos:definition xml:lang="en">There are some indications in the information security policy of a commitment to satisfy applicable requirements related to information security. However, this commitment may not be clearly articulated or fully comprehensive.</skos:definition>
  </rdf:Description>
  
  <rdf:Description rdf:about="https://jenni0608.github.io/NIS2V-Vocabulary-Repository/assessment_ontology_final/MCQ16AnswerA">
    <rdf:type rdf:resource="https://jenni0608.github.io/NIS2V-Vocabulary-Repository/assessment_ontology_final/MCQ.16"/>
    <skos:prefLabel xml:lang="en">MCQ.16 - Not Implemented</skos:prefLabel>
    <skos:altLabel xml:lang="en">(i)</skos:altLabel>
    <skos:definition xml:lang="en">The organisation does not systematically identify, document, or keep up to date with legal, statutory, regulatory, and contractual requirements relevant to information security, nor does it document its approach to meeting these requirements.</skos:definition>
  </rdf:Description>
  
  <rdf:Description rdf:about="https://jenni0608.github.io/NIS2V-Vocabulary-Repository/assessment_ontology_final/MCQ56.2AnswerB">
    <rdf:type rdf:resource="https://jenni0608.github.io/NIS2V-Vocabulary-Repository/assessment_ontology_final/MCQ.56.2"/>
    <skos:prefLabel xml:lang="en">MCQ.56.2 - Partially Implemented</skos:prefLabel>
    <skos:altLabel xml:lang="en">(ii)</skos:altLabel>
    <skos:definition xml:lang="en">There are some established timings for communication related to the information security management system, but they may not cover all necessary instances.</skos:definition>
  </rdf:Description>
  
  <rdf:Description rdf:about="https://jenni0608.github.io/NIS2V-Vocabulary-Repository/assessment_ontology_final/MCQ65AnswerB">
    <rdf:type rdf:resource="https://jenni0608.github.io/NIS2V-Vocabulary-Repository/assessment_ontology_final/MCQ.65"/>
    <skos:prefLabel xml:lang="en">MCQ.65 - Partially Implemented</skos:prefLabel>
    <skos:altLabel xml:lang="en">(ii)</skos:altLabel>
    <skos:definition xml:lang="en">There are some efforts to implement protection against malware, but user awareness measures may be lacking or insufficient to adequately support the protection against malware.</skos:definition>
  </rdf:Description>
  
  <rdf:Description rdf:about="https://jenni0608.github.io/NIS2V-Vocabulary-Repository/assessment_ontology_final/MCQ43AnswerC">
    <rdf:type rdf:resource="https://jenni0608.github.io/NIS2V-Vocabulary-Repository/assessment_ontology_final/MCQ.43"/>
    <skos:prefLabel xml:lang="en">MCQ.43 - Fully Implemented</skos:prefLabel>
    <skos:altLabel xml:lang="en">(iii)</skos:altLabel>
    <skos:definition xml:lang="en">Configurations, including security configurations, of hardware, software, services, and networks are fully established, documented, implemented, monitored, and reviewed, ensuring consistent and effective security measures across the organisation.</skos:definition>
  </rdf:Description>
  
  <rdf:Description rdf:about="https://jenni0608.github.io/NIS2V-Vocabulary-Repository/assessment_ontology_final/MCQ62AnswerC">
    <rdf:type rdf:resource="https://jenni0608.github.io/NIS2V-Vocabulary-Repository/assessment_ontology_final/MCQ.62"/>
    <skos:prefLabel xml:lang="en">MCQ.62 - Fully Implemented</skos:prefLabel>
    <skos:altLabel xml:lang="en">(iii)</skos:altLabel>
    <skos:definition xml:lang="en">Access rights to information and associated assets are provisioned, reviewed, modified, and removed in strict accordance with the organisation’s topic-specific policy and rules for access control, ensuring effective access management and adherence to security requirements.</skos:definition>
  </rdf:Description>
  
  <rdf:Description rdf:about="https://jenni0608.github.io/NIS2V-Vocabulary-Repository/assessment_ontology_final/MCQ35AnswerA">
    <rdf:type rdf:resource="https://jenni0608.github.io/NIS2V-Vocabulary-Repository/assessment_ontology_final/MCQ.35"/>
    <skos:prefLabel xml:lang="en">MCQ.35 - Not Implemented</skos:prefLabel>
    <skos:altLabel xml:lang="en">(i)</skos:altLabel>
    <skos:definition xml:lang="en">Information security is not integrated into project management, leading to potential security vulnerabilities and risks being overlooked during project implementation.</skos:definition>
  </rdf:Description>
  
  <rdf:Description rdf:about="https://jenni0608.github.io/NIS2V-Vocabulary-Repository/assessment_ontology_final/MCQ82AnswerC">
    <rdf:type rdf:resource="https://jenni0608.github.io/NIS2V-Vocabulary-Repository/assessment_ontology_final/MCQ.82"/>
    <skos:prefLabel xml:lang="en">MCQ.82 - Fully Implemented</skos:prefLabel>
    <skos:altLabel xml:lang="en">(iii)</skos:altLabel>
    <skos:definition xml:lang="en">The allocation and management of authentication information are controlled by a robust management process, which includes advising personnel on the appropriate handling of authentication information, ensuring secure and effective management of authentication credentials.</skos:definition>
  </rdf:Description>
  
  <rdf:Description rdf:about="https://jenni0608.github.io/NIS2V-Vocabulary-Repository/assessment_ontology_final/MCQ6.2AnswerA">
    <rdf:type rdf:resource="https://jenni0608.github.io/NIS2V-Vocabulary-Repository/assessment_ontology_final/MCQ.6.2"/>
    <skos:prefLabel xml:lang="en">MCQ.6.2 - Not Implemented</skos:prefLabel>
    <skos:altLabel xml:lang="en">(i)</skos:altLabel>
    <skos:definition xml:lang="en">There is no established timing for communication related to the information security management system.</skos:definition>
  </rdf:Description>
  
  <rdf:Description rdf:about="https://jenni0608.github.io/NIS2V-Vocabulary-Repository/assessment_ontology_final/MCQ23AnswerC">
    <rdf:type rdf:resource="https://jenni0608.github.io/NIS2V-Vocabulary-Repository/assessment_ontology_final/MCQ.23"/>
    <skos:prefLabel xml:lang="en">MCQ.23 - Fully Implemented</skos:prefLabel>
    <skos:altLabel xml:lang="en">(iii)</skos:altLabel>
    <skos:definition xml:lang="en">The organisation has fully implemented a mechanism for personnel to report observed or suspected information security events through appropriate channels in a timely manner, facilitating prompt detection and response to security incidents.</skos:definition>
  </rdf:Description>
  
  <rdf:Description rdf:about="https://jenni0608.github.io/NIS2V-Vocabulary-Repository/assessment_ontology_final/MCQ27AnswerA">
    <rdf:type rdf:resource="https://jenni0608.github.io/NIS2V-Vocabulary-Repository/assessment_ontology_final/MCQ.27"/>
    <skos:prefLabel xml:lang="en">MCQ.27 - Not Implemented</skos:prefLabel>
    <skos:altLabel xml:lang="en">(i)</skos:altLabel>
    <skos:definition xml:lang="en">The organisation does not have a plan in place to maintain information security at an appropriate level during disruptions, leaving it vulnerable to potential security breaches during such events.</skos:definition>
  </rdf:Description>
  
  <rdf:Description rdf:about="https://jenni0608.github.io/NIS2V-Vocabulary-Repository/assessment_ontology_final/MCQ28AnswerB">
    <rdf:type rdf:resource="https://jenni0608.github.io/NIS2V-Vocabulary-Repository/assessment_ontology_final/MCQ.28"/>
    <skos:prefLabel xml:lang="en">MCQ.28 - Partially Implemented</skos:prefLabel>
    <skos:altLabel xml:lang="en">(ii)</skos:altLabel>
    <skos:definition xml:lang="en">There are some efforts to plan, implement, maintain, and test ICT readiness based on business continuity objectives and ICT continuity requirements, but the process may be inconsistent or not fully comprehensive.</skos:definition>
  </rdf:Description>
  
  <rdf:Description rdf:about="https://jenni0608.github.io/NIS2V-Vocabulary-Repository/assessment_ontology_final/MCQ104AnswerB">
    <rdf:type rdf:resource="https://jenni0608.github.io/NIS2V-Vocabulary-Repository/assessment_ontology_final/MCQ.104"/>
    <skos:prefLabel xml:lang="en">MCQ.104 - Partially Implemented</skos:prefLabel>
    <skos:altLabel xml:lang="en">(ii)</skos:altLabel>
    <skos:definition xml:lang="en">Some processes for establishing, implementing, maintaining, and improving the ISMS are in place, but there are inconsistencies or gaps in their execution, leading to partial compliance withISO27001's requirements.</skos:definition>
  </rdf:Description>
  
  <rdf:Description rdf:about="https://jenni0608.github.io/NIS2V-Vocabulary-Repository/assessment_ontology_final/MCQ11AnswerC">
    <rdf:type rdf:resource="https://jenni0608.github.io/NIS2V-Vocabulary-Repository/assessment_ontology_final/MCQ.11"/>
    <skos:prefLabel xml:lang="en">MCQ.11 - Fully Implemented</skos:prefLabel>
    <skos:altLabel xml:lang="en">(iii)</skos:altLabel>
    <skos:definition xml:lang="en">Clear and comprehensive information security roles and responsibilities are defined and allocated based on the specific needs of the organisation. There are established processes for regularly reviewing and updating these roles to ensure alignment with organisational requirements.</skos:definition>
  </rdf:Description>
  
  <rdf:Description rdf:about="https://jenni0608.github.io/NIS2V-Vocabulary-Repository/assessment_ontology_final/MCQ52AnswerB">
    <rdf:type rdf:resource="https://jenni0608.github.io/NIS2V-Vocabulary-Repository/assessment_ontology_final/MCQ.52"/>
    <skos:prefLabel xml:lang="en">MCQ.52 - Partially Implemented</skos:prefLabel>
    <skos:altLabel xml:lang="en">(ii)</skos:altLabel>
    <skos:definition xml:lang="en">There are some efforts to review the organisation’s approach to managing information security and its implementation independently at planned intervals or when significant changes occur, but the reviews may be inconsistent or not fully comprehensive.</skos:definition>
  </rdf:Description>
  
  <rdf:Description rdf:about="https://jenni0608.github.io/NIS2V-Vocabulary-Repository/assessment_ontology_final/MCQ84AnswerB">
    <rdf:type rdf:resource="https://jenni0608.github.io/NIS2V-Vocabulary-Repository/assessment_ontology_final/MCQ.84"/>
    <skos:prefLabel xml:lang="en">MCQ.84 - Partially Implemented</skos:prefLabel>
    <skos:altLabel xml:lang="en">(ii)</skos:altLabel>
    <skos:definition xml:lang="en">There are some efforts to restrict and manage the allocation and use of privileged access rights, but the process may be inconsistent or not fully comprehensive.</skos:definition>
  </rdf:Description>
  
  <rdf:Description rdf:about="https://jenni0608.github.io/NIS2V-Vocabulary-Repository/assessment_ontology_final/MCQ105.1AnswerC">
    <rdf:type rdf:resource="https://jenni0608.github.io/NIS2V-Vocabulary-Repository/assessment_ontology_final/MCQ.105.1"/>
    <skos:prefLabel xml:lang="en">MCQ.105.1 - Fully Implemented</skos:prefLabel>
    <skos:altLabel xml:lang="en">(iii)</skos:altLabel>
    <skos:definition xml:lang="en">Senior management ensures that the information security policy and objectives are fully compatible with the strategic direction of the organisation, ensuring alignment of goals and objectives.</skos:definition>
  </rdf:Description>
  
  <rdf:Description rdf:about="https://jenni0608.github.io/NIS2V-Vocabulary-Repository/assessment_ontology_final/MCQ22.4AnswerB">
    <rdf:type rdf:resource="https://jenni0608.github.io/NIS2V-Vocabulary-Repository/assessment_ontology_final/MCQ.22.4"/>
    <skos:prefLabel xml:lang="en">MCQ.22.4 - Partially Implemented</skos:prefLabel>
    <skos:altLabel xml:lang="en">(ii)</skos:altLabel>
    <skos:definition xml:lang="en">There are some indications in the information security policy of a commitment to continual improvement of the information security management system. However, this commitment may not be clearly emphasized or fully integrated into organisational processes.</skos:definition>
  </rdf:Description>
  
  <rdf:Description rdf:about="https://jenni0608.github.io/NIS2V-Vocabulary-Repository/assessment_ontology_final/MCQ105.1AnswerB">
    <rdf:type rdf:resource="https://jenni0608.github.io/NIS2V-Vocabulary-Repository/assessment_ontology_final/MCQ.105.1"/>
    <skos:prefLabel xml:lang="en">MCQ.105.1 - Partially Implemented</skos:prefLabel>
    <skos:altLabel xml:lang="en">(ii)</skos:altLabel>
    <skos:definition xml:lang="en">Some efforts are made to align the information security policy and objectives with the organisation's strategic direction, but there are inconsistencies or gaps in the process.</skos:definition>
  </rdf:Description>
  
  <rdf:Description rdf:about="https://jenni0608.github.io/NIS2V-Vocabulary-Repository/assessment_ontology_final/MCQ49.2AnswerC">
    <rdf:type rdf:resource="https://jenni0608.github.io/NIS2V-Vocabulary-Repository/assessment_ontology_final/MCQ.49.2"/>
    <skos:prefLabel xml:lang="en">MCQ.49.2 - Fully Implemented</skos:prefLabel>
    <skos:altLabel xml:lang="en">(iii)</skos:altLabel>
    <skos:definition xml:lang="en">The organisation has fully established methods for monitoring, measurement, analysis, and evaluation to ensure valid results. These methods are carefully selected and capable of producing comparable and reproducible results, ensuring the reliability of data for decision-making purposes.</skos:definition>
  </rdf:Description>
  
  <rdf:Description rdf:about="https://jenni0608.github.io/NIS2V-Vocabulary-Repository/assessment_ontology_final/MCQ31AnswerA">
    <rdf:type rdf:resource="https://jenni0608.github.io/NIS2V-Vocabulary-Repository/assessment_ontology_final/MCQ.31"/>
    <skos:prefLabel xml:lang="en">MCQ.31 - Not Implemented</skos:prefLabel>
    <skos:altLabel xml:lang="en">(i)</skos:altLabel>
    <skos:definition xml:lang="en">The use of resources is not monitored, and there are no adjustments made in line with current and expected capacity requirements, leading to potential inefficiencies and resource shortages.</skos:definition>
  </rdf:Description>
  
  <rdf:Description rdf:about="https://jenni0608.github.io/NIS2V-Vocabulary-Repository/assessment_ontology_final/MCQ27AnswerC">
    <rdf:type rdf:resource="https://jenni0608.github.io/NIS2V-Vocabulary-Repository/assessment_ontology_final/MCQ.27"/>
    <skos:prefLabel xml:lang="en">MCQ.27 - Fully Implemented</skos:prefLabel>
    <skos:altLabel xml:lang="en">(iii)</skos:altLabel>
    <skos:definition xml:lang="en">The organisation has a comprehensive plan in place to maintain information security at an appropriate level during disruptions, ensuring resilience and continuity of operations even in challenging circumstances.</skos:definition>
  </rdf:Description>
  
  <rdf:Description rdf:about="https://jenni0608.github.io/NIS2V-Vocabulary-Repository/assessment_ontology_final/MCQ56.1AnswerC">
    <rdf:type rdf:resource="https://jenni0608.github.io/NIS2V-Vocabulary-Repository/assessment_ontology_final/MCQ.56.1"/>
    <skos:prefLabel xml:lang="en">MCQ.56.1 - Fully Implemented</skos:prefLabel>
    <skos:altLabel xml:lang="en">(iii)</skos:altLabel>
    <skos:definition xml:lang="en">The organisation has a clear understanding of what information to communicate regarding the information security management system, covering all relevant aspects.</skos:definition>
  </rdf:Description>
  
  <rdf:Description rdf:about="https://jenni0608.github.io/NIS2V-Vocabulary-Repository/assessment_ontology_final/MCQ34.4AnswerA">
    <rdf:type rdf:resource="https://jenni0608.github.io/NIS2V-Vocabulary-Repository/assessment_ontology_final/MCQ.34.4"/>
    <skos:prefLabel xml:lang="en">MCQ.34.4 - Not Implemented</skos:prefLabel>
    <skos:altLabel xml:lang="en">(i)</skos:altLabel>
    <skos:definition xml:lang="en">The organisation does not ensure that externally provided processes, products, or services relevant to the information security management system are controlled, potentially exposing the organisation to security risks associated with external dependencies.</skos:definition>
  </rdf:Description>
  
  <rdf:Description rdf:about="https://jenni0608.github.io/NIS2V-Vocabulary-Repository/assessment_ontology_final/MCQ46AnswerA">
    <rdf:type rdf:resource="https://jenni0608.github.io/NIS2V-Vocabulary-Repository/assessment_ontology_final/MCQ.46"/>
    <skos:prefLabel xml:lang="en">MCQ.46 - Not Implemented</skos:prefLabel>
    <skos:altLabel xml:lang="en">(i)</skos:altLabel>
    <skos:definition xml:lang="en">Security mechanisms, service levels, and service requirements of network services are not identified, implemented, or monitored, posing risks of security vulnerabilities and service disruptions.</skos:definition>
  </rdf:Description>
  
  <rdf:Description rdf:about="https://jenni0608.github.io/NIS2V-Vocabulary-Repository/assessment_ontology_final/MCQ103AnswerC">
    <rdf:type rdf:resource="https://jenni0608.github.io/NIS2V-Vocabulary-Repository/assessment_ontology_final/MCQ.103"/>
    <skos:prefLabel xml:lang="en">MCQ.103 - Fully Implemented</skos:prefLabel>
    <skos:altLabel xml:lang="en">(iii)</skos:altLabel>
    <skos:definition xml:lang="en">The organisation systematically considers external and internal issues, requirements, and interfaces with other organisations when determining the scope of its ISMS, ensuring clarity and alignment with organisational objectives. The scope is documented and readily available for reference.</skos:definition>
  </rdf:Description>
  
  <rdf:Description rdf:about="https://jenni0608.github.io/NIS2V-Vocabulary-Repository/assessment_ontology_final/MCQ95AnswerB">
    <rdf:type rdf:resource="https://jenni0608.github.io/NIS2V-Vocabulary-Repository/assessment_ontology_final/MCQ.95"/>
    <skos:prefLabel xml:lang="en">MCQ.95 - Partially Implemented</skos:prefLabel>
    <skos:altLabel xml:lang="en">(ii)</skos:altLabel>
    <skos:definition xml:lang="en">There are some efforts to implement protection against malware, but user awareness measures may be lacking or insufficient to adequately support the protection against malware.</skos:definition>
  </rdf:Description>
  
  <rdf:Description rdf:about="https://jenni0608.github.io/NIS2V-Vocabulary-Repository/assessment_ontology_final/MCQ69AnswerA">
    <rdf:type rdf:resource="https://jenni0608.github.io/NIS2V-Vocabulary-Repository/assessment_ontology_final/MCQ.69"/>
    <skos:prefLabel xml:lang="en">MCQ.69 - Not Implemented</skos:prefLabel>
    <skos:altLabel xml:lang="en">(i)</skos:altLabel>
    <skos:definition xml:lang="en">Rules for the effective use of cryptography, including cryptographic key management, are not defined or implemented, leading to potential vulnerabilities and inadequate protection of sensitive information.</skos:definition>
  </rdf:Description>
  
  <rdf:Description rdf:about="https://jenni0608.github.io/NIS2V-Vocabulary-Repository/assessment_ontology_final/MCQ112.1AnswerA">
    <rdf:type rdf:resource="https://jenni0608.github.io/NIS2V-Vocabulary-Repository/assessment_ontology_final/MCQ.112.1"/>
    <skos:prefLabel xml:lang="en">MCQ.112.1 - Not Implemented</skos:prefLabel>
    <skos:altLabel xml:lang="en">(i)</skos:altLabel>
    <skos:definition xml:lang="en">The organisation lacks determination of what needs to be monitored and measured, methods for monitoring and measurement, and specification of when or by whom monitoring and measurement should be performed, resulting in insufficient evidence of information security performance and ISMS effectiveness.</skos:definition>
  </rdf:Description>
  
  <rdf:Description rdf:about="https://jenni0608.github.io/NIS2V-Vocabulary-Repository/assessment_ontology_final/MCQ51AnswerC">
    <rdf:type rdf:resource="https://jenni0608.github.io/NIS2V-Vocabulary-Repository/assessment_ontology_final/MCQ.51"/>
    <skos:prefLabel xml:lang="en">MCQ.51 - Fully Implemented</skos:prefLabel>
    <skos:altLabel xml:lang="en">(iii)</skos:altLabel>
    <skos:definition xml:lang="en">Top management conducts regular reviews of the organisation's information security management system at planned intervals, ensuring its continuing suitability, adequacy, and effectiveness in addressing security risks and meeting organisational objectives.</skos:definition>
  </rdf:Description>
  
  <rdf:Description rdf:about="https://jenni0608.github.io/NIS2V-Vocabulary-Repository/assessment_ontology_final/MCQ55.1AnswerB">
    <rdf:type rdf:resource="https://jenni0608.github.io/NIS2V-Vocabulary-Repository/assessment_ontology_final/MCQ.55.1"/>
    <skos:prefLabel xml:lang="en">MCQ.55.1 - Partially Implemented</skos:prefLabel>
    <skos:altLabel xml:lang="en">(ii)</skos:altLabel>
    <skos:definition xml:lang="en">There are some efforts to make persons doing work under the organisation’s control aware of the information security policy. However, awareness may be inconsistent or not fully comprehensive.</skos:definition>
  </rdf:Description>
  
  <rdf:Description rdf:about="https://jenni0608.github.io/NIS2V-Vocabulary-Repository/assessment_ontology_final/MCQ24AnswerA">
    <rdf:type rdf:resource="https://jenni0608.github.io/NIS2V-Vocabulary-Repository/assessment_ontology_final/MCQ.24"/>
    <skos:prefLabel xml:lang="en">MCQ.24 - Not Implemented</skos:prefLabel>
    <skos:altLabel xml:lang="en">(i)</skos:altLabel>
    <skos:definition xml:lang="en">Logs that record activities, exceptions, faults, and other relevant events are not produced, stored, protected, or analysed, resulting in a lack of visibility into the organisation's security posture and potential risks.</skos:definition>
  </rdf:Description>
  
  <rdf:Description rdf:about="https://jenni0608.github.io/NIS2V-Vocabulary-Repository/assessment_ontology_final/MCQ119AnswerB">
    <rdf:type rdf:resource="https://jenni0608.github.io/NIS2V-Vocabulary-Repository/assessment_ontology_final/MCQ.119"/>
    <skos:prefLabel xml:lang="en">MCQ.119 - Partially Implemented</skos:prefLabel>
    <skos:altLabel xml:lang="en">(ii)</skos:altLabel>
    <skos:definition xml:lang="en">Some efforts are made by management to require personnel to follow information security policies and procedures, but there are inconsistencies or gaps in enforcement.</skos:definition>
  </rdf:Description>
  
  <rdf:Description rdf:about="https://jenni0608.github.io/NIS2V-Vocabulary-Repository/assessment_ontology_final/MCQ84AnswerC">
    <rdf:type rdf:resource="https://jenni0608.github.io/NIS2V-Vocabulary-Repository/assessment_ontology_final/MCQ.84"/>
    <skos:prefLabel xml:lang="en">MCQ.84 - Fully Implemented</skos:prefLabel>
    <skos:altLabel xml:lang="en">(iii)</skos:altLabel>
    <skos:definition xml:lang="en">The allocation and use of privileged access rights are fully restricted and managed, ensuring that only authorized personnel have access to sensitive systems and data, and activities are monitored and audited for compliance.</skos:definition>
  </rdf:Description>
  
  <rdf:Description rdf:about="https://jenni0608.github.io/NIS2V-Vocabulary-Repository/assessment_ontology_final/MCQ4.5AnswerA">
    <rdf:type rdf:resource="https://jenni0608.github.io/NIS2V-Vocabulary-Repository/assessment_ontology_final/MCQ.4.5"/>
    <skos:prefLabel xml:lang="en">MCQ.4.5 - Not Implemented</skos:prefLabel>
    <skos:altLabel xml:lang="en">(i)</skos:altLabel>
    <skos:definition xml:lang="en">The organisation does not effectively evaluate information security risks, failing to compare the results of risk analysis with established risk criteria and prioritize the analysed risks for risk treatment.</skos:definition>
  </rdf:Description>
  
  <rdf:Description rdf:about="https://jenni0608.github.io/NIS2V-Vocabulary-Repository/assessment_ontology_final/MCQ95AnswerC">
    <rdf:type rdf:resource="https://jenni0608.github.io/NIS2V-Vocabulary-Repository/assessment_ontology_final/MCQ.95"/>
    <skos:prefLabel xml:lang="en">MCQ.95 - Fully Implemented</skos:prefLabel>
    <skos:altLabel xml:lang="en">(iii)</skos:altLabel>
    <skos:definition xml:lang="en">Protection against malware is fully implemented and supported by appropriate user awareness measures, ensuring proactive defense against malware threats and enhancing overall security posture.</skos:definition>
  </rdf:Description>
  
  <rdf:Description rdf:about="https://jenni0608.github.io/NIS2V-Vocabulary-Repository/assessment_ontology_final/MCQ35AnswerB">
    <rdf:type rdf:resource="https://jenni0608.github.io/NIS2V-Vocabulary-Repository/assessment_ontology_final/MCQ.35"/>
    <skos:prefLabel xml:lang="en">MCQ.35 - Partially Implemented</skos:prefLabel>
    <skos:altLabel xml:lang="en">(ii)</skos:altLabel>
    <skos:definition xml:lang="en">There are some efforts to integrate information security into project management, but the integration may be inconsistent or not fully comprehensive across all projects.</skos:definition>
  </rdf:Description>
  
  <rdf:Description rdf:about="https://jenni0608.github.io/NIS2V-Vocabulary-Repository/assessment_ontology_final/MCQ5.1AnswerC">
    <rdf:type rdf:resource="https://jenni0608.github.io/NIS2V-Vocabulary-Repository/assessment_ontology_final/MCQ.5.1"/>
    <skos:prefLabel xml:lang="en">MCQ.5.1 - Fully Implemented</skos:prefLabel>
    <skos:altLabel xml:lang="en">(iii)</skos:altLabel>
    <skos:definition xml:lang="en">The organisation fully selects appropriate information security risk treatment options, considering the results of the risk assessment, ensuring effective risk mitigation strategies are implemented to address identified security risks.</skos:definition>
  </rdf:Description>
  
  <rdf:Description rdf:about="https://jenni0608.github.io/NIS2V-Vocabulary-Repository/assessment_ontology_final/MCQ103AnswerA">
    <rdf:type rdf:resource="https://jenni0608.github.io/NIS2V-Vocabulary-Repository/assessment_ontology_final/MCQ.103"/>
    <skos:prefLabel xml:lang="en">MCQ.103 - Not Implemented</skos:prefLabel>
    <skos:altLabel xml:lang="en">(i)</skos:altLabel>
    <skos:definition xml:lang="en">The organisation does not consider external and internal issues, requirements, or interfaces with other organisations when determining the scope of its ISMS, resulting in an unclear or inadequate scope.</skos:definition>
  </rdf:Description>
  
  <rdf:Description rdf:about="https://jenni0608.github.io/NIS2V-Vocabulary-Repository/assessment_ontology_final/MCQ83AnswerB">
    <rdf:type rdf:resource="https://jenni0608.github.io/NIS2V-Vocabulary-Repository/assessment_ontology_final/MCQ.83"/>
    <skos:prefLabel xml:lang="en">MCQ.83 - Partially Implemented</skos:prefLabel>
    <skos:altLabel xml:lang="en">(ii)</skos:altLabel>
    <skos:definition xml:lang="en">There are some efforts to provision, review, modify, and remove access rights in accordance with the organisation’s topic-specific policy and rules for access control, but the process may be inconsistent or incomplete.</skos:definition>
  </rdf:Description>
  
  <rdf:Description rdf:about="https://jenni0608.github.io/NIS2V-Vocabulary-Repository/assessment_ontology_final/MCQ3.1AnswerA">
    <rdf:type rdf:resource="https://jenni0608.github.io/NIS2V-Vocabulary-Repository/assessment_ontology_final/MCQ.3.1"/>
    <skos:prefLabel xml:lang="en">MCQ.3.1 - Not Implemented</skos:prefLabel>
    <skos:altLabel xml:lang="en">(i)</skos:altLabel>
    <skos:definition xml:lang="en">Top management does not ensure that responsibilities and authorities for roles relevant to information security are assigned and communicated within the organisation, potentially leading to confusion and lack of clarity regarding information security responsibilities.</skos:definition>
  </rdf:Description>
  
  <rdf:Description rdf:about="https://jenni0608.github.io/NIS2V-Vocabulary-Repository/assessment_ontology_final/MCQ75.1AnswerA">
    <rdf:type rdf:resource="https://jenni0608.github.io/NIS2V-Vocabulary-Repository/assessment_ontology_final/MCQ.75.1"/>
    <skos:prefLabel xml:lang="en">MCQ.75.1 - Not Implemented</skos:prefLabel>
    <skos:altLabel xml:lang="en">(i)</skos:altLabel>
    <skos:definition xml:lang="en">Personnel of the organisation and relevant interested parties do not receive appropriate information security awareness, education, and training, leaving them ill-prepared to address security risks and threats.</skos:definition>
  </rdf:Description>
  
  <rdf:Description rdf:about="https://jenni0608.github.io/NIS2V-Vocabulary-Repository/assessment_ontology_final/MCQ116AnswerB">
    <rdf:type rdf:resource="https://jenni0608.github.io/NIS2V-Vocabulary-Repository/assessment_ontology_final/MCQ.116"/>
    <skos:prefLabel xml:lang="en">MCQ.116 - Partially Implemented</skos:prefLabel>
    <skos:altLabel xml:lang="en">(ii)</skos:altLabel>
    <skos:definition xml:lang="en">Some decisions related to continual improvement opportunities and changes to the ISMS are included in the results of the management review, but there are inconsistencies or gaps in the process.</skos:definition>
  </rdf:Description>
  
  <rdf:Description rdf:about="https://jenni0608.github.io/NIS2V-Vocabulary-Repository/assessment_ontology_final/MCQ5.4AnswerA">
    <rdf:type rdf:resource="https://jenni0608.github.io/NIS2V-Vocabulary-Repository/assessment_ontology_final/MCQ.5.4"/>
    <skos:prefLabel xml:lang="en">MCQ.5.4 - Not Implemented</skos:prefLabel>
    <skos:altLabel xml:lang="en">(i)</skos:altLabel>
    <skos:definition xml:lang="en">The organisation does not formulate an information security risk treatment plan, potentially leaving security risks unaddressed and increasing the likelihood of security incidents.</skos:definition>
  </rdf:Description>
  
  <rdf:Description rdf:about="https://jenni0608.github.io/NIS2V-Vocabulary-Repository/assessment_ontology_final/MCQ5.4AnswerB">
    <rdf:type rdf:resource="https://jenni0608.github.io/NIS2V-Vocabulary-Repository/assessment_ontology_final/MCQ.5.4"/>
    <skos:prefLabel xml:lang="en">MCQ.5.4 - Partially Implemented</skos:prefLabel>
    <skos:altLabel xml:lang="en">(ii)</skos:altLabel>
    <skos:definition xml:lang="en">There are some efforts by the organisation to formulate an information security risk treatment plan. However, the plan may be incomplete or inconsistently applied across different areas of the organisation.</skos:definition>
  </rdf:Description>
  
  <rdf:Description rdf:about="https://jenni0608.github.io/NIS2V-Vocabulary-Repository/assessment_ontology_final/MCQ76AnswerB">
    <rdf:type rdf:resource="https://jenni0608.github.io/NIS2V-Vocabulary-Repository/assessment_ontology_final/MCQ.76"/>
    <skos:prefLabel xml:lang="en">MCQ.76 - Partially Implemented</skos:prefLabel>
    <skos:altLabel xml:lang="en">(ii)</skos:altLabel>
    <skos:definition xml:lang="en">There are some elements of a disciplinary process to take actions against personnel and other relevant interested parties who have committed an information security policy violation, but the process may not be fully formalized or effectively communicated.</skos:definition>
  </rdf:Description>
  
  <rdf:Description rdf:about="https://jenni0608.github.io/NIS2V-Vocabulary-Repository/assessment_ontology_final/MCQ25AnswerC">
    <rdf:type rdf:resource="https://jenni0608.github.io/NIS2V-Vocabulary-Repository/assessment_ontology_final/MCQ.25"/>
    <skos:prefLabel xml:lang="en">MCQ.25 - Fully Implemented</skos:prefLabel>
    <skos:altLabel xml:lang="en">(iii)</skos:altLabel>
    <skos:definition xml:lang="en">Networks, systems, and applications are continuously monitored for anomalous behaviour, and appropriate actions are promptly taken to evaluate potential information security incidents, ensuring timely detection and response to security threats.</skos:definition>
  </rdf:Description>
  
  <rdf:Description rdf:about="https://jenni0608.github.io/NIS2V-Vocabulary-Repository/assessment_ontology_final/MCQ48AnswerA">
    <rdf:type rdf:resource="https://jenni0608.github.io/NIS2V-Vocabulary-Repository/assessment_ontology_final/MCQ.48"/>
    <skos:prefLabel xml:lang="en">MCQ.48 - Not Implemented</skos:prefLabel>
    <skos:altLabel xml:lang="en">(i)</skos:altLabel>
    <skos:definition xml:lang="en">Access to external websites is not managed, exposing the organisation to potential risks from malicious content and cyber threats.</skos:definition>
  </rdf:Description>
  
  <rdf:Description rdf:about="https://jenni0608.github.io/NIS2V-Vocabulary-Repository/assessment_ontology_final/MCQ125AnswerC">
    <rdf:type rdf:resource="https://jenni0608.github.io/NIS2V-Vocabulary-Repository/assessment_ontology_final/MCQ.125"/>
    <skos:prefLabel xml:lang="en">MCQ.125 - Fully Implemented</skos:prefLabel>
    <skos:altLabel xml:lang="en">(iii)</skos:altLabel>
    <skos:definition xml:lang="en">The organisation has established and consistently applies rules for the secure development of software and systems, ensuring that security measures are integrated throughout the development lifecycle to mitigate potential vulnerabilities.</skos:definition>
  </rdf:Description>
  
  <rdf:Description rdf:about="https://jenni0608.github.io/NIS2V-Vocabulary-Repository/assessment_ontology_final/MCQ94AnswerB">
    <rdf:type rdf:resource="https://jenni0608.github.io/NIS2V-Vocabulary-Repository/assessment_ontology_final/MCQ.94"/>
    <skos:prefLabel xml:lang="en">MCQ.94 - Partially Implemented</skos:prefLabel>
    <skos:altLabel xml:lang="en">(ii)</skos:altLabel>
    <skos:definition xml:lang="en">There are some measures in place to protect information stored on, processed by, or accessible via user endpoint devices, but the protection may be inconsistent or not fully comprehensive.</skos:definition>
  </rdf:Description>
  
  <rdf:Description rdf:about="https://jenni0608.github.io/NIS2V-Vocabulary-Repository/assessment_ontology_final/MCQ2.1AnswerA">
    <rdf:type rdf:resource="https://jenni0608.github.io/NIS2V-Vocabulary-Repository/assessment_ontology_final/MCQ.2.1"/>
    <skos:prefLabel xml:lang="en">MCQ.2.1 - Not Implemented</skos:prefLabel>
    <skos:altLabel xml:lang="en">(i)</skos:altLabel>
    <skos:definition xml:lang="en">Top management does not establish an information security policy appropriate to the organisation's purpose, nor does it include information security objectives or commit to satisfying applicable requirements related to information security or continually improving the information security management system.</skos:definition>
  </rdf:Description>
  
  <rdf:Description rdf:about="https://jenni0608.github.io/NIS2V-Vocabulary-Repository/assessment_ontology_final/MCQ88AnswerC">
    <rdf:type rdf:resource="https://jenni0608.github.io/NIS2V-Vocabulary-Repository/assessment_ontology_final/MCQ.88"/>
    <skos:prefLabel xml:lang="en">MCQ.88 - Fully Implemented</skos:prefLabel>
    <skos:altLabel xml:lang="en">(iii)</skos:altLabel>
    <skos:definition xml:lang="en">The use of utility programs capable of overriding system and application controls is fully restricted and tightly controlled, ensuring that such programs are only used by authorized personnel for legitimate purposes and activities are closely monitored to prevent misuse.</skos:definition>
  </rdf:Description>
  
  <rdf:Description rdf:about="https://jenni0608.github.io/NIS2V-Vocabulary-Repository/assessment_ontology_final/MCQ97AnswerB">
    <rdf:type rdf:resource="https://jenni0608.github.io/NIS2V-Vocabulary-Repository/assessment_ontology_final/MCQ.97"/>
    <skos:prefLabel xml:lang="en">MCQ.97 - Partially Implemented</skos:prefLabel>
    <skos:altLabel xml:lang="en">(ii)</skos:altLabel>
    <skos:definition xml:lang="en">There are some efforts to manage the complete life cycle of identities, including activities such as creation, authentication, and periodic reviews, but the process may be inconsistent or lacking in certain areas.</skos:definition>
  </rdf:Description>
  
  <rdf:Description rdf:about="https://jenni0608.github.io/NIS2V-Vocabulary-Repository/assessment_ontology_final/MCQ2.2AnswerB">
    <rdf:type rdf:resource="https://jenni0608.github.io/NIS2V-Vocabulary-Repository/assessment_ontology_final/MCQ.2.2"/>
    <skos:prefLabel xml:lang="en">MCQ.2.2 - Partially Implemented</skos:prefLabel>
    <skos:altLabel xml:lang="en">(ii)</skos:altLabel>
    <skos:definition xml:lang="en">There are some efforts to make the information security policy available as documented information, and it may be communicated within the organisation to some extent. However, it may not be consistently available to interested parties, leading to potential gaps in awareness.</skos:definition>
  </rdf:Description>
  
  <rdf:Description rdf:about="https://jenni0608.github.io/NIS2V-Vocabulary-Repository/assessment_ontology_final/MCQ4.5AnswerB">
    <rdf:type rdf:resource="https://jenni0608.github.io/NIS2V-Vocabulary-Repository/assessment_ontology_final/MCQ.4.5"/>
    <skos:prefLabel xml:lang="en">MCQ.4.5 - Partially Implemented</skos:prefLabel>
    <skos:altLabel xml:lang="en">(ii)</skos:altLabel>
    <skos:definition xml:lang="en">There are some efforts by the organisation to evaluate information security risks, including comparing the results of risk analysis with established risk criteria. However, the process may be inconsistent or not fully comprehensive in prioritizing the analysed risks for risk treatment.</skos:definition>
  </rdf:Description>
  
  <rdf:Description rdf:about="https://jenni0608.github.io/NIS2V-Vocabulary-Repository/assessment_ontology_final/MCQ32AnswerB">
    <rdf:type rdf:resource="https://jenni0608.github.io/NIS2V-Vocabulary-Repository/assessment_ontology_final/MCQ.32"/>
    <skos:prefLabel xml:lang="en">MCQ.32 - Partially Implemented</skos:prefLabel>
    <skos:altLabel xml:lang="en">(ii)</skos:altLabel>
    <skos:definition xml:lang="en">There are some efforts to maintain backup copies of information, software, and systems, and occasional testing is conducted, but the process may be inconsistent or not fully aligned with the agreed topic-specific policy on backup.</skos:definition>
  </rdf:Description>
  
  <rdf:Description rdf:about="https://jenni0608.github.io/NIS2V-Vocabulary-Repository/assessment_ontology_final/MCQ98AnswerB">
    <rdf:type rdf:resource="https://jenni0608.github.io/NIS2V-Vocabulary-Repository/assessment_ontology_final/MCQ.98"/>
    <skos:prefLabel xml:lang="en">MCQ.98 - Partially Implemented</skos:prefLabel>
    <skos:altLabel xml:lang="en">(ii)</skos:altLabel>
    <skos:definition xml:lang="en">There are some efforts to control the allocation and management of authentication information through a management process, but personnel may not consistently receive advice on the appropriate handling of authentication information.</skos:definition>
  </rdf:Description>
  
  <rdf:Description rdf:about="https://jenni0608.github.io/NIS2V-Vocabulary-Repository/assessment_ontology_final/MCQ49.1AnswerB">
    <rdf:type rdf:resource="https://jenni0608.github.io/NIS2V-Vocabulary-Repository/assessment_ontology_final/MCQ.49.1"/>
    <skos:prefLabel xml:lang="en">MCQ.49.1 - Partially Implemented</skos:prefLabel>
    <skos:altLabel xml:lang="en">(ii)</skos:altLabel>
    <skos:definition xml:lang="en">There are some efforts by the organisation to determine what needs to be monitored and measured, including information security processes and controls. However, this determination may be inconsistent or not fully comprehensive.</skos:definition>
  </rdf:Description>
  
  <rdf:Description rdf:about="https://jenni0608.github.io/NIS2V-Vocabulary-Repository/assessment_ontology_final/MCQ9.1AnswerB">
    <rdf:type rdf:resource="https://jenni0608.github.io/NIS2V-Vocabulary-Repository/assessment_ontology_final/MCQ.9.1"/>
    <skos:prefLabel xml:lang="en">MCQ.9.1 - Partially Implemented</skos:prefLabel>
    <skos:altLabel xml:lang="en">(ii)</skos:altLabel>
    <skos:definition xml:lang="en">There are some efforts by the organisation to implement the information security risk treatment plan. However, the implementation may be inconsistent or not fully comprehensive.</skos:definition>
  </rdf:Description>
  
  <rdf:Description rdf:about="https://jenni0608.github.io/NIS2V-Vocabulary-Repository/assessment_ontology_final/MCQ62AnswerB">
    <rdf:type rdf:resource="https://jenni0608.github.io/NIS2V-Vocabulary-Repository/assessment_ontology_final/MCQ.62"/>
    <skos:prefLabel xml:lang="en">MCQ.62 - Partially Implemented</skos:prefLabel>
    <skos:altLabel xml:lang="en">(ii)</skos:altLabel>
    <skos:definition xml:lang="en">There are some efforts to provision, review, modify, and remove access rights in accordance with the organisation’s topic-specific policy and rules for access control, but the process may be inconsistent or incomplete.</skos:definition>
  </rdf:Description>
  
  <rdf:Description rdf:about="https://jenni0608.github.io/NIS2V-Vocabulary-Repository/assessment_ontology_final/MCQ45AnswerA">
    <rdf:type rdf:resource="https://jenni0608.github.io/NIS2V-Vocabulary-Repository/assessment_ontology_final/MCQ.45"/>
    <skos:prefLabel xml:lang="en">MCQ.45 - Not Implemented</skos:prefLabel>
    <skos:altLabel xml:lang="en">(i)</skos:altLabel>
    <skos:definition xml:lang="en">Networks and network devices are not secured, managed, or controlled to protect information in systems and applications, leaving the organisation vulnerable to potential security breaches and data compromise.</skos:definition>
  </rdf:Description>
  
  <rdf:Description rdf:about="https://jenni0608.github.io/NIS2V-Vocabulary-Repository/assessment_ontology_final/MCQ34.1AnswerA">
    <rdf:type rdf:resource="https://jenni0608.github.io/NIS2V-Vocabulary-Repository/assessment_ontology_final/MCQ.34.1"/>
    <skos:prefLabel xml:lang="en">MCQ.34.1 - Not Implemented</skos:prefLabel>
    <skos:altLabel xml:lang="en">(i)</skos:altLabel>
    <skos:definition xml:lang="en">The organisation is not planning, implementing, and controlling processes to meet requirements and implement actions by establishing criteria for the processes and implementing control. This lack of control may result in inconsistent or ineffective processes.</skos:definition>
  </rdf:Description>
  
  <rdf:Description rdf:about="https://jenni0608.github.io/NIS2V-Vocabulary-Repository/assessment_ontology_final/MCQ45AnswerC">
    <rdf:type rdf:resource="https://jenni0608.github.io/NIS2V-Vocabulary-Repository/assessment_ontology_final/MCQ.45"/>
    <skos:prefLabel xml:lang="en">MCQ.45 - Fully Implemented</skos:prefLabel>
    <skos:altLabel xml:lang="en">(iii)</skos:altLabel>
    <skos:definition xml:lang="en">Networks and network devices are fully secured, managed, and controlled to protect information in systems and applications, ensuring robust defence against security threats and data breaches.</skos:definition>
  </rdf:Description>
  
  <rdf:Description rdf:about="https://jenni0608.github.io/NIS2V-Vocabulary-Repository/assessment_ontology_final/MCQ22.3AnswerC">
    <rdf:type rdf:resource="https://jenni0608.github.io/NIS2V-Vocabulary-Repository/assessment_ontology_final/MCQ.22.3"/>
    <skos:prefLabel xml:lang="en">MCQ.22.3 - Fully Implemented</skos:prefLabel>
    <skos:altLabel xml:lang="en">(iii)</skos:altLabel>
    <skos:definition xml:lang="en">The information security policy fully includes a commitment to satisfy applicable requirements related to information security, demonstrating the organisation's commitment to compliance and proactive management of security risks.</skos:definition>
  </rdf:Description>
  
  <rdf:Description rdf:about="https://jenni0608.github.io/NIS2V-Vocabulary-Repository/assessment_ontology_final/MCQ19AnswerB">
    <rdf:type rdf:resource="https://jenni0608.github.io/NIS2V-Vocabulary-Repository/assessment_ontology_final/MCQ.19"/>
    <skos:prefLabel xml:lang="en">MCQ.19 - Partially Implemented</skos:prefLabel>
    <skos:altLabel xml:lang="en">(ii)</skos:altLabel>
    <skos:definition xml:lang="en">There are some efforts to assess information security events to determine if they should be categorized as incidents, but the process may be inconsistent or not fully comprehensive.</skos:definition>
  </rdf:Description>
  
  <rdf:Description rdf:about="https://jenni0608.github.io/NIS2V-Vocabulary-Repository/assessment_ontology_final/MCQ127AnswerC">
    <rdf:type rdf:resource="https://jenni0608.github.io/NIS2V-Vocabulary-Repository/assessment_ontology_final/MCQ.127"/>
    <skos:prefLabel xml:lang="en">MCQ.127 - Fully Implemented</skos:prefLabel>
    <skos:altLabel xml:lang="en">(iii)</skos:altLabel>
    <skos:definition xml:lang="en">The organisation has established, documented, maintained, and consistently applies principles for engineering secure systems to all information system development activities. This ensures that security is integrated throughout the development process, mitigating potential vulnerabilities effectively.</skos:definition>
  </rdf:Description>
  
  <rdf:Description rdf:about="https://jenni0608.github.io/NIS2V-Vocabulary-Repository/assessment_ontology_final/MCQ34.4AnswerB">
    <rdf:type rdf:resource="https://jenni0608.github.io/NIS2V-Vocabulary-Repository/assessment_ontology_final/MCQ.34.4"/>
    <skos:prefLabel xml:lang="en">MCQ.34.4 - Partially Implemented</skos:prefLabel>
    <skos:altLabel xml:lang="en">(ii)</skos:altLabel>
    <skos:definition xml:lang="en">There are some efforts by the organisation to ensure that externally provided processes, products, or services relevant to the information security management system are controlled. However, these efforts may be inconsistent or not fully comprehensive.</skos:definition>
  </rdf:Description>
  
  <rdf:Description rdf:about="https://jenni0608.github.io/NIS2V-Vocabulary-Repository/assessment_ontology_final/MCQ4.4AnswerA">
    <rdf:type rdf:resource="https://jenni0608.github.io/NIS2V-Vocabulary-Repository/assessment_ontology_final/MCQ.4.4"/>
    <skos:prefLabel xml:lang="en">MCQ.4.4 - Not Implemented</skos:prefLabel>
    <skos:altLabel xml:lang="en">(i)</skos:altLabel>
    <skos:definition xml:lang="en">The organisation does not effectively analyse information security risks, failing to assess the potential consequences and realistic likelihood of the occurrence of identified risks, nor does it determine the levels of risk.</skos:definition>
  </rdf:Description>
  
  <rdf:Description rdf:about="https://jenni0608.github.io/NIS2V-Vocabulary-Repository/assessment_ontology_final/MCQ78AnswerA">
    <rdf:type rdf:resource="https://jenni0608.github.io/NIS2V-Vocabulary-Repository/assessment_ontology_final/MCQ.78"/>
    <skos:prefLabel xml:lang="en">MCQ.78 - Not Implemented</skos:prefLabel>
    <skos:altLabel xml:lang="en">(i)</skos:altLabel>
    <skos:definition xml:lang="en">Confidentiality or non-disclosure agreements reflecting the organisation's needs for the protection of information are not identified, documented, regularly reviewed, or signed by personnel and other relevant interested parties, potentially exposing sensitive information to unauthorized disclosure.</skos:definition>
  </rdf:Description>
  
  <rdf:Description rdf:about="https://jenni0608.github.io/NIS2V-Vocabulary-Repository/assessment_ontology_final/MCQ32AnswerC">
    <rdf:type rdf:resource="https://jenni0608.github.io/NIS2V-Vocabulary-Repository/assessment_ontology_final/MCQ.32"/>
    <skos:prefLabel xml:lang="en">MCQ.32 - Fully Implemented</skos:prefLabel>
    <skos:altLabel xml:lang="en">(iii)</skos:altLabel>
    <skos:definition xml:lang="en">Backup copies of information, software, and systems are consistently maintained and regularly tested in accordance with the agreed topic-specific policy on backup, ensuring data integrity and system availability in the event of disruptions.</skos:definition>
  </rdf:Description>
  
  <rdf:Description rdf:about="https://jenni0608.github.io/NIS2V-Vocabulary-Repository/assessment_ontology_final/MCQ109AnswerC">
    <rdf:type rdf:resource="https://jenni0608.github.io/NIS2V-Vocabulary-Repository/assessment_ontology_final/MCQ.109"/>
    <skos:prefLabel xml:lang="en">MCQ.109 - Fully Implemented</skos:prefLabel>
    <skos:altLabel xml:lang="en">(iii)</skos:altLabel>
    <skos:definition xml:lang="en">The organisation systematically controls documented information to ensure its availability, suitability for use, and adequate protection from loss of confidentiality, improper use, or loss of integrity. This includes addressing activities such as distribution, access, retrieval, use, storage, preservation, change control, retention, and disposition, as applicable. Additionally, documented information of external origin necessary for the ISMS is identified and controlled appropriately.</skos:definition>
  </rdf:Description>
  
  <rdf:Description rdf:about="https://jenni0608.github.io/NIS2V-Vocabulary-Repository/assessment_ontology_final/MCQ101AnswerA">
    <rdf:type rdf:resource="https://jenni0608.github.io/NIS2V-Vocabulary-Repository/assessment_ontology_final/MCQ.101"/>
    <skos:prefLabel xml:lang="en">MCQ.101 - Not Implemented</skos:prefLabel>
    <skos:altLabel xml:lang="en">(i)</skos:altLabel>
    <skos:definition xml:lang="en">The organisation does not systematically identify or address external and internal issues relevant to its information security management system, leaving it vulnerable to unforeseen challenges.</skos:definition>
  </rdf:Description>
  
  <rdf:Description rdf:about="https://jenni0608.github.io/NIS2V-Vocabulary-Repository/assessment_ontology_final/MCQ8.1AnswerA">
    <rdf:type rdf:resource="https://jenni0608.github.io/NIS2V-Vocabulary-Repository/assessment_ontology_final/MCQ.8.1"/>
    <skos:prefLabel xml:lang="en">MCQ.8.1 - Not Implemented</skos:prefLabel>
    <skos:altLabel xml:lang="en">(i)</skos:altLabel>
    <skos:definition xml:lang="en">The organisation does not conduct information security risk assessments at planned intervals or when significant changes are proposed or occur, potentially leaving the organisation vulnerable to evolving threats and vulnerabilities.</skos:definition>
  </rdf:Description>
  
  <rdf:Description rdf:about="https://jenni0608.github.io/NIS2V-Vocabulary-Repository/assessment_ontology_final/MCQ6.3AnswerB">
    <rdf:type rdf:resource="https://jenni0608.github.io/NIS2V-Vocabulary-Repository/assessment_ontology_final/MCQ.6.3"/>
    <skos:prefLabel xml:lang="en">MCQ.6.3 - Partially Implemented</skos:prefLabel>
    <skos:altLabel xml:lang="en">(ii)</skos:altLabel>
    <skos:definition xml:lang="en">The organisation identifies some stakeholders with whom to communicate regarding the information security management system, but there may be gaps in coverage.</skos:definition>
  </rdf:Description>
  
  <rdf:Description rdf:about="https://jenni0608.github.io/NIS2V-Vocabulary-Repository/assessment_ontology_final/MCQ118AnswerB">
    <rdf:type rdf:resource="https://jenni0608.github.io/NIS2V-Vocabulary-Repository/assessment_ontology_final/MCQ.118"/>
    <skos:prefLabel xml:lang="en">MCQ.118 - Partially Implemented</skos:prefLabel>
    <skos:altLabel xml:lang="en">(ii)</skos:altLabel>
    <skos:definition xml:lang="en">Some actions are taken to address nonconformities, but there are inconsistencies or gaps in evaluating causes, implementing corrective actions, and reviewing effectiveness.</skos:definition>
  </rdf:Description>
  
  <rdf:Description rdf:about="https://jenni0608.github.io/NIS2V-Vocabulary-Repository/assessment_ontology_final/MCQ87AnswerB">
    <rdf:type rdf:resource="https://jenni0608.github.io/NIS2V-Vocabulary-Repository/assessment_ontology_final/MCQ.87"/>
    <skos:prefLabel xml:lang="en">MCQ.87 - Partially Implemented</skos:prefLabel>
    <skos:altLabel xml:lang="en">(ii)</skos:altLabel>
    <skos:definition xml:lang="en">There are some efforts to implement secure authentication technologies and procedures based on information access restrictions and the topic-specific policy on access control, but the implementation may be incomplete or inconsistent.</skos:definition>
  </rdf:Description>
  
  <rdf:Description rdf:about="https://jenni0608.github.io/NIS2V-Vocabulary-Repository/assessment_ontology_final/MCQ99AnswerC">
    <rdf:type rdf:resource="https://jenni0608.github.io/NIS2V-Vocabulary-Repository/assessment_ontology_final/MCQ.99"/>
    <skos:prefLabel xml:lang="en">MCQ.99 - Fully Implemented</skos:prefLabel>
    <skos:altLabel xml:lang="en">(iii)</skos:altLabel>
    <skos:definition xml:lang="en">Secure authentication technologies and procedures are fully implemented based on information access restrictions and the topic-specific policy on access control, ensuring robust access security and compliance with organisational policies.</skos:definition>
  </rdf:Description>
  
  <rdf:Description rdf:about="https://jenni0608.github.io/NIS2V-Vocabulary-Repository/assessment_ontology_final/MCQ128AnswerA">
    <rdf:type rdf:resource="https://jenni0608.github.io/NIS2V-Vocabulary-Repository/assessment_ontology_final/MCQ.128"/>
    <skos:prefLabel xml:lang="en">MCQ.128 - Not Implemented</skos:prefLabel>
    <skos:altLabel xml:lang="en">(i)</skos:altLabel>
    <skos:definition xml:lang="en">The organisation lacks a structured approach to direct, monitor, and review outsourced system development activities, leading to potential risks and inefficiencies.</skos:definition>
  </rdf:Description>
  
  <rdf:Description rdf:about="https://jenni0608.github.io/NIS2V-Vocabulary-Repository/assessment_ontology_final/MCQ22.5AnswerC">
    <rdf:type rdf:resource="https://jenni0608.github.io/NIS2V-Vocabulary-Repository/assessment_ontology_final/MCQ.22.5"/>
    <skos:prefLabel xml:lang="en">MCQ.22.5 - Fully Implemented</skos:prefLabel>
    <skos:altLabel xml:lang="en">(iii)</skos:altLabel>
    <skos:definition xml:lang="en">The information security policy is fully available as documented information, communicated within the organisation, and made available to interested parties as appropriate, ensuring widespread awareness and understanding of security requirements.</skos:definition>
  </rdf:Description>
  
  <rdf:Description rdf:about="https://jenni0608.github.io/NIS2V-Vocabulary-Repository/assessment_ontology_final/MCQ79AnswerC">
    <rdf:type rdf:resource="https://jenni0608.github.io/NIS2V-Vocabulary-Repository/assessment_ontology_final/MCQ.79"/>
    <skos:prefLabel xml:lang="en">MCQ.79 - Fully Implemented</skos:prefLabel>
    <skos:altLabel xml:lang="en">(iii)</skos:altLabel>
    <skos:definition xml:lang="en">Security measures are fully implemented when personnel are working remotely, ensuring that information accessed, processed, or stored outside the organisation’s premises is adequately protected from unauthorized access or breaches.</skos:definition>
  </rdf:Description>
  
  <rdf:Description rdf:about="https://jenni0608.github.io/NIS2V-Vocabulary-Repository/assessment_ontology_final/MCQ102AnswerC">
    <rdf:type rdf:resource="https://jenni0608.github.io/NIS2V-Vocabulary-Repository/assessment_ontology_final/MCQ.102"/>
    <skos:prefLabel xml:lang="en">MCQ.102 - Fully Implemented</skos:prefLabel>
    <skos:altLabel xml:lang="en">(iii)</skos:altLabel>
    <skos:definition xml:lang="en">The organisation systematically identifies interested parties, their requirements, and determines which requirements will be addressed through the information security management system, ensuring alignment with stakeholder needs and expectations.</skos:definition>
  </rdf:Description>
  
  <rdf:Description rdf:about="https://jenni0608.github.io/NIS2V-Vocabulary-Repository/assessment_ontology_final/MCQ37AnswerC">
    <rdf:type rdf:resource="https://jenni0608.github.io/NIS2V-Vocabulary-Repository/assessment_ontology_final/MCQ.37"/>
    <skos:prefLabel xml:lang="en">MCQ.37 - Fully Implemented</skos:prefLabel>
    <skos:altLabel xml:lang="en">(iii)</skos:altLabel>
    <skos:definition xml:lang="en">Relevant information security requirements are fully established and agreed upon with each supplier based on the type of supplier relationship, ensuring alignment with organisational security objectives and mitigation of risks associated with third-party engagements.</skos:definition>
  </rdf:Description>
  
  <rdf:Description rdf:about="https://jenni0608.github.io/NIS2V-Vocabulary-Repository/assessment_ontology_final/MCQ40AnswerA">
    <rdf:type rdf:resource="https://jenni0608.github.io/NIS2V-Vocabulary-Repository/assessment_ontology_final/MCQ.40"/>
    <skos:prefLabel xml:lang="en">MCQ.40 - Not Implemented</skos:prefLabel>
    <skos:altLabel xml:lang="en">(i)</skos:altLabel>
    <skos:definition xml:lang="en">Processes for the acquisition, use, management, and exit from cloud services are not established in accordance with the organisation’s information security requirements, posing risks of security breaches and data loss.</skos:definition>
  </rdf:Description>
  
  <rdf:Description rdf:about="https://jenni0608.github.io/NIS2V-Vocabulary-Repository/assessment_ontology_final/MCQ19AnswerA">
    <rdf:type rdf:resource="https://jenni0608.github.io/NIS2V-Vocabulary-Repository/assessment_ontology_final/MCQ.19"/>
    <skos:prefLabel xml:lang="en">MCQ.19 - Not Implemented</skos:prefLabel>
    <skos:altLabel xml:lang="en">(i)</skos:altLabel>
    <skos:definition xml:lang="en">The organisation lacks a structured process for assessing information security events to determine if they should be categorized as incidents, leading to inconsistencies and potential oversight.</skos:definition>
  </rdf:Description>
  
  <rdf:Description rdf:about="https://jenni0608.github.io/NIS2V-Vocabulary-Repository/assessment_ontology_final/MCQ105.2AnswerB">
    <rdf:type rdf:resource="https://jenni0608.github.io/NIS2V-Vocabulary-Repository/assessment_ontology_final/MCQ.105.2"/>
    <skos:prefLabel xml:lang="en">MCQ.105.2 - Partially Implemented</skos:prefLabel>
    <skos:altLabel xml:lang="en">(ii)</skos:altLabel>
    <skos:definition xml:lang="en">Some efforts are made to integrate ISMS requirements into organisational processes, but there are inconsistencies or gaps in the integration process.</skos:definition>
  </rdf:Description>
  
  <rdf:Description rdf:about="https://jenni0608.github.io/NIS2V-Vocabulary-Repository/assessment_ontology_final/MCQ3.1AnswerB">
    <rdf:type rdf:resource="https://jenni0608.github.io/NIS2V-Vocabulary-Repository/assessment_ontology_final/MCQ.3.1"/>
    <skos:prefLabel xml:lang="en">MCQ.3.1 - Partially Implemented</skos:prefLabel>
    <skos:altLabel xml:lang="en">(ii)</skos:altLabel>
    <skos:definition xml:lang="en">There are some efforts by top management to ensure that responsibilities and authorities for roles relevant to information security are assigned within the organisation. However, communication of these responsibilities may be inconsistent or not fully comprehensive.</skos:definition>
  </rdf:Description>
  
  <rdf:Description rdf:about="https://jenni0608.github.io/NIS2V-Vocabulary-Repository/assessment_ontology_final/MCQ6.1AnswerB">
    <rdf:type rdf:resource="https://jenni0608.github.io/NIS2V-Vocabulary-Repository/assessment_ontology_final/MCQ.6.1"/>
    <skos:prefLabel xml:lang="en">MCQ.6.1 - Partially Implemented</skos:prefLabel>
    <skos:altLabel xml:lang="en">(ii)</skos:altLabel>
    <skos:definition xml:lang="en">The organisation has some understanding of what information to communicate regarding the information security management system, but it may not be comprehensive.</skos:definition>
  </rdf:Description>
  
  <rdf:Description rdf:about="https://jenni0608.github.io/NIS2V-Vocabulary-Repository/assessment_ontology_final/MCQ16AnswerC">
    <rdf:type rdf:resource="https://jenni0608.github.io/NIS2V-Vocabulary-Repository/assessment_ontology_final/MCQ.16"/>
    <skos:prefLabel xml:lang="en">MCQ.16 - Fully Implemented</skos:prefLabel>
    <skos:altLabel xml:lang="en">(iii)</skos:altLabel>
    <skos:definition xml:lang="en">The organisation systematically identifies, documents, and keeps up to date with legal, statutory, regulatory, and contractual requirements relevant to information security, and its approach to meeting these requirements is well-documented and regularly updated, ensuring compliance with applicable laws and regulations.</skos:definition>
  </rdf:Description>
  
  <rdf:Description rdf:about="https://jenni0608.github.io/NIS2V-Vocabulary-Repository/assessment_ontology_final/MCQ86AnswerB">
    <rdf:type rdf:resource="https://jenni0608.github.io/NIS2V-Vocabulary-Repository/assessment_ontology_final/MCQ.86"/>
    <skos:prefLabel xml:lang="en">MCQ.86 - Partially Implemented</skos:prefLabel>
    <skos:altLabel xml:lang="en">(ii)</skos:altLabel>
    <skos:definition xml:lang="en">There are some efforts to manage read and write access to source code, development tools, and software libraries, but the management may be inconsistent or not fully comprehensive.</skos:definition>
  </rdf:Description>
  
  <rdf:Description rdf:about="https://jenni0608.github.io/NIS2V-Vocabulary-Repository/assessment_ontology_final/MCQ49.3AnswerC">
    <rdf:type rdf:resource="https://jenni0608.github.io/NIS2V-Vocabulary-Repository/assessment_ontology_final/MCQ.49.3"/>
    <skos:prefLabel xml:lang="en">MCQ.49.3 - Fully Implemented</skos:prefLabel>
    <skos:altLabel xml:lang="en">(iii)</skos:altLabel>
    <skos:definition xml:lang="en">The organisation fully specifies when monitoring and measuring shall be performed, who shall conduct it, and when the results shall be analysed and evaluated, ensuring consistency and timeliness in performance assessment.</skos:definition>
  </rdf:Description>
  
  <rdf:Description rdf:about="https://jenni0608.github.io/NIS2V-Vocabulary-Repository/assessment_ontology_final/MCQ89AnswerB">
    <rdf:type rdf:resource="https://jenni0608.github.io/NIS2V-Vocabulary-Repository/assessment_ontology_final/MCQ.89"/>
    <skos:prefLabel xml:lang="en">MCQ.89 - Partially Implemented</skos:prefLabel>
    <skos:altLabel xml:lang="en">(ii)</skos:altLabel>
    <skos:definition xml:lang="en">There are some efforts to develop and maintain an inventory of information and other associated assets, including owners, but the inventory may be incomplete or not regularly updated.</skos:definition>
  </rdf:Description>
  
  <rdf:Description rdf:about="https://jenni0608.github.io/NIS2V-Vocabulary-Repository/assessment_ontology_final/MCQ79AnswerA">
    <rdf:type rdf:resource="https://jenni0608.github.io/NIS2V-Vocabulary-Repository/assessment_ontology_final/MCQ.79"/>
    <skos:prefLabel xml:lang="en">MCQ.79 - Not Implemented</skos:prefLabel>
    <skos:altLabel xml:lang="en">(i)</skos:altLabel>
    <skos:definition xml:lang="en">Security measures are not implemented when personnel are working remotely, leaving information accessed, processed, or stored outside the organisation’s premises vulnerable to unauthorized access or breaches.</skos:definition>
  </rdf:Description>
  
  <rdf:Description rdf:about="https://jenni0608.github.io/NIS2V-Vocabulary-Repository/assessment_ontology_final/MCQ57.1AnswerB">
    <rdf:type rdf:resource="https://jenni0608.github.io/NIS2V-Vocabulary-Repository/assessment_ontology_final/MCQ.57.1"/>
    <skos:prefLabel xml:lang="en">MCQ.57.1 - Partially Implemented</skos:prefLabel>
    <skos:altLabel xml:lang="en">(ii)</skos:altLabel>
    <skos:definition xml:lang="en">There are some efforts to provide personnel of the organisation and relevant interested parties with information security awareness, education, and training, but it may be inconsistent or not fully comprehensive.</skos:definition>
  </rdf:Description>
  
  <rdf:Description rdf:about="https://jenni0608.github.io/NIS2V-Vocabulary-Repository/assessment_ontology_final/MCQ22.3AnswerA">
    <rdf:type rdf:resource="https://jenni0608.github.io/NIS2V-Vocabulary-Repository/assessment_ontology_final/MCQ.22.3"/>
    <skos:prefLabel xml:lang="en">MCQ.22.3 - Not Implemented</skos:prefLabel>
    <skos:altLabel xml:lang="en">(i)</skos:altLabel>
    <skos:definition xml:lang="en">The information security policy does not include a commitment to satisfy applicable requirements related to information security, potentially leading to non-compliance and increased security risks.</skos:definition>
  </rdf:Description>
  
  <rdf:Description rdf:about="https://jenni0608.github.io/NIS2V-Vocabulary-Repository/assessment_ontology_final/MCQ112.1AnswerC">
    <rdf:type rdf:resource="https://jenni0608.github.io/NIS2V-Vocabulary-Repository/assessment_ontology_final/MCQ.112.1"/>
    <skos:prefLabel xml:lang="en">MCQ.112.1 - Fully Implemented</skos:prefLabel>
    <skos:altLabel xml:lang="en">(iii)</skos:altLabel>
    <skos:definition xml:lang="en">The organisation systematically determines what needs to be monitored and measured, establishes methods for monitoring and measurement that produce comparable and reproducible results, specifies when and by whom monitoring and measurement should be performed, and analyzes and evaluates the results.</skos:definition>
  </rdf:Description>
  
  <rdf:Description rdf:about="https://jenni0608.github.io/NIS2V-Vocabulary-Repository/assessment_ontology_final/MCQ9.2AnswerA">
    <rdf:type rdf:resource="https://jenni0608.github.io/NIS2V-Vocabulary-Repository/assessment_ontology_final/MCQ.9.2"/>
    <skos:prefLabel xml:lang="en">MCQ.9.2 - Not Implemented</skos:prefLabel>
    <skos:altLabel xml:lang="en">(i)</skos:altLabel>
    <skos:definition xml:lang="en">The organisation does not retain documented information of the results of the information security risk treatment, potentially leading to a lack of accountability and traceability in the risk management process.</skos:definition>
  </rdf:Description>
  
  <rdf:Description rdf:about="https://jenni0608.github.io/NIS2V-Vocabulary-Repository/assessment_ontology_final/MCQ66AnswerA">
    <rdf:type rdf:resource="https://jenni0608.github.io/NIS2V-Vocabulary-Repository/assessment_ontology_final/MCQ.66"/>
    <skos:prefLabel xml:lang="en">MCQ.66 - Not Implemented</skos:prefLabel>
    <skos:altLabel xml:lang="en">(i)</skos:altLabel>
    <skos:definition xml:lang="en">Procedures and measures to securely manage software installation on operational systems have not been implemented, potentially leading to unauthorized or insecure software installations.</skos:definition>
  </rdf:Description>
  
  <rdf:Description rdf:about="https://jenni0608.github.io/NIS2V-Vocabulary-Repository/assessment_ontology_final/MCQ20AnswerB">
    <rdf:type rdf:resource="https://jenni0608.github.io/NIS2V-Vocabulary-Repository/assessment_ontology_final/MCQ.20"/>
    <skos:prefLabel xml:lang="en">MCQ.20 - Partially Implemented</skos:prefLabel>
    <skos:altLabel xml:lang="en">(ii)</skos:altLabel>
    <skos:definition xml:lang="en">There are some efforts to respond to information security incidents in accordance with documented procedures, but adherence to these procedures may be inconsistent or incomplete.</skos:definition>
  </rdf:Description>
  
  <rdf:Description rdf:about="https://jenni0608.github.io/NIS2V-Vocabulary-Repository/assessment_ontology_final/MCQ90AnswerB">
    <rdf:type rdf:resource="https://jenni0608.github.io/NIS2V-Vocabulary-Repository/assessment_ontology_final/MCQ.90"/>
    <skos:prefLabel xml:lang="en">MCQ.90 - Partially Implemented</skos:prefLabel>
    <skos:altLabel xml:lang="en">(ii)</skos:altLabel>
    <skos:definition xml:lang="en">There are some efforts to identify, document, and implement rules for acceptable use and procedures for handling information and other associated assets, but the documentation may be incomplete or not consistently followed.</skos:definition>
  </rdf:Description>
  
  <rdf:Description rdf:about="https://jenni0608.github.io/NIS2V-Vocabulary-Repository/assessment_ontology_final/MCQ.10.1AnswerC">
    <rdf:type rdf:resource="https://jenni0608.github.io/NIS2V-Vocabulary-Repository/assessment_ontology_final/MCQ.10.1"/>
    <skos:prefLabel xml:lang="en">MCQ.10.1 - Fully Implemented</skos:prefLabel>
    <skos:altLabel xml:lang="en">(iii)</skos:altLabel>
    <skos:definition xml:lang="en">The information security policy and topic-specific policies are fully defined, approved by management, published, communicated to, and acknowledged by relevant personnel and interested parties, ensuring widespread awareness and compliance with security requirements.</skos:definition>
  </rdf:Description>
  
  <rdf:Description rdf:about="https://jenni0608.github.io/NIS2V-Vocabulary-Repository/assessment_ontology_final/MCQ18AnswerC">
    <rdf:type rdf:resource="https://jenni0608.github.io/NIS2V-Vocabulary-Repository/assessment_ontology_final/MCQ.18"/>
    <skos:prefLabel xml:lang="en">MCQ.18 - Fully Implemented</skos:prefLabel>
    <skos:altLabel xml:lang="en">(iii)</skos:altLabel>
    <skos:definition xml:lang="en">The organisation has clearly defined, established, and communicated information security incident management processes, roles, and responsibilities, ensuring preparedness for managing incidents effectively.</skos:definition>
  </rdf:Description>
  
  <rdf:Description rdf:about="https://jenni0608.github.io/NIS2V-Vocabulary-Repository/assessment_ontology_final/MCQ80AnswerA">
    <rdf:type rdf:resource="https://jenni0608.github.io/NIS2V-Vocabulary-Repository/assessment_ontology_final/MCQ.80"/>
    <skos:prefLabel xml:lang="en">MCQ.80 - Not Implemented</skos:prefLabel>
    <skos:altLabel xml:lang="en">(i)</skos:altLabel>
    <skos:definition xml:lang="en">Rules to control physical and logical access to information and associated assets are not established or implemented, lacking alignment with business and information security requirements.</skos:definition>
  </rdf:Description>
  
  <rdf:Description rdf:about="https://jenni0608.github.io/NIS2V-Vocabulary-Repository/assessment_ontology_final/MCQ55.3AnswerC">
    <rdf:type rdf:resource="https://jenni0608.github.io/NIS2V-Vocabulary-Repository/assessment_ontology_final/MCQ.55.3"/>
    <skos:prefLabel xml:lang="en">MCQ.55.3 - Fully Implemented</skos:prefLabel>
    <skos:altLabel xml:lang="en">(iii)</skos:altLabel>
    <skos:definition xml:lang="en">Persons doing work under the organisation’s control are fully aware of the implications of not conforming with the information security management system requirements, fostering a culture of compliance and accountability in information security practices.</skos:definition>
  </rdf:Description>
  
  <rdf:Description rdf:about="https://jenni0608.github.io/NIS2V-Vocabulary-Repository/assessment_ontology_final/MCQ9.2AnswerB">
    <rdf:type rdf:resource="https://jenni0608.github.io/NIS2V-Vocabulary-Repository/assessment_ontology_final/MCQ.9.2"/>
    <skos:prefLabel xml:lang="en">MCQ.9.2 - Partially Implemented</skos:prefLabel>
    <skos:altLabel xml:lang="en">(ii)</skos:altLabel>
    <skos:definition xml:lang="en">There are some efforts by the organisation to retain documented information of the results of the information security risk treatment. However, the documentation may be incomplete or not consistently maintained.</skos:definition>
  </rdf:Description>
  
  <rdf:Description rdf:about="https://jenni0608.github.io/NIS2V-Vocabulary-Repository/assessment_ontology_final/MCQ8.2AnswerB">
    <rdf:type rdf:resource="https://jenni0608.github.io/NIS2V-Vocabulary-Repository/assessment_ontology_final/MCQ.8.2"/>
    <skos:prefLabel xml:lang="en">MCQ.8.2 - Partially Implemented</skos:prefLabel>
    <skos:altLabel xml:lang="en">(ii)</skos:altLabel>
    <skos:definition xml:lang="en">There are some efforts by the organisation to retain documented information of the results of these assessments. However, the documentation may be incomplete or not consistently maintained.</skos:definition>
  </rdf:Description>
  
  <rdf:Description rdf:about="https://jenni0608.github.io/NIS2V-Vocabulary-Repository/assessment_ontology_final/MCQ61AnswerA">
    <rdf:type rdf:resource="https://jenni0608.github.io/NIS2V-Vocabulary-Repository/assessment_ontology_final/MCQ.61"/>
    <skos:prefLabel xml:lang="en">MCQ.61 - Not Implemented</skos:prefLabel>
    <skos:altLabel xml:lang="en">(i)</skos:altLabel>
    <skos:definition xml:lang="en">There is no management process in place to control the allocation and management of authentication information, and personnel are not advised on the appropriate handling of authentication information.</skos:definition>
  </rdf:Description>
  
  <rdf:Description rdf:about="https://jenni0608.github.io/NIS2V-Vocabulary-Repository/assessment_ontology_final/MCQ56.3AnswerC">
    <rdf:type rdf:resource="https://jenni0608.github.io/NIS2V-Vocabulary-Repository/assessment_ontology_final/MCQ.56.3"/>
    <skos:prefLabel xml:lang="en">MCQ.56.3 - Fully Implemented</skos:prefLabel>
    <skos:altLabel xml:lang="en">(iii)</skos:altLabel>
    <skos:definition xml:lang="en">The organisation has identified all relevant stakeholders with whom to communicate regarding the information security management system, ensuring comprehensive coverage.</skos:definition>
  </rdf:Description>
  
  <rdf:Description rdf:about="https://jenni0608.github.io/NIS2V-Vocabulary-Repository/assessment_ontology_final/MCQ129AnswerC">
    <rdf:type rdf:resource="https://jenni0608.github.io/NIS2V-Vocabulary-Repository/assessment_ontology_final/MCQ.129"/>
    <skos:prefLabel xml:lang="en">MCQ.129 - Fully Implemented</skos:prefLabel>
    <skos:altLabel xml:lang="en">(iii)</skos:altLabel>
    <skos:definition xml:lang="en">The organisation has clearly defined security perimeters and consistently utilizes them to protect areas containing information and associated assets. This ensures that unauthorized access is prevented, and information security is maintained effectively.</skos:definition>
  </rdf:Description>
  
  <rdf:Description rdf:about="https://jenni0608.github.io/NIS2V-Vocabulary-Repository/assessment_ontology_final/MCQ130AnswerA">
    <rdf:type rdf:resource="https://jenni0608.github.io/NIS2V-Vocabulary-Repository/assessment_ontology_final/MCQ.130"/>
    <skos:prefLabel xml:lang="en">MCQ.130 - Not Implemented</skos:prefLabel>
    <skos:altLabel xml:lang="en">(i)</skos:altLabel>
    <skos:definition xml:lang="en">The organisation lacks proper maintenance procedures for equipment, leading to potential disruptions in availability and compromises to the integrity and confidentiality of information.</skos:definition>
  </rdf:Description>
  
  <rdf:Description rdf:about="https://jenni0608.github.io/NIS2V-Vocabulary-Repository/assessment_ontology_final/MCQ71AnswerA">
    <rdf:type rdf:resource="https://jenni0608.github.io/NIS2V-Vocabulary-Repository/assessment_ontology_final/MCQ.71"/>
    <skos:prefLabel xml:lang="en">MCQ.71 - Not Implemented</skos:prefLabel>
    <skos:altLabel xml:lang="en">(i)</skos:altLabel>
    <skos:definition xml:lang="en">Roles and responsibilities related to information security are not clearly defined, and there is no formal allocation process based on organisational needs.</skos:definition>
  </rdf:Description>
  
  <rdf:Description rdf:about="https://jenni0608.github.io/NIS2V-Vocabulary-Repository/assessment_ontology_final/MCQ118AnswerA">
    <rdf:type rdf:resource="https://jenni0608.github.io/NIS2V-Vocabulary-Repository/assessment_ontology_final/MCQ.118"/>
    <skos:prefLabel xml:lang="en">MCQ.118 - Not Implemented</skos:prefLabel>
    <skos:altLabel xml:lang="en">(i)</skos:altLabel>
    <skos:definition xml:lang="en">The organisation does not have a systematic approach to address nonconformities, leading to ineffective control and correction, and potential recurrence.</skos:definition>
  </rdf:Description>
  
  
  <rdf:Description rdf:about="https://jenni0608.github.io/NIS2V-Vocabulary-Repository/assessment_ontology_final/MCQ.10.1AnswerA">
    <rdf:type rdf:resource="https://jenni0608.github.io/NIS2V-Vocabulary-Repository/assessment_ontology_final/MCQ.10.1"/>
    <skos:prefLabel xml:lang="en">MCQ.10.1 - Not Implemented</skos:prefLabel>
    <skos:altLabel xml:lang="en">(i)</skos:altLabel>
    <skos:definition xml:lang="en">The information security policy and topic-specific policies are not defined, approved by management, published, communicated to, or acknowledged by relevant personnel and interested parties, potentially leading to confusion and lack of awareness regarding security requirements.</skos:definition>
  </rdf:Description>
  
  <rdf:Description rdf:about="https://jenni0608.github.io/NIS2V-Vocabulary-Repository/assessment_ontology_final/MCQ76AnswerC">
    <rdf:type rdf:resource="https://jenni0608.github.io/NIS2V-Vocabulary-Repository/assessment_ontology_final/MCQ.76"/>
    <skos:prefLabel xml:lang="en">MCQ.76 - Fully Implemented</skos:prefLabel>
    <skos:altLabel xml:lang="en">(iii)</skos:altLabel>
    <skos:definition xml:lang="en">A disciplinary process to take actions against personnel and other relevant interested parties who have committed an information security policy violation is fully formalized and communicated, ensuring consistent enforcement of security policies and accountability for non-compliance.</skos:definition>
  </rdf:Description>
  
  <rdf:Description rdf:about="https://jenni0608.github.io/NIS2V-Vocabulary-Repository/assessment_ontology_final/MCQ38AnswerA">
    <rdf:type rdf:resource="https://jenni0608.github.io/NIS2V-Vocabulary-Repository/assessment_ontology_final/MCQ.38"/>
    <skos:prefLabel xml:lang="en">MCQ.38 - Not Implemented</skos:prefLabel>
    <skos:altLabel xml:lang="en">(i)</skos:altLabel>
    <skos:definition xml:lang="en">Processes and procedures to manage information security risks associated with the ICT products and services supply chain are not defined or implemented, leaving the organisation vulnerable to potential security breaches from suppliers.</skos:definition>
  </rdf:Description>
  
  <rdf:Description rdf:about="https://jenni0608.github.io/NIS2V-Vocabulary-Repository/assessment_ontology_final/MCQ96AnswerC">
    <rdf:type rdf:resource="https://jenni0608.github.io/NIS2V-Vocabulary-Repository/assessment_ontology_final/MCQ.96"/>
    <skos:prefLabel xml:lang="en">MCQ.96 - Fully Implemented</skos:prefLabel>
    <skos:altLabel xml:lang="en">(iii)</skos:altLabel>
    <skos:definition xml:lang="en">Information transfer rules, procedures, or agreements are fully established for all types of transfer facilities within the organisation and between the organisation and other parties, ensuring secure and compliant transfer of information.</skos:definition>
  </rdf:Description>
  
  <rdf:Description rdf:about="https://jenni0608.github.io/NIS2V-Vocabulary-Repository/assessment_ontology_final/MCQ129AnswerB">
    <rdf:type rdf:resource="https://jenni0608.github.io/NIS2V-Vocabulary-Repository/assessment_ontology_final/MCQ.129"/>
    <skos:prefLabel xml:lang="en">MCQ.129 - Partially Implemented</skos:prefLabel>
    <skos:altLabel xml:lang="en">(ii)</skos:altLabel>
    <skos:definition xml:lang="en">Some effort is made to define security perimeters, but there are inconsistencies or gaps in their utilization, potentially leaving areas containing information and assets vulnerable.</skos:definition>
  </rdf:Description>
  
  <rdf:Description rdf:about="https://jenni0608.github.io/NIS2V-Vocabulary-Repository/assessment_ontology_final/MCQ118AnswerC">
    <rdf:type rdf:resource="https://jenni0608.github.io/NIS2V-Vocabulary-Repository/assessment_ontology_final/MCQ.118"/>
    <skos:prefLabel xml:lang="en">MCQ.118 - Fully Implemented</skos:prefLabel>
    <skos:altLabel xml:lang="en">(iii)</skos:altLabel>
    <skos:definition xml:lang="en">The organisation reacts to nonconformities by taking action to control and correct them, addressing consequences, evaluating causes to prevent recurrence, implementing necessary actions, reviewing the effectiveness of corrective actions, and making changes to the ISMS if necessary. Corrective actions are appropriate to the effects of the nonconformities encountered. Documented information is available as evidence of the nature of nonconformities, subsequent actions taken, and the results of corrective actions.</skos:definition>
  </rdf:Description>
  
  <rdf:Description rdf:about="https://jenni0608.github.io/NIS2V-Vocabulary-Repository/assessment_ontology_final/MCQ102AnswerA">
    <rdf:type rdf:resource="https://jenni0608.github.io/NIS2V-Vocabulary-Repository/assessment_ontology_final/MCQ.102"/>
    <skos:prefLabel xml:lang="en">MCQ.102 - Not Implemented</skos:prefLabel>
    <skos:altLabel xml:lang="en">(i)</skos:altLabel>
    <skos:definition xml:lang="en">The organisation does not identify interested parties or their requirements, leading to a lack of alignment between the information security management system and stakeholder expectations.</skos:definition>
  </rdf:Description>
  
  <rdf:Description rdf:about="https://jenni0608.github.io/NIS2V-Vocabulary-Repository/assessment_ontology_final/MCQ4.6AnswerA">
    <rdf:type rdf:resource="https://jenni0608.github.io/NIS2V-Vocabulary-Repository/assessment_ontology_final/MCQ.4.6"/>
    <skos:prefLabel xml:lang="en">MCQ.4.6 - Not Implemented</skos:prefLabel>
    <skos:altLabel xml:lang="en">(i)</skos:altLabel>
    <skos:definition xml:lang="en">The organisation does not retain documented information about the information security risk assessment process, potentially leading to difficulties in ensuring accountability and traceability in the risk management process.</skos:definition>
  </rdf:Description>
  
  <rdf:Description rdf:about="https://jenni0608.github.io/NIS2V-Vocabulary-Repository/assessment_ontology_final/MCQ5.5AnswerC">
    <rdf:type rdf:resource="https://jenni0608.github.io/NIS2V-Vocabulary-Repository/assessment_ontology_final/MCQ.5.5"/>
    <skos:prefLabel xml:lang="en">MCQ.5.5 - Fully Implemented</skos:prefLabel>
    <skos:altLabel xml:lang="en">(iii)</skos:altLabel>
    <skos:definition xml:lang="en">The organisation fully obtains approval from risk owners for the information security risk treatment plan and acceptance of residual information security risks, ensuring that all stakeholders are aligned and accountable for managing security risks effectively.</skos:definition>
  </rdf:Description>
  
  <rdf:Description rdf:about="https://jenni0608.github.io/NIS2V-Vocabulary-Repository/assessment_ontology_final/MCQ42AnswerC">
    <rdf:type rdf:resource="https://jenni0608.github.io/NIS2V-Vocabulary-Repository/assessment_ontology_final/MCQ.42"/>
    <skos:prefLabel xml:lang="en">MCQ.42 - Fully Implemented</skos:prefLabel>
    <skos:altLabel xml:lang="en">(iii)</skos:altLabel>
    <skos:definition xml:lang="en">The organisation consistently acquires information regarding technical vulnerabilities of the information systems in use, evaluates its exposure to such vulnerabilities, and implements appropriate measures accordingly, ensuring proactive management of security risks.</skos:definition>
  </rdf:Description>
  
  <rdf:Description rdf:about="https://jenni0608.github.io/NIS2V-Vocabulary-Repository/assessment_ontology_final/MCQ51AnswerA">
    <rdf:type rdf:resource="https://jenni0608.github.io/NIS2V-Vocabulary-Repository/assessment_ontology_final/MCQ.51"/>
    <skos:prefLabel xml:lang="en">MCQ.51 - Not Implemented</skos:prefLabel>
    <skos:altLabel xml:lang="en">(i)</skos:altLabel>
    <skos:definition xml:lang="en">Top management is not conducting reviews of the organisation's information security management system at planned intervals, leading to potential gaps in its suitability, adequacy, and effectiveness.</skos:definition>
  </rdf:Description>
  
  <rdf:Description rdf:about="https://jenni0608.github.io/NIS2V-Vocabulary-Repository/assessment_ontology_final/MCQ4.6AnswerC">
    <rdf:type rdf:resource="https://jenni0608.github.io/NIS2V-Vocabulary-Repository/assessment_ontology_final/MCQ.4.6"/>
    <skos:prefLabel xml:lang="en">MCQ.4.6 - Fully Implemented</skos:prefLabel>
    <skos:altLabel xml:lang="en">(iii)</skos:altLabel>
    <skos:definition xml:lang="en">The organisation fully retains documented information about the information security risk assessment process, ensuring comprehensive records are maintained to demonstrate accountability and traceability in the risk management process.</skos:definition>
  </rdf:Description>
  
  <rdf:Description rdf:about="https://jenni0608.github.io/NIS2V-Vocabulary-Repository/assessment_ontology_final/MCQ34.3AnswerB">
    <rdf:type rdf:resource="https://jenni0608.github.io/NIS2V-Vocabulary-Repository/assessment_ontology_final/MCQ.34.3"/>
    <skos:prefLabel xml:lang="en">MCQ.34.3 - Partially Implemented</skos:prefLabel>
    <skos:altLabel xml:lang="en">(ii)</skos:altLabel>
    <skos:definition xml:lang="en">There are some efforts by the organisation to control planned changes and review the consequences of unintended changes. However, these efforts may be inconsistent or not fully comprehensive in mitigating adverse effects.</skos:definition>
  </rdf:Description>
  
  <rdf:Description rdf:about="https://jenni0608.github.io/NIS2V-Vocabulary-Repository/assessment_ontology_final/MCQ55.1AnswerA">
    <rdf:type rdf:resource="https://jenni0608.github.io/NIS2V-Vocabulary-Repository/assessment_ontology_final/MCQ.55.1"/>
    <skos:prefLabel xml:lang="en">MCQ.55.1 - Not Implemented</skos:prefLabel>
    <skos:altLabel xml:lang="en">(i)</skos:altLabel>
    <skos:definition xml:lang="en">Persons doing work under the organisation’s control are not aware of the information security policy, potentially leading to non-compliance and increased security risks.</skos:definition>
  </rdf:Description>
  
  <rdf:Description rdf:about="https://jenni0608.github.io/NIS2V-Vocabulary-Repository/assessment_ontology_final/MCQ75.2AnswerA">
    <rdf:type rdf:resource="https://jenni0608.github.io/NIS2V-Vocabulary-Repository/assessment_ontology_final/MCQ.75.2"/>
    <skos:prefLabel xml:lang="en">MCQ.75.2 - Not Implemented</skos:prefLabel>
    <skos:altLabel xml:lang="en">(i)</skos:altLabel>
    <skos:definition xml:lang="en">Regular updates of the organisation's information security policy, topic-specific policies, and procedures relevant to their job function are not provided to personnel of the organisation and relevant interested parties, potentially leading to outdated practices and non-compliance with security standards.</skos:definition>
  </rdf:Description>
  
  <rdf:Description rdf:about="https://jenni0608.github.io/NIS2V-Vocabulary-Repository/assessment_ontology_final/MCQ100AnswerB">
    <rdf:type rdf:resource="https://jenni0608.github.io/NIS2V-Vocabulary-Repository/assessment_ontology_final/MCQ.100"/>
    <skos:prefLabel xml:lang="en">MCQ.100 - Partially Implemented</skos:prefLabel>
    <skos:altLabel xml:lang="en">(ii)</skos:altLabel>
    <skos:definition xml:lang="en">There are some efforts to apply data leakage prevention measures to systems, networks, and other devices that handle sensitive information, but the implementation may be inconsistent or not fully comprehensive.</skos:definition>
  </rdf:Description>
  
  <rdf:Description rdf:about="https://jenni0608.github.io/NIS2V-Vocabulary-Repository/assessment_ontology_final/MCQ28AnswerA">
    <rdf:type rdf:resource="https://jenni0608.github.io/NIS2V-Vocabulary-Repository/assessment_ontology_final/MCQ.28"/>
    <skos:prefLabel xml:lang="en">MCQ.28 - Not Implemented</skos:prefLabel>
    <skos:altLabel xml:lang="en">(i)</skos:altLabel>
    <skos:definition xml:lang="en">ICT readiness is not planned, implemented, maintained, or tested based on business continuity objectives and ICT continuity requirements, leaving the organisation vulnerable to disruptions in ICT services during crises.</skos:definition>
  </rdf:Description>
  
  <rdf:Description rdf:about="https://jenni0608.github.io/NIS2V-Vocabulary-Repository/assessment_ontology_final/MCQ5.3AnswerA">
    <rdf:type rdf:resource="https://jenni0608.github.io/NIS2V-Vocabulary-Repository/assessment_ontology_final/MCQ.5.3"/>
    <skos:prefLabel xml:lang="en">MCQ.5.3 - Not Implemented</skos:prefLabel>
    <skos:altLabel xml:lang="en">(i)</skos:altLabel>
    <skos:definition xml:lang="en">The organisation does not produce a control of Applicability containing the necessary controls, justification for their inclusion, status of implementation, and reasoning for excluding any controls, potentially leading to ambiguity and lack of transparency in the management of information security controls.</skos:definition>
  </rdf:Description>
  
  <rdf:Description rdf:about="https://jenni0608.github.io/NIS2V-Vocabulary-Repository/assessment_ontology_final/MCQ4.1AnswerC">
    <rdf:type rdf:resource="https://jenni0608.github.io/NIS2V-Vocabulary-Repository/assessment_ontology_final/MCQ.4.1"/>
    <skos:prefLabel xml:lang="en">MCQ.4.1 - Fully Implemented</skos:prefLabel>
    <skos:altLabel xml:lang="en">(iii)</skos:altLabel>
    <skos:definition xml:lang="en">The organisation fully establishes and maintains information security risk criteria, including clear risk acceptance criteria and criteria for performing information security risk assessments, ensuring consistent and effective risk management practices</skos:definition>
  </rdf:Description>
  
  <rdf:Description rdf:about="https://jenni0608.github.io/NIS2V-Vocabulary-Repository/assessment_ontology_final/MCQ63AnswerA">
    <rdf:type rdf:resource="https://jenni0608.github.io/NIS2V-Vocabulary-Repository/assessment_ontology_final/MCQ.63"/>
    <skos:prefLabel xml:lang="en">MCQ.63 - Not Implemented</skos:prefLabel>
    <skos:altLabel xml:lang="en">(i)</skos:altLabel>
    <skos:definition xml:lang="en">The organisation does not acquire information regarding technical vulnerabilities of the information systems in use, evaluate its exposure to such vulnerabilities, or implement appropriate measures, leaving it susceptible to potential security breaches.</skos:definition>
  </rdf:Description>
  
  <rdf:Description rdf:about="https://jenni0608.github.io/NIS2V-Vocabulary-Repository/assessment_ontology_final/MCQ75.2AnswerC">
    <rdf:type rdf:resource="https://jenni0608.github.io/NIS2V-Vocabulary-Repository/assessment_ontology_final/MCQ.75.2"/>
    <skos:prefLabel xml:lang="en">MCQ.75.2 - Fully Implemented</skos:prefLabel>
    <skos:altLabel xml:lang="en">(iii)</skos:altLabel>
    <skos:definition xml:lang="en">Regular updates of the organisation's information security policy, topic-specific policies, and procedures relevant to their job function are fully provided to personnel of the organisation and relevant interested parties, ensuring they are well-informed and compliant with current security standards.</skos:definition>
  </rdf:Description>
  
  <rdf:Description rdf:about="https://jenni0608.github.io/NIS2V-Vocabulary-Repository/assessment_ontology_final/MCQ57.1AnswerC">
    <rdf:type rdf:resource="https://jenni0608.github.io/NIS2V-Vocabulary-Repository/assessment_ontology_final/MCQ.57.1"/>
    <skos:prefLabel xml:lang="en">MCQ.57.1 - Fully Implemented</skos:prefLabel>
    <skos:altLabel xml:lang="en">(iii)</skos:altLabel>
    <skos:definition xml:lang="en">Personnel of the organisation and relevant interested parties receive appropriate information security awareness, education, and training, ensuring they are well-equipped to understand and address security risks and threats effectively.</skos:definition>
  </rdf:Description>
  
  <rdf:Description rdf:about="https://jenni0608.github.io/NIS2V-Vocabulary-Repository/assessment_ontology_final/MCQ6.4AnswerA">
    <rdf:type rdf:resource="https://jenni0608.github.io/NIS2V-Vocabulary-Repository/assessment_ontology_final/MCQ.6.4"/>
    <skos:prefLabel xml:lang="en">MCQ.6.4 - Not Implemented</skos:prefLabel>
    <skos:altLabel xml:lang="en">(i)</skos:altLabel>
    <skos:definition xml:lang="en">There are no established methods or protocols for how to communicate information about the information security management system.</skos:definition>
  </rdf:Description>
  
  <rdf:Description rdf:about="https://jenni0608.github.io/NIS2V-Vocabulary-Repository/assessment_ontology_final/MCQ101AnswerB">
    <rdf:type rdf:resource="https://jenni0608.github.io/NIS2V-Vocabulary-Repository/assessment_ontology_final/MCQ.101"/>
    <skos:prefLabel xml:lang="en">MCQ.101 - Partially Implemented</skos:prefLabel>
    <skos:altLabel xml:lang="en">(ii)</skos:altLabel>
    <skos:definition xml:lang="en">Some efforts are made to identify external and internal issues, but the process lacks thoroughness or consistency, resulting in gaps in understanding potential impacts on the information security management system.</skos:definition>
  </rdf:Description>
  
  <rdf:Description rdf:about="https://jenni0608.github.io/NIS2V-Vocabulary-Repository/assessment_ontology_final/MCQ4.2AnswerB">
    <rdf:type rdf:resource="https://jenni0608.github.io/NIS2V-Vocabulary-Repository/assessment_ontology_final/MCQ.4.2"/>
    <skos:prefLabel xml:lang="en">MCQ.4.2 - Partially Implemented</skos:prefLabel>
    <skos:altLabel xml:lang="en">(ii)</skos:altLabel>
    <skos:definition xml:lang="en">There are some efforts by the organisation to ensure that repeated information security risk assessments produce consistent, valid, and comparable results. However, the process may be inconsistent or not fully effective.</skos:definition>
  </rdf:Description>
  
  <rdf:Description rdf:about="https://jenni0608.github.io/NIS2V-Vocabulary-Repository/assessment_ontology_final/MCQ113AnswerC">
    <rdf:type rdf:resource="https://jenni0608.github.io/NIS2V-Vocabulary-Repository/assessment_ontology_final/MCQ.113"/>
    <skos:prefLabel xml:lang="en">MCQ.113 - Fully Implemented</skos:prefLabel>
    <skos:altLabel xml:lang="en">(iii)</skos:altLabel>
    <skos:definition xml:lang="en">The organisation conducts internal audits at planned intervals to assess conformity to its own requirements and the requirements of relevant documents, as well as the effectiveness of ISMS implementation and maintenance. This ensures a comprehensive evaluation of the ISMS.</skos:definition>
  </rdf:Description>
  
  <rdf:Description rdf:about="https://jenni0608.github.io/NIS2V-Vocabulary-Repository/assessment_ontology_final/MCQ75.2AnswerB">
    <rdf:type rdf:resource="https://jenni0608.github.io/NIS2V-Vocabulary-Repository/assessment_ontology_final/MCQ.75.2"/>
    <skos:prefLabel xml:lang="en">MCQ.75.2 - Partially Implemented</skos:prefLabel>
    <skos:altLabel xml:lang="en">(ii)</skos:altLabel>
    <skos:definition xml:lang="en">There are some efforts to provide regular updates of the organisation's information security policy, topic-specific policies, and procedures relevant to their job function to personnel of the organisation and relevant interested parties, but the updates may be inconsistent or not fully comprehensive.</skos:definition>
  </rdf:Description>
  
  <rdf:Description rdf:about="https://jenni0608.github.io/NIS2V-Vocabulary-Repository/assessment_ontology_final/MCQ89AnswerC">
    <rdf:type rdf:resource="https://jenni0608.github.io/NIS2V-Vocabulary-Repository/assessment_ontology_final/MCQ.89"/>
    <skos:prefLabel xml:lang="en">MCQ.89 - Fully Implemented</skos:prefLabel>
    <skos:altLabel xml:lang="en">(iii)</skos:altLabel>
    <skos:definition xml:lang="en">An inventory of information and other associated assets, including owners, is fully developed and maintained, providing comprehensive visibility and control over organisational assets and their ownership.</skos:definition>
  </rdf:Description>
  
  <rdf:Description rdf:about="https://jenni0608.github.io/NIS2V-Vocabulary-Repository/assessment_ontology_final/MCQ80AnswerB">
    <rdf:type rdf:resource="https://jenni0608.github.io/NIS2V-Vocabulary-Repository/assessment_ontology_final/MCQ.80"/>
    <skos:prefLabel xml:lang="en">MCQ.80 - Partially Implemented</skos:prefLabel>
    <skos:altLabel xml:lang="en">(ii)</skos:altLabel>
    <skos:definition xml:lang="en">There are some rules established to control physical and logical access to information and associated assets, but they may not fully align with both business and information security requirements, leading to potential gaps in access control.</skos:definition>
  </rdf:Description>
  
  <rdf:Description rdf:about="https://jenni0608.github.io/NIS2V-Vocabulary-Repository/assessment_ontology_final/MCQ5.3AnswerC">
    <rdf:type rdf:resource="https://jenni0608.github.io/NIS2V-Vocabulary-Repository/assessment_ontology_final/MCQ.5.3"/>
    <skos:prefLabel xml:lang="en">MCQ.5.3 - Fully Implemented</skos:prefLabel>
    <skos:altLabel xml:lang="en">(iii)</skos:altLabel>
    <skos:definition xml:lang="en">The organisation fully produces a control of Applicability containing the necessary controls, justification for their inclusion, status of implementation, and reasoning for excluding any controls, ensuring transparency and accountability in the management of information security controls.</skos:definition>
  </rdf:Description>
  
  <rdf:Description rdf:about="https://jenni0608.github.io/NIS2V-Vocabulary-Repository/assessment_ontology_final/MCQ106.2AnswerC">
    <rdf:type rdf:resource="https://jenni0608.github.io/NIS2V-Vocabulary-Repository/assessment_ontology_final/MCQ.106.2"/>
    <skos:prefLabel xml:lang="en">MCQ.106.2 - Fully Implemented</skos:prefLabel>
    <skos:altLabel xml:lang="en">(iii)</skos:altLabel>
    <skos:definition xml:lang="en">The organisation plans actions to address identified risks and opportunities for the ISMS and integrates them into ISMS processes, ensuring proactive management and continual improvement. Additionally, mechanisms are established to evaluate the effectiveness of these actions.</skos:definition>
  </rdf:Description>
  
  <rdf:Description rdf:about="https://jenni0608.github.io/NIS2V-Vocabulary-Repository/assessment_ontology_final/MCQ49.3AnswerB">
    <rdf:type rdf:resource="https://jenni0608.github.io/NIS2V-Vocabulary-Repository/assessment_ontology_final/MCQ.49.3"/>
    <skos:prefLabel xml:lang="en">MCQ.49.3 - Partially Implemented</skos:prefLabel>
    <skos:altLabel xml:lang="en">(ii)</skos:altLabel>
    <skos:definition xml:lang="en">There are some efforts by the organisation to specify when monitoring and measuring shall be performed, who shall conduct it, and when the results shall be analysed and evaluated. However, these specifications may be incomplete or not consistently followed.</skos:definition>
  </rdf:Description>
  
  <rdf:Description rdf:about="https://jenni0608.github.io/NIS2V-Vocabulary-Repository/assessment_ontology_final/MCQ39AnswerC">
    <rdf:type rdf:resource="https://jenni0608.github.io/NIS2V-Vocabulary-Repository/assessment_ontology_final/MCQ.39"/>
    <skos:prefLabel xml:lang="en">MCQ.39 - Fully Implemented</skos:prefLabel>
    <skos:altLabel xml:lang="en">(iii)</skos:altLabel>
    <skos:definition xml:lang="en">The organisation regularly monitors, reviews, evaluates, and manages change in supplier information security practices and service delivery, ensuring ongoing compliance and alignment with organisational security requirements and objectives.</skos:definition>
  </rdf:Description>
  
  <rdf:Description rdf:about="https://jenni0608.github.io/NIS2V-Vocabulary-Repository/assessment_ontology_final/MCQ8.2AnswerC">
    <rdf:type rdf:resource="https://jenni0608.github.io/NIS2V-Vocabulary-Repository/assessment_ontology_final/MCQ.8.2"/>
    <skos:prefLabel xml:lang="en">MCQ.8.2 - Fully Implemented</skos:prefLabel>
    <skos:altLabel xml:lang="en">(iii)</skos:altLabel>
    <skos:definition xml:lang="en">The organisation fully retains documented information of the results of these assessments, ensuring comprehensive records are maintained for analysis, decision-making, and reference purposes.</skos:definition>
  </rdf:Description>
  
  <rdf:Description rdf:about="https://jenni0608.github.io/NIS2V-Vocabulary-Repository/assessment_ontology_final/MCQ4.3AnswerA">
    <rdf:type rdf:resource="https://jenni0608.github.io/NIS2V-Vocabulary-Repository/assessment_ontology_final/MCQ.4.3"/>
    <skos:prefLabel xml:lang="en">MCQ.4.3 - Not Implemented</skos:prefLabel>
    <skos:altLabel xml:lang="en">(i)</skos:altLabel>
    <skos:definition xml:lang="en">The organisation does not effectively identify information security risks, failing to apply the information security risk assessment process to identify risks associated with the loss of confidentiality, integrity, and availability for information within the scope of the information security management system, nor does it consistently identify risk owners.</skos:definition>
  </rdf:Description>
  
  <rdf:Description rdf:about="https://jenni0608.github.io/NIS2V-Vocabulary-Repository/assessment_ontology_final/MCQ36AnswerA">
    <rdf:type rdf:resource="https://jenni0608.github.io/NIS2V-Vocabulary-Repository/assessment_ontology_final/MCQ.36"/>
    <skos:prefLabel xml:lang="en">MCQ.36 - Not Implemented</skos:prefLabel>
    <skos:altLabel xml:lang="en">(i)</skos:altLabel>
    <skos:definition xml:lang="en">Processes and procedures to manage information security risks associated with the use of supplier’s products or services are not defined or implemented, leaving the organisation vulnerable to potential security breaches from third-party sources.</skos:definition>
  </rdf:Description>
  
  <rdf:Description rdf:about="https://jenni0608.github.io/NIS2V-Vocabulary-Repository/assessment_ontology_final/MCQ44AnswerA">
    <rdf:type rdf:resource="https://jenni0608.github.io/NIS2V-Vocabulary-Repository/assessment_ontology_final/MCQ.44"/>
    <skos:prefLabel xml:lang="en">MCQ.44 - Not Implemented</skos:prefLabel>
    <skos:altLabel xml:lang="en">(i)</skos:altLabel>
    <skos:definition xml:lang="en">Procedures and measures to securely manage software installation on operational systems have not been implemented, potentially leading to unauthorized or insecure software installations.</skos:definition>
  </rdf:Description>
  
  <rdf:Description rdf:about="https://jenni0608.github.io/NIS2V-Vocabulary-Repository/assessment_ontology_final/MCQ56.4AnswerC">
    <rdf:type rdf:resource="https://jenni0608.github.io/NIS2V-Vocabulary-Repository/assessment_ontology_final/MCQ.56.4"/>
    <skos:prefLabel xml:lang="en">MCQ.56.4 - Fully Implemented</skos:prefLabel>
    <skos:altLabel xml:lang="en">(iii)</skos:altLabel>
    <skos:definition xml:lang="en">There are established methods and protocols for how to communicate information about the information security management system, ensuring effective and secure communication channels.</skos:definition>
  </rdf:Description>
  
  <rdf:Description rdf:about="https://jenni0608.github.io/NIS2V-Vocabulary-Repository/assessment_ontology_final/MCQ50AnswerA">
    <rdf:type rdf:resource="https://jenni0608.github.io/NIS2V-Vocabulary-Repository/assessment_ontology_final/MCQ.50"/>
    <skos:prefLabel xml:lang="en">MCQ.50 - Not Implemented</skos:prefLabel>
    <skos:altLabel xml:lang="en">(i)</skos:altLabel>
    <skos:definition xml:lang="en">The organisation is not conducting internal audits at planned intervals to determine conformity to its own requirements and the requirements outlined in the document, risking non-compliance and ineffective implementation of the information security management system.</skos:definition>
  </rdf:Description>
  
  <rdf:Description rdf:about="https://jenni0608.github.io/NIS2V-Vocabulary-Repository/assessment_ontology_final/MCQ49.3AnswerA">
    <rdf:type rdf:resource="https://jenni0608.github.io/NIS2V-Vocabulary-Repository/assessment_ontology_final/MCQ.49.3"/>
    <skos:prefLabel xml:lang="en">MCQ.49.3 - Not Implemented</skos:prefLabel>
    <skos:altLabel xml:lang="en">(i)</skos:altLabel>
    <skos:definition xml:lang="en">The organisation does not specify when monitoring and measuring shall be performed, who shall conduct it, and when the results shall be analysed and evaluated, potentially leading to inconsistencies and delays in the assessment of performance.</skos:definition>
  </rdf:Description>
  
  <rdf:Description rdf:about="https://jenni0608.github.io/NIS2V-Vocabulary-Repository/assessment_ontology_final/MCQ71AnswerC">
    <rdf:type rdf:resource="https://jenni0608.github.io/NIS2V-Vocabulary-Repository/assessment_ontology_final/MCQ.71"/>
    <skos:prefLabel xml:lang="en">MCQ.71 - Fully Implemented</skos:prefLabel>
    <skos:altLabel xml:lang="en">(iii)</skos:altLabel>
    <skos:definition xml:lang="en">Clear and comprehensive information security roles and responsibilities are defined and allocated based on the specific needs of the organisation. There are established processes for regularly reviewing and updating these roles to ensure alignment with organisational requirements.</skos:definition>
  </rdf:Description>
  
  <rdf:Description rdf:about="https://jenni0608.github.io/NIS2V-Vocabulary-Repository/assessment_ontology_final/MCQ55.2AnswerA">
    <rdf:type rdf:resource="https://jenni0608.github.io/NIS2V-Vocabulary-Repository/assessment_ontology_final/MCQ.55.2"/>
    <skos:prefLabel xml:lang="en">MCQ.55.2 - Not Implemented</skos:prefLabel>
    <skos:altLabel xml:lang="en">(i)</skos:altLabel>
    <skos:definition xml:lang="en">Persons doing work under the organisation’s control are not aware of their contribution to the effectiveness of the information security management system, nor do they understand the benefits of improved information security performance, potentially leading to decreased motivation and engagement in security efforts.</skos:definition>
  </rdf:Description>
  
  <rdf:Description rdf:about="https://jenni0608.github.io/NIS2V-Vocabulary-Repository/assessment_ontology_final/MCQ2.2AnswerC">
    <rdf:type rdf:resource="https://jenni0608.github.io/NIS2V-Vocabulary-Repository/assessment_ontology_final/MCQ.2.2"/>
    <skos:prefLabel xml:lang="en">MCQ.2.2 - Fully Implemented</skos:prefLabel>
    <skos:altLabel xml:lang="en">(iii)</skos:altLabel>
    <skos:definition xml:lang="en">The information security policy is fully available as documented information, communicated effectively within the organisation, and made available to interested parties as appropriate, ensuring broad awareness and understanding of the organisation's information security objectives.</skos:definition>
  </rdf:Description>
  
  <rdf:Description rdf:about="https://jenni0608.github.io/NIS2V-Vocabulary-Repository/assessment_ontology_final/MCQ5.6AnswerA">
    <rdf:type rdf:resource="https://jenni0608.github.io/NIS2V-Vocabulary-Repository/assessment_ontology_final/MCQ.5.6"/>
    <skos:prefLabel xml:lang="en">MCQ.5.6 - Not Implemented</skos:prefLabel>
    <skos:altLabel xml:lang="en">(i)</skos:altLabel>
    <skos:definition xml:lang="en">The organisation does not retain documented information about the information security risk treatment process, potentially leading to lack of accountability and transparency in managing security risks.</skos:definition>
  </rdf:Description>
  
  <rdf:Description rdf:about="https://jenni0608.github.io/NIS2V-Vocabulary-Repository/assessment_ontology_final/MCQ62AnswerA">
    <rdf:type rdf:resource="https://jenni0608.github.io/NIS2V-Vocabulary-Repository/assessment_ontology_final/MCQ.62"/>
    <skos:prefLabel xml:lang="en">MCQ.62 - Not Implemented</skos:prefLabel>
    <skos:altLabel xml:lang="en">(i)</skos:altLabel>
    <skos:definition xml:lang="en">Access rights to information and associated assets are not provisioned, reviewed, modified, or removed in accordance with the organisation’s topic-specific policy and rules for access control, leading to inconsistencies and potential security risks.</skos:definition>
  </rdf:Description>
  
  <rdf:Description rdf:about="https://jenni0608.github.io/NIS2V-Vocabulary-Repository/assessment_ontology_final/MCQ123AnswerC">
    <rdf:type rdf:resource="https://jenni0608.github.io/NIS2V-Vocabulary-Repository/assessment_ontology_final/MCQ.123"/>
    <skos:prefLabel xml:lang="en">MCQ.123 - Fully Implemented</skos:prefLabel>
    <skos:altLabel xml:lang="en">(iii)</skos:altLabel>
    <skos:definition xml:lang="en">The organisation has established and implemented appropriate procedures to protect intellectual property rights effectively, safeguarding valuable assets from infringement or unauthorized use.</skos:definition>
  </rdf:Description>
  
  <rdf:Description rdf:about="https://jenni0608.github.io/NIS2V-Vocabulary-Repository/assessment_ontology_final/MCQ81AnswerA">
    <rdf:type rdf:resource="https://jenni0608.github.io/NIS2V-Vocabulary-Repository/assessment_ontology_final/MCQ.81"/>
    <skos:prefLabel xml:lang="en">MCQ.81 - Not Implemented</skos:prefLabel>
    <skos:altLabel xml:lang="en">(i)</skos:altLabel>
    <skos:definition xml:lang="en">The organisation lacks a structured approach to managing the complete life cycle of identities, leading to inconsistencies and gaps in identity-related processes such as creation, authentication, and termination.</skos:definition>
  </rdf:Description>
  
  <rdf:Description rdf:about="https://jenni0608.github.io/NIS2V-Vocabulary-Repository/assessment_ontology_final/MCQ124AnswerC">
    <rdf:type rdf:resource="https://jenni0608.github.io/NIS2V-Vocabulary-Repository/assessment_ontology_final/MCQ.124"/>
    <skos:prefLabel xml:lang="en">MCQ.124 - Fully Implemented</skos:prefLabel>
    <skos:altLabel xml:lang="en">(iii)</skos:altLabel>
    <skos:definition xml:lang="en">The organisation has established and implemented robust measures to protect records effectively from risks such as loss, destruction, falsification, unauthorized access, and unauthorized release, ensuring the integrity and confidentiality of sensitive information.</skos:definition>
  </rdf:Description>
  
  <rdf:Description rdf:about="https://jenni0608.github.io/NIS2V-Vocabulary-Repository/assessment_ontology_final/MCQ117AnswerC">
    <rdf:type rdf:resource="https://jenni0608.github.io/NIS2V-Vocabulary-Repository/assessment_ontology_final/MCQ.117"/>
    <skos:prefLabel xml:lang="en">MCQ.117 - Fully Implemented</skos:prefLabel>
    <skos:altLabel xml:lang="en">(iii)</skos:altLabel>
    <skos:definition xml:lang="en">The organisation has established processes to continually improve the suitability, adequacy, and effectiveness of the ISMS. These processes are systematically implemented and monitored to ensure ongoing enhancement of information security practices.</skos:definition>
  </rdf:Description>
  
  <rdf:Description rdf:about="https://jenni0608.github.io/NIS2V-Vocabulary-Repository/assessment_ontology_final/MCQ94AnswerA">
    <rdf:type rdf:resource="https://jenni0608.github.io/NIS2V-Vocabulary-Repository/assessment_ontology_final/MCQ.94"/>
    <skos:prefLabel xml:lang="en">MCQ.94 - Not Implemented</skos:prefLabel>
    <skos:altLabel xml:lang="en">(i)</skos:altLabel>
    <skos:definition xml:lang="en">Information stored on, processed by, or accessible via user endpoint devices is not adequately protected, leaving it vulnerable to unauthorized access, theft, or manipulation.</skos:definition>
  </rdf:Description>
  
  <rdf:Description rdf:about="https://jenni0608.github.io/NIS2V-Vocabulary-Repository/assessment_ontology_final/MCQ75.1AnswerB">
    <rdf:type rdf:resource="https://jenni0608.github.io/NIS2V-Vocabulary-Repository/assessment_ontology_final/MCQ.75.1"/>
    <skos:prefLabel xml:lang="en">MCQ.75.1 - Partially Implemented</skos:prefLabel>
    <skos:altLabel xml:lang="en">(ii)</skos:altLabel>
    <skos:definition xml:lang="en">There are some efforts to provide personnel of the organisation and relevant interested parties with information security awareness, education, and training, but it may be inconsistent or not fully comprehensive.</skos:definition>
  </rdf:Description>
  
  <rdf:Description rdf:about="https://jenni0608.github.io/NIS2V-Vocabulary-Repository/assessment_ontology_final/MCQ107.2AnswerC">
    <rdf:type rdf:resource="https://jenni0608.github.io/NIS2V-Vocabulary-Repository/assessment_ontology_final/MCQ.107.2"/>
    <skos:prefLabel xml:lang="en">MCQ.107.2 - Fully Implemented</skos:prefLabel>
    <skos:altLabel xml:lang="en">(iii)</skos:altLabel>
    <skos:definition xml:lang="en">The organisation systematically plans for the achievement of information security objectives by determining actions, resource requirements, responsibilities, timelines, and evaluation methods, ensuring clarity and effectiveness in implementation and monitoring. Additionally, documented information is retained on the information security objectives.</skos:definition>
  </rdf:Description>
  
  <rdf:Description rdf:about="https://jenni0608.github.io/NIS2V-Vocabulary-Repository/assessment_ontology_final/MCQ22.4AnswerC">
    <rdf:type rdf:resource="https://jenni0608.github.io/NIS2V-Vocabulary-Repository/assessment_ontology_final/MCQ.22.4"/>
    <skos:prefLabel xml:lang="en">MCQ.22.4 - Fully Implemented</skos:prefLabel>
    <skos:altLabel xml:lang="en">(iii)</skos:altLabel>
    <skos:definition xml:lang="en">The information security policy fully includes a commitment to continual improvement of the information security management system, reflecting the organisation's proactive stance towards enhancing security processes and practices over time.</skos:definition>
  </rdf:Description>
  
  <rdf:Description rdf:about="https://jenni0608.github.io/NIS2V-Vocabulary-Repository/assessment_ontology_final/MCQ44AnswerC">
    <rdf:type rdf:resource="https://jenni0608.github.io/NIS2V-Vocabulary-Repository/assessment_ontology_final/MCQ.44"/>
    <skos:prefLabel xml:lang="en">MCQ.44 - Fully Implemented</skos:prefLabel>
    <skos:altLabel xml:lang="en">(iii)</skos:altLabel>
    <skos:definition xml:lang="en">Procedures and measures to securely manage software installation on operational systems are fully implemented, ensuring that only authorized and secure software is installed, minimizing security risks.</skos:definition>
  </rdf:Description>
  
  <rdf:Description rdf:about="https://jenni0608.github.io/NIS2V-Vocabulary-Repository/assessment_ontology_final/MCQ4.5AnswerC">
    <rdf:type rdf:resource="https://jenni0608.github.io/NIS2V-Vocabulary-Repository/assessment_ontology_final/MCQ.4.5"/>
    <skos:prefLabel xml:lang="en">MCQ.4.5 - Fully Implemented</skos:prefLabel>
    <skos:altLabel xml:lang="en">(iii)</skos:altLabel>
    <skos:definition xml:lang="en">The organisation fully evaluates information security risks, effectively comparing the results of risk analysis with established risk criteria and prioritizing the analysed risks for risk treatment comprehensively.</skos:definition>
  </rdf:Description>
  
  <rdf:Description rdf:about="https://jenni0608.github.io/NIS2V-Vocabulary-Repository/assessment_ontology_final/MCQ130AnswerC">
    <rdf:type rdf:resource="https://jenni0608.github.io/NIS2V-Vocabulary-Repository/assessment_ontology_final/MCQ.130"/>
    <skos:prefLabel xml:lang="en">MCQ.130 - Fully Implemented</skos:prefLabel>
    <skos:altLabel xml:lang="en">(iii)</skos:altLabel>
    <skos:definition xml:lang="en">The organisation has established and consistently follows maintenance procedures to ensure the availability, integrity, and confidentiality of information by maintaining equipment effectively. This ensures that equipment remains reliable and secure, minimizing disruptions and risks to information security.</skos:definition>
  </rdf:Description>
  
  <rdf:Description rdf:about="https://jenni0608.github.io/NIS2V-Vocabulary-Repository/assessment_ontology_final/MCQ57.2AnswerB">
    <rdf:type rdf:resource="https://jenni0608.github.io/NIS2V-Vocabulary-Repository/assessment_ontology_final/MCQ.57.2"/>
    <skos:prefLabel xml:lang="en">MCQ.57.2 - Partially Implemented</skos:prefLabel>
    <skos:altLabel xml:lang="en">(ii)</skos:altLabel>
    <skos:definition xml:lang="en">There are some efforts to provide regular updates of the organisation's information security policy, topic-specific policies, and procedures relevant to their job function to personnel of the organisation and relevant interested parties, but the updates may be inconsistent or not fully comprehensive.</skos:definition>
  </rdf:Description>
  
  <rdf:Description rdf:about="https://jenni0608.github.io/NIS2V-Vocabulary-Repository/assessment_ontology_final/MCQ38AnswerB">
    <rdf:type rdf:resource="https://jenni0608.github.io/NIS2V-Vocabulary-Repository/assessment_ontology_final/MCQ.38"/>
    <skos:prefLabel xml:lang="en">MCQ.38 - Partially Implemented</skos:prefLabel>
    <skos:altLabel xml:lang="en">(ii)</skos:altLabel>
    <skos:definition xml:lang="en">There are some processes and procedures in place to manage information security risks associated with the ICT products and services supply chain, but they may be incomplete or inconsistently applied.</skos:definition>
  </rdf:Description>
  
  <rdf:Description rdf:about="https://jenni0608.github.io/NIS2V-Vocabulary-Repository/assessment_ontology_final/MCQ122AnswerC">
    <rdf:type rdf:resource="https://jenni0608.github.io/NIS2V-Vocabulary-Repository/assessment_ontology_final/MCQ.122"/>
    <skos:prefLabel xml:lang="en">MCQ.122 - Fully Implemented</skos:prefLabel>
    <skos:altLabel xml:lang="en">(iii)</skos:altLabel>
    <skos:definition xml:lang="en">The organisation systematically plans and agrees upon audit tests and assurance activities involving the assessment of operational systems between testers and appropriate management. This ensures a structured and coordinated approach, leading to effective assessment and improvement of operational systems.
	</skos:definition>
  </rdf:Description>
  
  <rdf:Description rdf:about="https://jenni0608.github.io/NIS2V-Vocabulary-Repository/assessment_ontology_final/MCQ52AnswerC">
    <rdf:type rdf:resource="https://jenni0608.github.io/NIS2V-Vocabulary-Repository/assessment_ontology_final/MCQ.52"/>
    <skos:prefLabel xml:lang="en">MCQ.52 - Fully Implemented</skos:prefLabel>
    <skos:altLabel xml:lang="en">(iii)</skos:altLabel>
    <skos:definition xml:lang="en">The organisation’s approach to managing information security and its implementation, including people, processes, and technologies, is independently reviewed at planned intervals or when significant changes occur, ensuring ongoing effectiveness and alignment with security objectives.</skos:definition>
  </rdf:Description>
  
  <rdf:Description rdf:about="https://jenni0608.github.io/NIS2V-Vocabulary-Repository/assessment_ontology_final/MCQ9.1AnswerA">
    <rdf:type rdf:resource="https://jenni0608.github.io/NIS2V-Vocabulary-Repository/assessment_ontology_final/MCQ.9.1"/>
    <skos:prefLabel xml:lang="en">MCQ.9.1 - Not Implemented</skos:prefLabel>
    <skos:altLabel xml:lang="en">(i)</skos:altLabel>
    <skos:definition xml:lang="en">The organisation does not implement the information security risk treatment plan, potentially leaving identified risks unaddressed and the organisation vulnerable to security threats.</skos:definition>
  </rdf:Description>
  
  <rdf:Description rdf:about="https://jenni0608.github.io/NIS2V-Vocabulary-Repository/assessment_ontology_final/MCQ65AnswerA">
    <rdf:type rdf:resource="https://jenni0608.github.io/NIS2V-Vocabulary-Repository/assessment_ontology_final/MCQ.65"/>
    <skos:prefLabel xml:lang="en">MCQ.65 - Not Implemented</skos:prefLabel>
    <skos:altLabel xml:lang="en">(i)</skos:altLabel>
    <skos:definition xml:lang="en">Protection against malware is not implemented, and there are no appropriate user awareness measures in place, leaving the organisation vulnerable to malware attacks.</skos:definition>
  </rdf:Description>
  
  <rdf:Description rdf:about="https://jenni0608.github.io/NIS2V-Vocabulary-Repository/assessment_ontology_final/MCQ22.1AnswerB">
    <rdf:type rdf:resource="https://jenni0608.github.io/NIS2V-Vocabulary-Repository/assessment_ontology_final/MCQ.22.1"/>
    <skos:prefLabel xml:lang="en">MCQ.22.1 - Partially Implemented</skos:prefLabel>
    <skos:altLabel xml:lang="en">(ii)</skos:altLabel>
    <skos:definition xml:lang="en">There are some efforts by top management to establish an information security policy appropriate to the purpose of the organisation. However, the policy may not fully align with the organisation's objectives or may lack specificity.</skos:definition>
  </rdf:Description>
  
  <rdf:Description rdf:about="https://jenni0608.github.io/NIS2V-Vocabulary-Repository/assessment_ontology_final/MCQ87AnswerA">
    <rdf:type rdf:resource="https://jenni0608.github.io/NIS2V-Vocabulary-Repository/assessment_ontology_final/MCQ.87"/>
    <skos:prefLabel xml:lang="en">MCQ.87 - Not Implemented</skos:prefLabel>
    <skos:altLabel xml:lang="en">(i)</skos:altLabel>
    <skos:definition xml:lang="en">Secure authentication technologies and procedures are not implemented based on information access restrictions and the topic-specific policy on access control, resulting in potential vulnerabilities and unauthorized access.</skos:definition>
  </rdf:Description>
  
  <rdf:Description rdf:about="https://jenni0608.github.io/NIS2V-Vocabulary-Repository/assessment_ontology_final/MCQ112.2AnswerA">
    <rdf:type rdf:resource="https://jenni0608.github.io/NIS2V-Vocabulary-Repository/assessment_ontology_final/MCQ.112.2"/>
    <skos:prefLabel xml:lang="en">MCQ.112.2 - Not Implemented</skos:prefLabel>
    <skos:altLabel xml:lang="en">(i)</skos:altLabel>
    <skos:definition xml:lang="en">The organisation lacks a systematic approach to evaluating information security performance and ISMS effectiveness, resulting in a lack of understanding of the system's performance and effectiveness.</skos:definition>
  </rdf:Description>
  
  <rdf:Description rdf:about="https://jenni0608.github.io/NIS2V-Vocabulary-Repository/assessment_ontology_final/MCQ5.1AnswerB">
    <rdf:type rdf:resource="https://jenni0608.github.io/NIS2V-Vocabulary-Repository/assessment_ontology_final/MCQ.5.1"/>
    <skos:prefLabel xml:lang="en">MCQ.5.1 - Partially Implemented</skos:prefLabel>
    <skos:altLabel xml:lang="en">(ii)</skos:altLabel>
    <skos:definition xml:lang="en">There are some efforts by the organisation to select appropriate information security risk treatment options, considering the results of the risk assessment. However, the process may be inconsistent or not fully comprehensive.</skos:definition>
  </rdf:Description>
  
  <rdf:Description rdf:about="https://jenni0608.github.io/NIS2V-Vocabulary-Repository/assessment_ontology_final/MCQ25AnswerA">
    <rdf:type rdf:resource="https://jenni0608.github.io/NIS2V-Vocabulary-Repository/assessment_ontology_final/MCQ.25"/>
    <skos:prefLabel xml:lang="en">MCQ.25 - Not Implemented</skos:prefLabel>
    <skos:altLabel xml:lang="en">(i)</skos:altLabel>
    <skos:definition xml:lang="en">Networks, systems, and applications are not monitored for anomalous behaviour, and there are no processes in place to evaluate potential information security incidents, leaving the organisation vulnerable to undetected threats.</skos:definition>
  </rdf:Description>
  
  <rdf:Description rdf:about="https://jenni0608.github.io/NIS2V-Vocabulary-Repository/assessment_ontology_final/MCQ90AnswerC">
    <rdf:type rdf:resource="https://jenni0608.github.io/NIS2V-Vocabulary-Repository/assessment_ontology_final/MCQ.90"/>
    <skos:prefLabel xml:lang="en">MCQ.90 - Fully Implemented</skos:prefLabel>
    <skos:altLabel xml:lang="en">(iii)</skos:altLabel>
    <skos:definition xml:lang="en">Rules for acceptable use and procedures for handling information and other associated assets are fully identified, documented, and implemented, ensuring that personnel understand and adhere to appropriate practices for safeguarding sensitive information.</skos:definition>
  </rdf:Description>
  
  <rdf:Description rdf:about="https://jenni0608.github.io/NIS2V-Vocabulary-Repository/assessment_ontology_final/MCQ99AnswerB">
    <rdf:type rdf:resource="https://jenni0608.github.io/NIS2V-Vocabulary-Repository/assessment_ontology_final/MCQ.99"/>
    <skos:prefLabel xml:lang="en">MCQ.99 - Partially Implemented</skos:prefLabel>
    <skos:altLabel xml:lang="en">(ii)</skos:altLabel>
    <skos:definition xml:lang="en">There are some efforts to implement secure authentication technologies and procedures based on information access restrictions and the topic-specific policy on access control, but the implementation may be incomplete or inconsistent.</skos:definition>
  </rdf:Description>
  
  <rdf:Description rdf:about="https://jenni0608.github.io/NIS2V-Vocabulary-Repository/assessment_ontology_final/MCQ115AnswerA">
    <rdf:type rdf:resource="https://jenni0608.github.io/NIS2V-Vocabulary-Repository/assessment_ontology_final/MCQ.115"/>
    <skos:prefLabel xml:lang="en">MCQ.115 - Not Implemented</skos:prefLabel>
    <skos:altLabel xml:lang="en">(i)</skos:altLabel>
    <skos:definition xml:lang="en">Senior management does not conduct reviews of the information security management system at planned intervals, leading to uncertainty regarding its suitability, adequacy, and effectiveness.</skos:definition>
  </rdf:Description>
  
  <rdf:Description rdf:about="https://jenni0608.github.io/NIS2V-Vocabulary-Repository/assessment_ontology_final/MCQ44AnswerB">
    <rdf:type rdf:resource="https://jenni0608.github.io/NIS2V-Vocabulary-Repository/assessment_ontology_final/MCQ.44"/>
    <skos:prefLabel xml:lang="en">MCQ.44 - Partially Implemented</skos:prefLabel>
    <skos:altLabel xml:lang="en">(ii)</skos:altLabel>
    <skos:definition xml:lang="en">There are some procedures and measures in place to securely manage software installation on operational systems, but they may be incomplete or inconsistently applied.</skos:definition>
  </rdf:Description>
  
  <rdf:Description rdf:about="https://jenni0608.github.io/NIS2V-Vocabulary-Repository/assessment_ontology_final/MCQ47AnswerA">
    <rdf:type rdf:resource="https://jenni0608.github.io/NIS2V-Vocabulary-Repository/assessment_ontology_final/MCQ.47"/>
    <skos:prefLabel xml:lang="en">MCQ.47 - Not Implemented</skos:prefLabel>
    <skos:altLabel xml:lang="en">(i)</skos:altLabel>
    <skos:definition xml:lang="en">Segregation is not implemented for groups of information services, users, and information systems within the organisation's networks, leading to potential risks of unauthorized access and data breaches.</skos:definition>
  </rdf:Description>
  
  <rdf:Description rdf:about="https://jenni0608.github.io/NIS2V-Vocabulary-Repository/assessment_ontology_final/MCQ11AnswerB">
    <rdf:type rdf:resource="https://jenni0608.github.io/NIS2V-Vocabulary-Repository/assessment_ontology_final/MCQ.11"/>
    <skos:prefLabel xml:lang="en">MCQ.11 - Partially Implemented</skos:prefLabel>
    <skos:altLabel xml:lang="en">(ii)</skos:altLabel>
    <skos:definition xml:lang="en">There is a general understanding of information security roles and responsibilities, but they are not consistently defined and allocated according to the organisation's needs. Some areas may lack clarity or consistency.</skos:definition>
  </rdf:Description>
  
  <rdf:Description rdf:about="https://jenni0608.github.io/NIS2V-Vocabulary-Repository/assessment_ontology_final/MCQ9.2AnswerC">
    <rdf:type rdf:resource="https://jenni0608.github.io/NIS2V-Vocabulary-Repository/assessment_ontology_final/MCQ.9.2"/>
    <skos:prefLabel xml:lang="en">MCQ.9.2 - Fully Implemented</skos:prefLabel>
    <skos:altLabel xml:lang="en">(iii)</skos:altLabel>
    <skos:definition xml:lang="en">The organisation fully retains documented information of the results of the information security risk treatment, ensuring comprehensive records are maintained for analysis, audit, and reference purposes.</skos:definition>
  </rdf:Description>
  
  <rdf:Description rdf:about="https://jenni0608.github.io/NIS2V-Vocabulary-Repository/assessment_ontology_final/MCQ3.1AnswerC">
    <rdf:type rdf:resource="https://jenni0608.github.io/NIS2V-Vocabulary-Repository/assessment_ontology_final/MCQ.3.1"/>
    <skos:prefLabel xml:lang="en">MCQ.3.1 - Fully Implemented</skos:prefLabel>
    <skos:altLabel xml:lang="en">(iii)</skos:altLabel>
    <skos:definition xml:lang="en">Top management ensures that responsibilities and authorities for roles relevant to information security are assigned and effectively communicated within the organisation, ensuring clear understanding and accountability for information security responsibilities.</skos:definition>
  </rdf:Description>
  
  <rdf:Description rdf:about="https://jenni0608.github.io/NIS2V-Vocabulary-Repository/assessment_ontology_final/MCQ85AnswerB">
    <rdf:type rdf:resource="https://jenni0608.github.io/NIS2V-Vocabulary-Repository/assessment_ontology_final/MCQ.85"/>
    <skos:prefLabel xml:lang="en">MCQ.85 - Partially Implemented</skos:prefLabel>
    <skos:altLabel xml:lang="en">(ii)</skos:altLabel>
    <skos:definition xml:lang="en">There are some efforts to restrict access to information and other associated assets in accordance with the established topic-specific policy on access control, but the implementation may be inconsistent or incomplete.</skos:definition>
  </rdf:Description>
  
  <rdf:Description rdf:about="https://jenni0608.github.io/NIS2V-Vocabulary-Repository/assessment_ontology_final/MCQ89AnswerA">
    <rdf:type rdf:resource="https://jenni0608.github.io/NIS2V-Vocabulary-Repository/assessment_ontology_final/MCQ.89"/>
    <skos:prefLabel xml:lang="en">MCQ.89 - Not Implemented</skos:prefLabel>
    <skos:altLabel xml:lang="en">(i)</skos:altLabel>
    <skos:definition xml:lang="en">An inventory of information and other associated assets, including owners, is not developed or maintained, leading to lack of visibility and control over organisational assets.</skos:definition>
  </rdf:Description>
  
  <rdf:Description rdf:about="https://jenni0608.github.io/NIS2V-Vocabulary-Repository/assessment_ontology_final/MCQ5.3AnswerB">
    <rdf:type rdf:resource="https://jenni0608.github.io/NIS2V-Vocabulary-Repository/assessment_ontology_final/MCQ.5.3"/>
    <skos:prefLabel xml:lang="en">MCQ.5.3 - Partially Implemented</skos:prefLabel>
    <skos:altLabel xml:lang="en">(ii)</skos:altLabel>
    <skos:definition xml:lang="en">There are some efforts by the organisation to produce a control of Applicability containing the necessary controls, justification for their inclusion, status of implementation, and reasoning for excluding any controls. However, this documentation may be incomplete or inconsistently maintained.</skos:definition>
  </rdf:Description>
  
  <rdf:Description rdf:about="https://jenni0608.github.io/NIS2V-Vocabulary-Repository/assessment_ontology_final/MCQ1AnswerA">
    <rdf:type rdf:resource="https://jenni0608.github.io/NIS2V-Vocabulary-Repository/assessment_ontology_final/MCQ.1"/>
    <skos:prefLabel xml:lang="en">MCQ.1 - Not Implemented</skos:prefLabel>
    <skos:altLabel xml:lang="en">(i)</skos:altLabel>
    <skos:definition xml:lang="en">The organisation does not actively identify external and internal issues relevant to its purpose or their potential impact on achieving the intended outcomes of its information security management system.</skos:definition>
  </rdf:Description>
  
  <rdf:Description rdf:about="https://jenni0608.github.io/NIS2V-Vocabulary-Repository/assessment_ontology_final/MCQ49.1AnswerA">
    <rdf:type rdf:resource="https://jenni0608.github.io/NIS2V-Vocabulary-Repository/assessment_ontology_final/MCQ.49.1"/>
    <skos:prefLabel xml:lang="en">MCQ.49.1 - Not Implemented</skos:prefLabel>
    <skos:altLabel xml:lang="en">(i)</skos:altLabel>
    <skos:definition xml:lang="en">The organisation does not determine what needs to be monitored and measured, including information security processes and controls, potentially leading to a lack of oversight and inability to assess the effectiveness of security measures.</skos:definition>
  </rdf:Description>
  
  <rdf:Description rdf:about="https://jenni0608.github.io/NIS2V-Vocabulary-Repository/assessment_ontology_final/MCQ6.1AnswerA">
    <rdf:type rdf:resource="https://jenni0608.github.io/NIS2V-Vocabulary-Repository/assessment_ontology_final/MCQ.6.1"/>
    <skos:prefLabel xml:lang="en">MCQ.6.1 - Not Implemented</skos:prefLabel>
    <skos:altLabel xml:lang="en">(i)</skos:altLabel>
    <skos:definition xml:lang="en">The organisation does not determine what information to communicate regarding the information security management system.</skos:definition>
  </rdf:Description>
  
  <rdf:Description rdf:about="https://jenni0608.github.io/NIS2V-Vocabulary-Repository/assessment_ontology_final/MCQ4.1AnswerB">
    <rdf:type rdf:resource="https://jenni0608.github.io/NIS2V-Vocabulary-Repository/assessment_ontology_final/MCQ.4.1"/>
    <skos:prefLabel xml:lang="en">MCQ.4.1 - Partially Implemented</skos:prefLabel>
    <skos:altLabel xml:lang="en">(ii)</skos:altLabel>
    <skos:definition xml:lang="en">There are some efforts by the organisation to establish and maintain information security risk criteria, including risk acceptance criteria and criteria for performing information security risk assessments. However, these criteria may be inconsistent or not fully comprehensive.</skos:definition>
  </rdf:Description>
  
  <rdf:Description rdf:about="https://jenni0608.github.io/NIS2V-Vocabulary-Repository/assessment_ontology_final/MCQ39AnswerB">
    <rdf:type rdf:resource="https://jenni0608.github.io/NIS2V-Vocabulary-Repository/assessment_ontology_final/MCQ.39"/>
    <skos:prefLabel xml:lang="en">MCQ.39 - Partially Implemented</skos:prefLabel>
    <skos:altLabel xml:lang="en">(ii)</skos:altLabel>
    <skos:definition xml:lang="en">There are some efforts to monitor, review, evaluate, and manage change in supplier information security practices and service delivery, but the process may be inconsistent or not fully comprehensive.</skos:definition>
  </rdf:Description>
  
  <rdf:Description rdf:about="https://jenni0608.github.io/NIS2V-Vocabulary-Repository/assessment_ontology_final/MCQ56.2AnswerA">
    <rdf:type rdf:resource="https://jenni0608.github.io/NIS2V-Vocabulary-Repository/assessment_ontology_final/MCQ.56.2"/>
    <skos:prefLabel xml:lang="en">MCQ.56.2 - Not Implemented</skos:prefLabel>
    <skos:altLabel xml:lang="en">(i)</skos:altLabel>
    <skos:definition xml:lang="en">There is no established timing for communication related to the information security management system.</skos:definition>
  </rdf:Description>
  
  <rdf:Description rdf:about="https://jenni0608.github.io/NIS2V-Vocabulary-Repository/assessment_ontology_final/MCQ34.2AnswerC">
    <rdf:type rdf:resource="https://jenni0608.github.io/NIS2V-Vocabulary-Repository/assessment_ontology_final/MCQ.34.2"/>
    <skos:prefLabel xml:lang="en">MCQ.34.2 - Fully Implemented</skos:prefLabel>
    <skos:altLabel xml:lang="en">(iii)</skos:altLabel>
    <skos:definition xml:lang="en">Documented information is fully available to the extent necessary to have confidence that the processes have been carried out as planned, ensuring transparency and accountability in process implementation.</skos:definition>
  </rdf:Description>
  
  <rdf:Description rdf:about="https://jenni0608.github.io/NIS2V-Vocabulary-Repository/assessment_ontology_final/MCQ2.1AnswerB">
    <rdf:type rdf:resource="https://jenni0608.github.io/NIS2V-Vocabulary-Repository/assessment_ontology_final/MCQ.2.1"/>
    <skos:prefLabel xml:lang="en">MCQ.2.1 - Partially Implemented</skos:prefLabel>
    <skos:altLabel xml:lang="en">(ii)</skos:altLabel>
    <skos:definition xml:lang="en">There are some efforts by top management to establish an information security policy appropriate to the organisation's purpose, and it may include information security objectives or provide a framework for setting them. However, the commitment to satisfying applicable requirements related to information security and continually improving the information security management system may be inconsistent.</skos:definition>
  </rdf:Description>
  
  <rdf:Description rdf:about="https://jenni0608.github.io/NIS2V-Vocabulary-Repository/assessment_ontology_final/MCQ120AnswerA">
    <rdf:type rdf:resource="https://jenni0608.github.io/NIS2V-Vocabulary-Repository/assessment_ontology_final/MCQ.120"/>
    <skos:prefLabel xml:lang="en">MCQ.120 - Not Implemented</skos:prefLabel>
    <skos:altLabel xml:lang="en">(i)</skos:altLabel>
    <skos:definition xml:lang="en">The organisation lacks procedures for information labeling or does not align them with the adopted information classification scheme, leading to confusion and inconsistency in labeling practices.</skos:definition>
  </rdf:Description>
  
  <rdf:Description rdf:about="https://jenni0608.github.io/NIS2V-Vocabulary-Repository/assessment_ontology_final/MCQ105.3AnswerC">
    <rdf:type rdf:resource="https://jenni0608.github.io/NIS2V-Vocabulary-Repository/assessment_ontology_final/MCQ.105.3"/>
    <skos:prefLabel xml:lang="en">MCQ.105.3 - Fully Implemented</skos:prefLabel>
    <skos:altLabel xml:lang="en">(iii)</skos:altLabel>
    <skos:definition xml:lang="en">Senior management ensures that all necessary resources for the ISMS are readily available, ensuring effective implementation and maintenance of security measures.</skos:definition>
  </rdf:Description>
  
  <rdf:Description rdf:about="https://jenni0608.github.io/NIS2V-Vocabulary-Repository/assessment_ontology_final/MCQ64AnswerA">
    <rdf:type rdf:resource="https://jenni0608.github.io/NIS2V-Vocabulary-Repository/assessment_ontology_final/MCQ.64"/>
    <skos:prefLabel xml:lang="en">MCQ.64 - Not Implemented</skos:prefLabel>
    <skos:altLabel xml:lang="en">(i)</skos:altLabel>
    <skos:definition xml:lang="en">Configurations, including security configurations, of hardware, software, services, and networks are not established, documented, implemented, monitored, or reviewed, leading to potential vulnerabilities and inconsistent security measures.</skos:definition>
  </rdf:Description>
  
  <rdf:Description rdf:about="https://jenni0608.github.io/NIS2V-Vocabulary-Repository/assessment_ontology_final/MCQ5.1AnswerA">
    <rdf:type rdf:resource="https://jenni0608.github.io/NIS2V-Vocabulary-Repository/assessment_ontology_final/MCQ.5.1"/>
    <skos:prefLabel xml:lang="en">MCQ.5.1 - Not Implemented</skos:prefLabel>
    <skos:altLabel xml:lang="en">(i)</skos:altLabel>
    <skos:definition xml:lang="en">The organisation does not select appropriate information security risk treatment options, potentially leading to ineffective risk mitigation strategies and heightened security risks.</skos:definition>
  </rdf:Description>
  
  <rdf:Description rdf:about="https://jenni0608.github.io/NIS2V-Vocabulary-Repository/assessment_ontology_final/MCQ46AnswerB">
    <rdf:type rdf:resource="https://jenni0608.github.io/NIS2V-Vocabulary-Repository/assessment_ontology_final/MCQ.46"/>
    <skos:prefLabel xml:lang="en">MCQ.46 - Partially Implemented</skos:prefLabel>
    <skos:altLabel xml:lang="en">(ii)</skos:altLabel>
    <skos:definition xml:lang="en">There are some efforts to identify, implement, and monitor security mechanisms, service levels, and service requirements of network services, but the process may be inconsistent or incomplete.</skos:definition>
  </rdf:Description>
  
  <rdf:Description rdf:about="https://jenni0608.github.io/NIS2V-Vocabulary-Repository/assessment_ontology_final/MCQ53AnswerC">
    <rdf:type rdf:resource="https://jenni0608.github.io/NIS2V-Vocabulary-Repository/assessment_ontology_final/MCQ.53"/>
    <skos:prefLabel xml:lang="en">MCQ.53 - Fully Implemented</skos:prefLabel>
    <skos:altLabel xml:lang="en">(iii)</skos:altLabel>
    <skos:definition xml:lang="en">Compliance with the organisation’s information security policy, topic-specific policies, rules, and standards is regularly reviewed, ensuring adherence to security requirements and prompt identification and resolution of any non-compliance issues.</skos:definition>
  </rdf:Description>
  
  <rdf:Description rdf:about="https://jenni0608.github.io/NIS2V-Vocabulary-Repository/assessment_ontology_final/MCQ60AnswerC">
    <rdf:type rdf:resource="https://jenni0608.github.io/NIS2V-Vocabulary-Repository/assessment_ontology_final/MCQ.60"/>
    <skos:prefLabel xml:lang="en">MCQ.60 - Fully Implemented</skos:prefLabel>
    <skos:altLabel xml:lang="en">(iii)</skos:altLabel>
    <skos:definition xml:lang="en">The organisation has implemented a comprehensive approach to managing the complete life cycle of identities, covering all activities from creation to termination, and ensuring robust oversight and control over identity-related processes.</skos:definition>
  </rdf:Description>
  
  <rdf:Description rdf:about="https://jenni0608.github.io/NIS2V-Vocabulary-Repository/assessment_ontology_final/MCQ6.1AnswerC">
    <rdf:type rdf:resource="https://jenni0608.github.io/NIS2V-Vocabulary-Repository/assessment_ontology_final/MCQ.6.1"/>
    <skos:prefLabel xml:lang="en">MCQ.6.1 - Fully Implemented</skos:prefLabel>
    <skos:altLabel xml:lang="en">(iii)</skos:altLabel>
    <skos:definition xml:lang="en">The organisation has a clear understanding of what information to communicate regarding the information security management system, covering all relevant aspects.</skos:definition>
  </rdf:Description>
  
  <rdf:Description rdf:about="https://jenni0608.github.io/NIS2V-Vocabulary-Repository/assessment_ontology_final/MCQ93AnswerA">
    <rdf:type rdf:resource="https://jenni0608.github.io/NIS2V-Vocabulary-Repository/assessment_ontology_final/MCQ.93"/>
    <skos:prefLabel xml:lang="en">MCQ.93 - Not Implemented</skos:prefLabel>
    <skos:altLabel xml:lang="en">(i)</skos:altLabel>
    <skos:definition xml:lang="en">Items of equipment containing storage media are not verified to ensure that sensitive data and licensed software have been removed or securely overwritten prior to disposal or re-use, potentially resulting in data breaches or unauthorized use of software.</skos:definition>
  </rdf:Description>
  
  <rdf:Description rdf:about="https://jenni0608.github.io/NIS2V-Vocabulary-Repository/assessment_ontology_final/MCQ40AnswerC">
    <rdf:type rdf:resource="https://jenni0608.github.io/NIS2V-Vocabulary-Repository/assessment_ontology_final/MCQ.40"/>
    <skos:prefLabel xml:lang="en">MCQ.40 - Fully Implemented</skos:prefLabel>
    <skos:altLabel xml:lang="en">(iii)</skos:altLabel>
    <skos:definition xml:lang="en">Processes are fully established for the acquisition, use, management, and exit from cloud services in accordance with the organisation’s information security requirements, ensuring secure and compliant adoption and operation of cloud services.</skos:definition>
  </rdf:Description>
  
  <rdf:Description rdf:about="https://jenni0608.github.io/NIS2V-Vocabulary-Repository/assessment_ontology_final/MCQ65AnswerC">
    <rdf:type rdf:resource="https://jenni0608.github.io/NIS2V-Vocabulary-Repository/assessment_ontology_final/MCQ.65"/>
    <skos:prefLabel xml:lang="en">MCQ.65 - Fully Implemented</skos:prefLabel>
    <skos:altLabel xml:lang="en">(iii)</skos:altLabel>
    <skos:definition xml:lang="en">Protection against malware is fully implemented and supported by appropriate user awareness measures, ensuring proactive defence against malware threats and enhancing overall security posture.</skos:definition>
  </rdf:Description>
  
  <rdf:Description rdf:about="https://jenni0608.github.io/NIS2V-Vocabulary-Repository/assessment_ontology_final/MCQ66AnswerC">
    <rdf:type rdf:resource="https://jenni0608.github.io/NIS2V-Vocabulary-Repository/assessment_ontology_final/MCQ.66"/>
    <skos:prefLabel xml:lang="en">MCQ.66 - Fully Implemented</skos:prefLabel>
    <skos:altLabel xml:lang="en">(iii)</skos:altLabel>
    <skos:definition xml:lang="en">Procedures and measures to securely manage software installation on operational systems are fully implemented, ensuring that only authorized and secure software is installed, minimizing security risks.</skos:definition>
  </rdf:Description>
  
  <rdf:Description rdf:about="https://jenni0608.github.io/NIS2V-Vocabulary-Repository/assessment_ontology_final/MCQ22.5AnswerA">
    <rdf:type rdf:resource="https://jenni0608.github.io/NIS2V-Vocabulary-Repository/assessment_ontology_final/MCQ.22.5"/>
    <skos:prefLabel xml:lang="en">MCQ.22.5 - Not Implemented</skos:prefLabel>
    <skos:altLabel xml:lang="en">(i)</skos:altLabel>
    <skos:definition xml:lang="en">The information security policy is not available as documented information, communicated within the organisation, or made available to interested parties, potentially leading to confusion and lack of awareness regarding security expectations.</skos:definition>
  </rdf:Description>
  
  <rdf:Description rdf:about="https://jenni0608.github.io/NIS2V-Vocabulary-Repository/assessment_ontology_final/MCQ121AnswerB">
    <rdf:type rdf:resource="https://jenni0608.github.io/NIS2V-Vocabulary-Repository/assessment_ontology_final/MCQ.121"/>
    <skos:prefLabel xml:lang="en">MCQ.121 - Partially Implemented</skos:prefLabel>
    <skos:altLabel xml:lang="en">(ii)</skos:altLabel>
    <skos:definition xml:lang="en">Some change management procedures are implemented, but there are inconsistencies or gaps in their application to information processing facilities and information systems, resulting in limited effectiveness.</skos:definition>
  </rdf:Description>
  
  <rdf:Description rdf:about="https://jenni0608.github.io/NIS2V-Vocabulary-Repository/assessment_ontology_final/MCQ75.1AnswerC">
    <rdf:type rdf:resource="https://jenni0608.github.io/NIS2V-Vocabulary-Repository/assessment_ontology_final/MCQ.75.1"/>
    <skos:prefLabel xml:lang="en">MCQ.75.1 - Fully Implemented</skos:prefLabel>
    <skos:altLabel xml:lang="en">(iii)</skos:altLabel>
    <skos:definition xml:lang="en">Personnel of the organisation and relevant interested parties receive appropriate information security awareness, education, and training, ensuring they are well-equipped to understand and address security risks and threats effectively.</skos:definition>
  </rdf:Description>
  
  <rdf:Description rdf:about="https://jenni0608.github.io/NIS2V-Vocabulary-Repository/assessment_ontology_final/MCQ28AnswerC">
    <rdf:type rdf:resource="https://jenni0608.github.io/NIS2V-Vocabulary-Repository/assessment_ontology_final/MCQ.28"/>
    <skos:prefLabel xml:lang="en">MCQ.28 - Fully Implemented</skos:prefLabel>
    <skos:altLabel xml:lang="en">(iii)</skos:altLabel>
    <skos:definition xml:lang="en">ICT readiness is fully planned, implemented, maintained, and tested based on business continuity objectives and ICT continuity requirements, ensuring resilience and continuity of ICT services even during crises.</skos:definition>
  </rdf:Description>
  
  <rdf:Description rdf:about="https://jenni0608.github.io/NIS2V-Vocabulary-Repository/assessment_ontology_final/MCQ119AnswerA">
    <rdf:type rdf:resource="https://jenni0608.github.io/NIS2V-Vocabulary-Repository/assessment_ontology_final/MCQ.119"/>
    <skos:prefLabel xml:lang="en">MCQ.119 - Not Implemented</skos:prefLabel>
    <skos:altLabel xml:lang="en">(i)</skos:altLabel>
    <skos:definition xml:lang="en">Management does not require personnel to adhere to information security policies and procedures, leading to inconsistent or inadequate implementation of security measures.</skos:definition>
  </rdf:Description>
  
  <rdf:Description rdf:about="https://jenni0608.github.io/NIS2V-Vocabulary-Repository/assessment_ontology_final/MCQ61AnswerC">
    <rdf:type rdf:resource="https://jenni0608.github.io/NIS2V-Vocabulary-Repository/assessment_ontology_final/MCQ.61"/>
    <skos:prefLabel xml:lang="en">MCQ.61 - Fully Implemented</skos:prefLabel>
    <skos:altLabel xml:lang="en">(iii)</skos:altLabel>
    <skos:definition xml:lang="en">The allocation and management of authentication information are controlled by a robust management process, which includes advising personnel on the appropriate handling of authentication information, ensuring secure and effective management of authentication credentials.</skos:definition>
  </rdf:Description>
  
  <rdf:Description rdf:about="https://jenni0608.github.io/NIS2V-Vocabulary-Repository/assessment_ontology_final/MCQ52AnswerA">
    <rdf:type rdf:resource="https://jenni0608.github.io/NIS2V-Vocabulary-Repository/assessment_ontology_final/MCQ.52"/>
    <skos:prefLabel xml:lang="en">MCQ.52 - Not Implemented</skos:prefLabel>
    <skos:altLabel xml:lang="en">(i)</skos:altLabel>
    <skos:definition xml:lang="en">The organisation’s approach to managing information security and its implementation, including people, processes, and technologies, is not reviewed independently at planned intervals or when significant changes occur, potentially leading to outdated or inadequate security measures.</skos:definition>
  </rdf:Description>
  
  <rdf:Description rdf:about="https://jenni0608.github.io/NIS2V-Vocabulary-Repository/assessment_ontology_final/MCQ2.2AnswerA">
    <rdf:type rdf:resource="https://jenni0608.github.io/NIS2V-Vocabulary-Repository/assessment_ontology_final/MCQ.2.2"/>
    <skos:prefLabel xml:lang="en">MCQ.2.2 - Not Implemented</skos:prefLabel>
    <skos:altLabel xml:lang="en">(i)</skos:altLabel>
    <skos:definition xml:lang="en">The information security policy is not available as documented information, nor is it communicated within the organisation or made available to interested parties, potentially leading to lack of awareness and understanding of the organisation's information security objectives</skos:definition>
  </rdf:Description>
  
  <rdf:Description rdf:about="https://jenni0608.github.io/NIS2V-Vocabulary-Repository/assessment_ontology_final/MCQ92AnswerC">
    <rdf:type rdf:resource="https://jenni0608.github.io/NIS2V-Vocabulary-Repository/assessment_ontology_final/MCQ.92"/>
    <skos:prefLabel xml:lang="en">MCQ.92 - Fully Implemented</skos:prefLabel>
    <skos:altLabel xml:lang="en">(iii)</skos:altLabel>
    <skos:definition xml:lang="en">Storage media are fully managed through their life cycle of acquisition, use, transportation, and disposal in accordance with the organisation’s classification scheme and handling requirements, ensuring proper protection and handling of sensitive data at all stages.</skos:definition>
  </rdf:Description>
  
  <rdf:Description rdf:about="https://jenni0608.github.io/NIS2V-Vocabulary-Repository/assessment_ontology_final/MCQ107.2AnswerB">
    <rdf:type rdf:resource="https://jenni0608.github.io/NIS2V-Vocabulary-Repository/assessment_ontology_final/MCQ.107.2"/>
    <skos:prefLabel xml:lang="en">MCQ.107.2 - Partially Implemented</skos:prefLabel>
    <skos:altLabel xml:lang="en">(ii)</skos:altLabel>
    <skos:definition xml:lang="en">Some planning is done to achieve information security objectives, but there are inconsistencies or gaps in determining actions, resource requirements, responsibilities, timelines, and evaluation methods.</skos:definition>
  </rdf:Description>
  
  <rdf:Description rdf:about="https://jenni0608.github.io/NIS2V-Vocabulary-Repository/assessment_ontology_final/MCQ56.4AnswerA">
    <rdf:type rdf:resource="https://jenni0608.github.io/NIS2V-Vocabulary-Repository/assessment_ontology_final/MCQ.56.4"/>
    <skos:prefLabel xml:lang="en">MCQ.56.4 - Not Implemented</skos:prefLabel>
    <skos:altLabel xml:lang="en">(i)</skos:altLabel>
    <skos:definition xml:lang="en">There are no established methods or protocols for how to communicate information about the information security management system.</skos:definition>
  </rdf:Description>
  
  <rdf:Description rdf:about="https://jenni0608.github.io/NIS2V-Vocabulary-Repository/assessment_ontology_final/MCQ95AnswerA">
    <rdf:type rdf:resource="https://jenni0608.github.io/NIS2V-Vocabulary-Repository/assessment_ontology_final/MCQ.95"/>
    <skos:prefLabel xml:lang="en">MCQ.95 - Not Implemented</skos:prefLabel>
    <skos:altLabel xml:lang="en">(i)</skos:altLabel>
    <skos:definition xml:lang="en">Protection against malware is not implemented, and there are no appropriate user awareness measures in place, leaving the organisation vulnerable to malware attacks.</skos:definition>
  </rdf:Description>
  
  <rdf:Description rdf:about="https://jenni0608.github.io/NIS2V-Vocabulary-Repository/assessment_ontology_final/MCQ69AnswerC">
    <rdf:type rdf:resource="https://jenni0608.github.io/NIS2V-Vocabulary-Repository/assessment_ontology_final/MCQ.69"/>
    <skos:prefLabel xml:lang="en">MCQ.69 - Fully Implemented</skos:prefLabel>
    <skos:altLabel xml:lang="en">(iii)</skos:altLabel>
    <skos:definition xml:lang="en">Rules for the effective use of cryptography, including cryptographic key management, are fully defined and implemented, ensuring secure encryption practices and robust protection of sensitive information.</skos:definition>
  </rdf:Description>
  
  <rdf:Description rdf:about="https://jenni0608.github.io/NIS2V-Vocabulary-Repository/assessment_ontology_final/MCQ111AnswerA">
    <rdf:type rdf:resource="https://jenni0608.github.io/NIS2V-Vocabulary-Repository/assessment_ontology_final/MCQ.111"/>
    <skos:prefLabel xml:lang="en">MCQ.111 - Not Implemented</skos:prefLabel>
    <skos:altLabel xml:lang="en">(i)</skos:altLabel>
    <skos:definition xml:lang="en">Development, testing, and production environments are not separated or adequately secured, leading to potential risks such as unauthorized access or data breaches.</skos:definition>
  </rdf:Description>
  
  <rdf:Description rdf:about="https://jenni0608.github.io/NIS2V-Vocabulary-Repository/assessment_ontology_final/MCQ23AnswerA">
    <rdf:type rdf:resource="https://jenni0608.github.io/NIS2V-Vocabulary-Repository/assessment_ontology_final/MCQ.23"/>
    <skos:prefLabel xml:lang="en">MCQ.23 - Not Implemented</skos:prefLabel>
    <skos:altLabel xml:lang="en">(i)</skos:altLabel>
    <skos:definition xml:lang="en">The organisation does not provide a mechanism for personnel to report observed or suspected information security events through appropriate channels, hindering timely detection and response to security incidents.</skos:definition>
  </rdf:Description>
  
  <rdf:Description rdf:about="https://jenni0608.github.io/NIS2V-Vocabulary-Repository/assessment_ontology_final/MCQ46AnswerC">
    <rdf:type rdf:resource="https://jenni0608.github.io/NIS2V-Vocabulary-Repository/assessment_ontology_final/MCQ.46"/>
    <skos:prefLabel xml:lang="en">MCQ.46 - Fully Implemented</skos:prefLabel>
    <skos:altLabel xml:lang="en">(iii)</skos:altLabel>
    <skos:definition xml:lang="en">Security mechanisms, service levels, and service requirements of network services are fully identified, implemented, and monitored, ensuring adherence to security standards and optimal performance of network services.</skos:definition>
  </rdf:Description>
  
  <rdf:Description rdf:about="https://jenni0608.github.io/NIS2V-Vocabulary-Repository/assessment_ontology_final/MCQ124AnswerB">
    <rdf:type rdf:resource="https://jenni0608.github.io/NIS2V-Vocabulary-Repository/assessment_ontology_final/MCQ.124"/>
    <skos:prefLabel xml:lang="en">MCQ.124 - Partially Implemented</skos:prefLabel>
    <skos:altLabel xml:lang="en">(ii)</skos:altLabel>
    <skos:definition xml:lang="en">Some measures are in place to protect records, but there are inconsistencies or gaps in ensuring comprehensive protection from risks such as loss, destruction, falsification, unauthorized access, and unauthorized release.</skos:definition>
  </rdf:Description>
  
  <rdf:Description rdf:about="https://jenni0608.github.io/NIS2V-Vocabulary-Repository/assessment_ontology_final/MCQ105.2AnswerC">
    <rdf:type rdf:resource="https://jenni0608.github.io/NIS2V-Vocabulary-Repository/assessment_ontology_final/MCQ.105.2"/>
    <skos:prefLabel xml:lang="en">MCQ.105.2 - Fully Implemented</skos:prefLabel>
    <skos:altLabel xml:lang="en">(iii)</skos:altLabel>
    <skos:definition xml:lang="en">Senior management ensures the seamless integration of ISMS requirements into all organisational processes, ensuring alignment and effectiveness of security measures.</skos:definition>
  </rdf:Description>
  
  <rdf:Description rdf:about="https://jenni0608.github.io/NIS2V-Vocabulary-Repository/assessment_ontology_final/MCQ119AnswerC">
    <rdf:type rdf:resource="https://jenni0608.github.io/NIS2V-Vocabulary-Repository/assessment_ontology_final/MCQ.119"/>
    <skos:prefLabel xml:lang="en">MCQ.119 - Fully Implemented</skos:prefLabel>
    <skos:altLabel xml:lang="en">(iii)</skos:altLabel>
    <skos:definition xml:lang="en">Management systematically requires all personnel to apply information security in accordance with established policies and procedures. This ensures consistent adherence to security measures across the organisation.</skos:definition>
  </rdf:Description>
  
  <rdf:Description rdf:about="https://jenni0608.github.io/NIS2V-Vocabulary-Repository/assessment_ontology_final/MCQ22.5AnswerB">
    <rdf:type rdf:resource="https://jenni0608.github.io/NIS2V-Vocabulary-Repository/assessment_ontology_final/MCQ.22.5"/>
    <skos:prefLabel xml:lang="en">MCQ.22.5 - Partially Implemented</skos:prefLabel>
    <skos:altLabel xml:lang="en">(ii)</skos:altLabel>
    <skos:definition xml:lang="en">There are some efforts to make the information security policy available as documented information, communicate it within the organisation, and make it available to interested parties. However, these efforts may be inconsistent or not fully comprehensive.</skos:definition>
  </rdf:Description>
  
  <rdf:Description rdf:about="https://jenni0608.github.io/NIS2V-Vocabulary-Repository/assessment_ontology_final/MCQ34.1AnswerC">
    <rdf:type rdf:resource="https://jenni0608.github.io/NIS2V-Vocabulary-Repository/assessment_ontology_final/MCQ.34.1"/>
    <skos:prefLabel xml:lang="en">MCQ.34.1 - Fully Implemented</skos:prefLabel>
    <skos:altLabel xml:lang="en">(iii)</skos:altLabel>
    <skos:definition xml:lang="en">The organisation has fully planned, implemented, and controlled processes to meet requirements and implement actions by establishing criteria for the processes and implementing control. This ensures consistent and effective implementation of processes to meet organisational requirements.</skos:definition>
  </rdf:Description>
  
  <rdf:Description rdf:about="https://jenni0608.github.io/NIS2V-Vocabulary-Repository/assessment_ontology_final/MCQ34.2AnswerB">
    <rdf:type rdf:resource="https://jenni0608.github.io/NIS2V-Vocabulary-Repository/assessment_ontology_final/MCQ.34.2"/>
    <skos:prefLabel xml:lang="en">MCQ.34.2 - Partially Implemented</skos:prefLabel>
    <skos:altLabel xml:lang="en">(ii)</skos:altLabel>
    <skos:definition xml:lang="en">There are some efforts to make documented information available to the extent necessary to have confidence that the processes have been carried out as planned. However, documentation may be incomplete or not consistently maintained.</skos:definition>
  </rdf:Description>
  
  <rdf:Description rdf:about="https://jenni0608.github.io/NIS2V-Vocabulary-Repository/assessment_ontology_final/MCQ34.3AnswerC">
    <rdf:type rdf:resource="https://jenni0608.github.io/NIS2V-Vocabulary-Repository/assessment_ontology_final/MCQ.34.3"/>
    <skos:prefLabel xml:lang="en">MCQ.34.3 - Fully Implemented</skos:prefLabel>
    <skos:altLabel xml:lang="en">(iii)</skos:altLabel>
    <skos:definition xml:lang="en">The organisation fully controls planned changes and reviews the consequences of unintended changes, taking action to mitigate any adverse effects as necessary, ensuring effective change management and risk mitigation.</skos:definition>
  </rdf:Description>
  
  <rdf:Description rdf:about="https://jenni0608.github.io/NIS2V-Vocabulary-Repository/assessment_ontology_final/MCQ122AnswerA">
    <rdf:type rdf:resource="https://jenni0608.github.io/NIS2V-Vocabulary-Repository/assessment_ontology_final/MCQ.122"/>
    <skos:prefLabel xml:lang="en">MCQ.122 - Not Implemented</skos:prefLabel>
    <skos:altLabel xml:lang="en">(i)</skos:altLabel>
    <skos:definition xml:lang="en">The organisation lacks a structured approach to planning and agreement between testers and appropriate management for audit tests and assurance activities, leading to ineffective assessment of operational systems.</skos:definition>
  </rdf:Description>
  
  <rdf:Description rdf:about="https://jenni0608.github.io/NIS2V-Vocabulary-Repository/assessment_ontology_final/MCQ12AnswerA">
    <rdf:type rdf:resource="https://jenni0608.github.io/NIS2V-Vocabulary-Repository/assessment_ontology_final/MCQ.12"/>
    <skos:prefLabel xml:lang="en">MCQ.12 - Not Implemented</skos:prefLabel>
    <skos:altLabel xml:lang="en">(i)</skos:altLabel>
    <skos:definition xml:lang="en">Management does not enforce the application of information security in accordance with established policies and procedures, resulting in inconsistent adherence across personnel.</skos:definition>
  </rdf:Description>
  
  <rdf:Description rdf:about="https://jenni0608.github.io/NIS2V-Vocabulary-Repository/assessment_ontology_final/MCQ51AnswerB">
    <rdf:type rdf:resource="https://jenni0608.github.io/NIS2V-Vocabulary-Repository/assessment_ontology_final/MCQ.51"/>
    <skos:prefLabel xml:lang="en">MCQ.51 - Partially Implemented</skos:prefLabel>
    <skos:altLabel xml:lang="en">(ii)</skos:altLabel>
    <skos:definition xml:lang="en">There are some efforts by top management to conduct reviews of the organisation's information security management system at planned intervals, but the process may be inconsistent or not fully comprehensive in assessing suitability, adequacy, and effectiveness.</skos:definition>
  </rdf:Description>
  
  <rdf:Description rdf:about="https://jenni0608.github.io/NIS2V-Vocabulary-Repository/assessment_ontology_final/MCQ5.6AnswerC">
    <rdf:type rdf:resource="https://jenni0608.github.io/NIS2V-Vocabulary-Repository/assessment_ontology_final/MCQ.5.6"/>
    <skos:prefLabel xml:lang="en">MCQ.5.6 - Fully Implemented</skos:prefLabel>
    <skos:altLabel xml:lang="en">(iii)</skos:altLabel>
    <skos:definition xml:lang="en">The organisation fully retains documented information about the information security risk treatment process, ensuring that all steps and decisions taken in the risk treatment process are recorded and accessible for reference and audit purposes.</skos:definition>
  </rdf:Description>
  
  <rdf:Description rdf:about="https://jenni0608.github.io/NIS2V-Vocabulary-Repository/assessment_ontology_final/MCQ21AnswerC">
    <rdf:type rdf:resource="https://jenni0608.github.io/NIS2V-Vocabulary-Repository/assessment_ontology_final/MCQ.21"/>
    <skos:prefLabel xml:lang="en">MCQ.21 - Fully Implemented</skos:prefLabel>
    <skos:altLabel xml:lang="en">(iii)</skos:altLabel>
    <skos:definition xml:lang="en">Knowledge gained from information security incidents is actively and systematically utilized to strengthen and improve information security controls, facilitating continuous enhancement and adaptation to emerging threats.</skos:definition>
  </rdf:Description>
  
  <rdf:Description rdf:about="https://jenni0608.github.io/NIS2V-Vocabulary-Repository/assessment_ontology_final/MCQ113AnswerA">
    <rdf:type rdf:resource="https://jenni0608.github.io/NIS2V-Vocabulary-Repository/assessment_ontology_final/MCQ.113"/>
    <skos:prefLabel xml:lang="en">MCQ.113 - Not Implemented</skos:prefLabel>
    <skos:altLabel xml:lang="en">(i)</skos:altLabel>
    <skos:definition xml:lang="en">The organisation does not conduct internal audits at planned intervals to assess conformity to its own requirements and the requirements of relevant documents, nor does it evaluate the effectiveness of ISMS implementation and maintenance.</skos:definition>
  </rdf:Description>
  
  <rdf:Description rdf:about="https://jenni0608.github.io/NIS2V-Vocabulary-Repository/assessment_ontology_final/MCQ27AnswerB">
    <rdf:type rdf:resource="https://jenni0608.github.io/NIS2V-Vocabulary-Repository/assessment_ontology_final/MCQ.27"/>
    <skos:prefLabel xml:lang="en">MCQ.27 - Partially Implemented</skos:prefLabel>
    <skos:altLabel xml:lang="en">(ii)</skos:altLabel>
    <skos:definition xml:lang="en">There are some efforts to plan for maintaining information security during disruptions, but the plan may be incomplete or not fully comprehensive.</skos:definition>
  </rdf:Description>
  
  <rdf:Description rdf:about="https://jenni0608.github.io/NIS2V-Vocabulary-Repository/assessment_ontology_final/MCQ12AnswerB">
    <rdf:type rdf:resource="https://jenni0608.github.io/NIS2V-Vocabulary-Repository/assessment_ontology_final/MCQ.12"/>
    <skos:prefLabel xml:lang="en">MCQ.12 - Partially Implemented</skos:prefLabel>
    <skos:altLabel xml:lang="en">(ii)</skos:altLabel>
    <skos:definition xml:lang="en">There are some efforts by management to require personnel to apply information security in accordance with established policies and procedures, but enforcement may be inconsistent or lacking in certain areas.</skos:definition>
  </rdf:Description>
  
  <rdf:Description rdf:about="https://jenni0608.github.io/NIS2V-Vocabulary-Repository/assessment_ontology_final/MCQ59AnswerB">
    <rdf:type rdf:resource="https://jenni0608.github.io/NIS2V-Vocabulary-Repository/assessment_ontology_final/MCQ.59"/>
    <skos:prefLabel xml:lang="en">MCQ.59 - Partially Implemented</skos:prefLabel>
    <skos:altLabel xml:lang="en">(ii)</skos:altLabel>
    <skos:definition xml:lang="en">There are some rules established to control physical and logical access to information and associated assets, but they may not fully align with both business and information security requirements, leading to potential gaps in access control.</skos:definition>
  </rdf:Description>
  
  <rdf:Description rdf:about="https://jenni0608.github.io/NIS2V-Vocabulary-Repository/assessment_ontology_final/MCQ106.1AnswerB">
    <rdf:type rdf:resource="https://jenni0608.github.io/NIS2V-Vocabulary-Repository/assessment_ontology_final/MCQ.106.1"/>
    <skos:prefLabel xml:lang="en">MCQ.106.1 - Partially Implemented</skos:prefLabel>
    <skos:altLabel xml:lang="en">(ii)</skos:altLabel>
    <skos:definition xml:lang="en">Some consideration is given to the organisation's context and the needs and expectations of interested parties when planning for the ISMS, but there are inconsistencies or gaps in addressing risks and opportunities, resulting in partial alignment with ISMS objectives.</skos:definition>
  </rdf:Description>
  
  <rdf:Description rdf:about="https://jenni0608.github.io/NIS2V-Vocabulary-Repository/assessment_ontology_final/MCQ32AnswerA">
    <rdf:type rdf:resource="https://jenni0608.github.io/NIS2V-Vocabulary-Repository/assessment_ontology_final/MCQ.32"/>
    <skos:prefLabel xml:lang="en">MCQ.32 - Not Implemented</skos:prefLabel>
    <skos:altLabel xml:lang="en">(i)</skos:altLabel>
    <skos:definition xml:lang="en">Backup copies of information, software, and systems are not maintained, and there is no regular testing conducted in accordance with the agreed topic-specific policy on backup, posing risks of data loss and system downtime.</skos:definition>
  </rdf:Description>
  
  <rdf:Description rdf:about="https://jenni0608.github.io/NIS2V-Vocabulary-Repository/assessment_ontology_final/MCQ20AnswerC">
    <rdf:type rdf:resource="https://jenni0608.github.io/NIS2V-Vocabulary-Repository/assessment_ontology_final/MCQ.20"/>
    <skos:prefLabel xml:lang="en">MCQ.20 - Fully Implemented</skos:prefLabel>
    <skos:altLabel xml:lang="en">(iii)</skos:altLabel>
    <skos:definition xml:lang="en">Information security incidents are consistently responded to in strict accordance with documented procedures, ensuring a standardized and effective approach to incident response across the organisation.</skos:definition>
  </rdf:Description>
  
  <rdf:Description rdf:about="https://jenni0608.github.io/NIS2V-Vocabulary-Repository/assessment_ontology_final/MCQ123AnswerB">
    <rdf:type rdf:resource="https://jenni0608.github.io/NIS2V-Vocabulary-Repository/assessment_ontology_final/MCQ.123"/>
    <skos:prefLabel xml:lang="en">MCQ.123 - Partially Implemented</skos:prefLabel>
    <skos:altLabel xml:lang="en">(ii)</skos:altLabel>
    <skos:definition xml:lang="en">Some procedures are implemented to protect intellectual property rights, but there are inconsistencies or gaps in their effectiveness, potentially leaving intellectual property vulnerable.</skos:definition>
  </rdf:Description>
  
  <rdf:Description rdf:about="https://jenni0608.github.io/NIS2V-Vocabulary-Repository/assessment_ontology_final/MCQ69AnswerB">
    <rdf:type rdf:resource="https://jenni0608.github.io/NIS2V-Vocabulary-Repository/assessment_ontology_final/MCQ.69"/>
    <skos:prefLabel xml:lang="en">MCQ.69 - Partially Implemented</skos:prefLabel>
    <skos:altLabel xml:lang="en">(ii)</skos:altLabel>
    <skos:definition xml:lang="en">There are some efforts to define and implement rules for the effective use of cryptography, including cryptographic key management, but the implementation may be inconsistent or incomplete.</skos:definition>
  </rdf:Description>
  
  <rdf:Description rdf:about="https://jenni0608.github.io/NIS2V-Vocabulary-Repository/assessment_ontology_final/MCQ5.2AnswerC">
    <rdf:type rdf:resource="https://jenni0608.github.io/NIS2V-Vocabulary-Repository/assessment_ontology_final/MCQ.5.2"/>
    <skos:prefLabel xml:lang="en">MCQ.5.2 - Fully Implemented</skos:prefLabel>
    <skos:altLabel xml:lang="en">(iii)</skos:altLabel>
    <skos:definition xml:lang="en">The organisation fully determines all necessary controls to implement the chosen information security risk treatment option(s), whether by designing them as required or identifying them from any source, ensuring comprehensive coverage in the implementation process.</skos:definition>
  </rdf:Description>
  
  <rdf:Description rdf:about="https://jenni0608.github.io/NIS2V-Vocabulary-Repository/assessment_ontology_final/MCQ20AnswerA">
    <rdf:type rdf:resource="https://jenni0608.github.io/NIS2V-Vocabulary-Repository/assessment_ontology_final/MCQ.20"/>
    <skos:prefLabel xml:lang="en">MCQ.20 - Not Implemented</skos:prefLabel>
    <skos:altLabel xml:lang="en">(i)</skos:altLabel>
    <skos:definition xml:lang="en">Information security incidents are not responded to in accordance with documented procedures, as there are no established guidelines or protocols for incident response.</skos:definition>
  </rdf:Description>
  
  <rdf:Description rdf:about="https://jenni0608.github.io/NIS2V-Vocabulary-Repository/assessment_ontology_final/MCQ112.2AnswerC">
    <rdf:type rdf:resource="https://jenni0608.github.io/NIS2V-Vocabulary-Repository/assessment_ontology_final/MCQ.112.2"/>
    <skos:prefLabel xml:lang="en">MCQ.112.2 - Fully Implemented</skos:prefLabel>
    <skos:altLabel xml:lang="en">(iii)</skos:altLabel>
    <skos:definition xml:lang="en">The organisation systematically evaluates information security performance and ISMS effectiveness, ensuring a comprehensive understanding of the system's performance and effectiveness.</skos:definition>
  </rdf:Description>
  
  <rdf:Description rdf:about="https://jenni0608.github.io/NIS2V-Vocabulary-Repository/assessment_ontology_final/MCQ112.1AnswerB">
    <rdf:type rdf:resource="https://jenni0608.github.io/NIS2V-Vocabulary-Repository/assessment_ontology_final/MCQ.112.1"/>
    <skos:prefLabel xml:lang="en">MCQ.112.1 - Partially Implemented</skos:prefLabel>
    <skos:altLabel xml:lang="en">(ii)</skos:altLabel>
    <skos:definition xml:lang="en">Some effort is made to determine what needs to be monitored and measured, and methods for monitoring and measurement are partially established. However, there are inconsistencies or gaps in determining when and by whom monitoring and measurement should be performed, leading to incomplete or unreliable evidence of information security performance and ISMS effectiveness.</skos:definition>
  </rdf:Description>
  
  <rdf:Description rdf:about="https://jenni0608.github.io/NIS2V-Vocabulary-Repository/assessment_ontology_final/MCQ109AnswerA">
    <rdf:type rdf:resource="https://jenni0608.github.io/NIS2V-Vocabulary-Repository/assessment_ontology_final/MCQ.109"/>
    <skos:prefLabel xml:lang="en">MCQ.109 - Not Implemented</skos:prefLabel>
    <skos:altLabel xml:lang="en">(i)</skos:altLabel>
    <skos:definition xml:lang="en">The organisation lacks systematic control over documented information, leading to its unavailability, unsuitability for use, and inadequate protection from loss of confidentiality, improper use, or loss of integrity.</skos:definition>
  </rdf:Description>
  
  <rdf:Description rdf:about="https://jenni0608.github.io/NIS2V-Vocabulary-Repository/assessment_ontology_final/MCQ113AnswerB">
    <rdf:type rdf:resource="https://jenni0608.github.io/NIS2V-Vocabulary-Repository/assessment_ontology_final/MCQ.113"/>
    <skos:prefLabel xml:lang="en">MCQ.113 - Partially Implemented</skos:prefLabel>
    <skos:altLabel xml:lang="en">(ii)</skos:altLabel>
    <skos:definition xml:lang="en">Some internal audits are conducted to assess conformity to the organisation's own requirements and relevant documents, as well as the effectiveness of ISMS implementation and maintenance. However, there are inconsistencies or gaps in the audit process.</skos:definition>
  </rdf:Description>
  
  <rdf:Description rdf:about="https://jenni0608.github.io/NIS2V-Vocabulary-Repository/assessment_ontology_final/MCQ93AnswerC">
    <rdf:type rdf:resource="https://jenni0608.github.io/NIS2V-Vocabulary-Repository/assessment_ontology_final/MCQ.93"/>
    <skos:prefLabel xml:lang="en">MCQ.93 - Fully Implemented</skos:prefLabel>
    <skos:altLabel xml:lang="en">(iii)</skos:altLabel>
    <skos:definition xml:lang="en">Items of equipment containing storage media are fully verified to ensure that any sensitive data and licensed software have been removed or securely overwritten prior to disposal or re-use, mitigating the risk of data breaches and unauthorized use of software.</skos:definition>
  </rdf:Description>
  
  <rdf:Description rdf:about="https://jenni0608.github.io/NIS2V-Vocabulary-Repository/assessment_ontology_final/MCQ4.2AnswerA">
    <rdf:type rdf:resource="https://jenni0608.github.io/NIS2V-Vocabulary-Repository/assessment_ontology_final/MCQ.4.2"/>
    <skos:prefLabel xml:lang="en">MCQ.4.2 - Not Implemented</skos:prefLabel>
    <skos:altLabel xml:lang="en">(i)</skos:altLabel>
    <skos:definition xml:lang="en">The organisation does not ensure that repeated information security risk assessments produce consistent, valid, and comparable results, potentially leading to unreliable risk management decisions.</skos:definition>
  </rdf:Description>
  
  <rdf:Description rdf:about="https://jenni0608.github.io/NIS2V-Vocabulary-Repository/assessment_ontology_final/MCQ5.4AnswerC">
    <rdf:type rdf:resource="https://jenni0608.github.io/NIS2V-Vocabulary-Repository/assessment_ontology_final/MCQ.5.4"/>
    <skos:prefLabel xml:lang="en">MCQ.5.4 - Fully Implemented</skos:prefLabel>
    <skos:altLabel xml:lang="en">(iii)</skos:altLabel>
    <skos:definition xml:lang="en">The organisation fully formulates an information security risk treatment plan, ensuring that all identified risks are adequately addressed and mitigated in a systematic and comprehensive manner.</skos:definition>
  </rdf:Description>
  
  <rdf:Description rdf:about="https://jenni0608.github.io/NIS2V-Vocabulary-Repository/assessment_ontology_final/MCQ4.4AnswerC">
    <rdf:type rdf:resource="https://jenni0608.github.io/NIS2V-Vocabulary-Repository/assessment_ontology_final/MCQ.4.4"/>
    <skos:prefLabel xml:lang="en">MCQ.4.4 - Fully Implemented</skos:prefLabel>
    <skos:altLabel xml:lang="en">(iii)</skos:altLabel>
    <skos:definition xml:lang="en">The organisation fully analyses information security risks, effectively assessing the potential consequences and realistic likelihood of the occurrence of identified risks, and determining the levels of risk comprehensively.</skos:definition>
  </rdf:Description>
  
  <rdf:Description rdf:about="https://jenni0608.github.io/NIS2V-Vocabulary-Repository/assessment_ontology_final/MCQ3.2AnswerC">
    <rdf:type rdf:resource="https://jenni0608.github.io/NIS2V-Vocabulary-Repository/assessment_ontology_final/MCQ.3.2"/>
    <skos:prefLabel xml:lang="en">MCQ.3.2 - Fully Implemented</skos:prefLabel>
    <skos:altLabel xml:lang="en">(iii)</skos:altLabel>
    <skos:definition xml:lang="en">Top management fully assigns the responsibility and authority for ensuring that the information security management system conforms to its requirements, and designates responsibility for reporting on its performance to top management, ensuring effective oversight and accountability for information security management.</skos:definition>
  </rdf:Description>
  
  <rdf:Description rdf:about="https://jenni0608.github.io/NIS2V-Vocabulary-Repository/assessment_ontology_final/MCQ57.2AnswerA">
    <rdf:type rdf:resource="https://jenni0608.github.io/NIS2V-Vocabulary-Repository/assessment_ontology_final/MCQ.57.2"/>
    <skos:prefLabel xml:lang="en">MCQ.57.2 - Not Implemented</skos:prefLabel>
    <skos:altLabel xml:lang="en">(i)</skos:altLabel>
    <skos:definition xml:lang="en">Regular updates of the organisation's information security policy, topic-specific policies, and procedures relevant to their job function are not provided to personnel of the organisation and relevant interested parties, potentially leading to outdated practices and non-compliance with security standards.</skos:definition>
  </rdf:Description>
  
  <rdf:Description rdf:about="https://jenni0608.github.io/NIS2V-Vocabulary-Repository/assessment_ontology_final/MCQ50AnswerC">
    <rdf:type rdf:resource="https://jenni0608.github.io/NIS2V-Vocabulary-Repository/assessment_ontology_final/MCQ.50"/>
    <skos:prefLabel xml:lang="en">MCQ.50 - Fully Implemented</skos:prefLabel>
    <skos:altLabel xml:lang="en">(iii)</skos:altLabel>
    <skos:definition xml:lang="en">The organisation conducts internal audits at planned intervals to determine conformity to its own requirements and the requirements outlined in the document, ensuring effective implementation and maintenance of the information security management system.</skos:definition>
  </rdf:Description>
  
  <rdf:Description rdf:about="https://jenni0608.github.io/NIS2V-Vocabulary-Repository/assessment_ontology_final/MCQ50AnswerB">
    <rdf:type rdf:resource="https://jenni0608.github.io/NIS2V-Vocabulary-Repository/assessment_ontology_final/MCQ.50"/>
    <skos:prefLabel xml:lang="en">MCQ.50 - Partially Implemented</skos:prefLabel>
    <skos:altLabel xml:lang="en">(ii)</skos:altLabel>
    <skos:definition xml:lang="en">There are some efforts to conduct internal audits at planned intervals, but the process may be inconsistent or not fully comprehensive in assessing conformity to both the organisation’s own requirements and the requirements outlined in the document.</skos:definition>
  </rdf:Description>
  
  <rdf:Description rdf:about="https://jenni0608.github.io/NIS2V-Vocabulary-Repository/assessment_ontology_final/MCQ5.2AnswerA">
    <rdf:type rdf:resource="https://jenni0608.github.io/NIS2V-Vocabulary-Repository/assessment_ontology_final/MCQ.5.2"/>
    <skos:prefLabel xml:lang="en">MCQ.5.2 - Not Implemented</skos:prefLabel>
    <skos:altLabel xml:lang="en">(i)</skos:altLabel>
    <skos:definition xml:lang="en">The organisation does not determine all necessary controls to implement the chosen information security risk treatment option(s), potentially leaving gaps in the implementation process and compromising the effectiveness of risk mitigation.</skos:definition>
  </rdf:Description>
  
  <rdf:Description rdf:about="https://jenni0608.github.io/NIS2V-Vocabulary-Repository/assessment_ontology_final/MCQ81AnswerC">
    <rdf:type rdf:resource="https://jenni0608.github.io/NIS2V-Vocabulary-Repository/assessment_ontology_final/MCQ.81"/>
    <skos:prefLabel xml:lang="en">MCQ.81 - Fully Implemented</skos:prefLabel>
    <skos:altLabel xml:lang="en">(iii)</skos:altLabel>
    <skos:definition xml:lang="en">The organisation has implemented a comprehensive approach to managing the complete life cycle of identities, covering all activities from creation to termination, and ensuring robust oversight and control over identity-related processes.</skos:definition>
  </rdf:Description>
  
  <rdf:Description rdf:about="https://jenni0608.github.io/NIS2V-Vocabulary-Repository/assessment_ontology_final/MCQ96AnswerB">
    <rdf:type rdf:resource="https://jenni0608.github.io/NIS2V-Vocabulary-Repository/assessment_ontology_final/MCQ.96"/>
    <skos:prefLabel xml:lang="en">MCQ.96 - Partially Implemented</skos:prefLabel>
    <skos:altLabel xml:lang="en">(ii)</skos:altLabel>
    <skos:definition xml:lang="en">There are some efforts to establish information transfer rules, procedures, or agreements, but they may not cover all types of transfer facilities within the organisation and between the organisation and other parties, or the implementation may be inconsistent.</skos:definition>
  </rdf:Description>
  
  <rdf:Description rdf:about="https://jenni0608.github.io/NIS2V-Vocabulary-Repository/assessment_ontology_final/MCQ48AnswerC">
    <rdf:type rdf:resource="https://jenni0608.github.io/NIS2V-Vocabulary-Repository/assessment_ontology_final/MCQ.48"/>
    <skos:prefLabel xml:lang="en">MCQ.48 - Fully Implemented</skos:prefLabel>
    <skos:altLabel xml:lang="en">(iii)</skos:altLabel>
    <skos:definition xml:lang="en">Access to external websites is fully managed to reduce exposure to malicious content, ensuring proactive protection against cyber threats and maintaining a secure browsing environment for users.</skos:definition>
  </rdf:Description>
  
  <rdf:Description rdf:about="https://jenni0608.github.io/NIS2V-Vocabulary-Repository/assessment_ontology_final/MCQ36AnswerC">
    <rdf:type rdf:resource="https://jenni0608.github.io/NIS2V-Vocabulary-Repository/assessment_ontology_final/MCQ.36"/>
    <skos:prefLabel xml:lang="en">MCQ.36 - Fully Implemented</skos:prefLabel>
    <skos:altLabel xml:lang="en">(iii)</skos:altLabel>
    <skos:definition xml:lang="en">Processes and procedures are fully defined and implemented to manage information security risks associated with the use of supplier’s products or services, ensuring thorough assessment and mitigation of potential risks from third-party sources.</skos:definition>
  </rdf:Description>
  
  <rdf:Description rdf:about="https://jenni0608.github.io/NIS2V-Vocabulary-Repository/assessment_ontology_final/MCQ4.6AnswerB">
    <rdf:type rdf:resource="https://jenni0608.github.io/NIS2V-Vocabulary-Repository/assessment_ontology_final/MCQ.4.6"/>
    <skos:prefLabel xml:lang="en">MCQ.4.6 - Partially Implemented</skos:prefLabel>
    <skos:altLabel xml:lang="en">(ii)</skos:altLabel>
    <skos:definition xml:lang="en">There are some efforts by the organisation to retain documented information about the information security risk assessment process. However, the documentation may be incomplete or not consistently maintained.</skos:definition>
  </rdf:Description>
  
  <rdf:Description rdf:about="https://jenni0608.github.io/NIS2V-Vocabulary-Repository/assessment_ontology_final/MCQ83AnswerA">
    <rdf:type rdf:resource="https://jenni0608.github.io/NIS2V-Vocabulary-Repository/assessment_ontology_final/MCQ.83"/>
    <skos:prefLabel xml:lang="en">MCQ.83 - Not Implemented</skos:prefLabel>
    <skos:altLabel xml:lang="en">(i)</skos:altLabel>
    <skos:definition xml:lang="en">Access rights to information and associated assets are not provisioned, reviewed, modified, or removed in accordance with the organisation’s topic-specific policy and rules for access control, leading to inconsistencies and potential security risks.</skos:definition>
  </rdf:Description>
  
  <rdf:Description rdf:about="https://jenni0608.github.io/NIS2V-Vocabulary-Repository/assessment_ontology_final/MCQ125AnswerA">
    <rdf:type rdf:resource="https://jenni0608.github.io/NIS2V-Vocabulary-Repository/assessment_ontology_final/MCQ.125"/>
    <skos:prefLabel xml:lang="en">MCQ.125 - Not Implemented</skos:prefLabel>
    <skos:altLabel xml:lang="en">(i)</skos:altLabel>
    <skos:definition xml:lang="en">The organisation lacks established rules for secure development of software and systems, leading to potential security vulnerabilities.</skos:definition>
  </rdf:Description>
  
  <rdf:Description rdf:about="https://jenni0608.github.io/NIS2V-Vocabulary-Repository/assessment_ontology_final/MCQ18AnswerB">
    <rdf:type rdf:resource="https://jenni0608.github.io/NIS2V-Vocabulary-Repository/assessment_ontology_final/MCQ.18"/>
    <skos:prefLabel xml:lang="en">MCQ.18 - Partially Implemented</skos:prefLabel>
    <skos:altLabel xml:lang="en">(ii)</skos:altLabel>
    <skos:definition xml:lang="en">There are some efforts to define and establish information security incident management processes, roles, and responsibilities, but communication regarding incident management preparedness may be inconsistent or incomplete.</skos:definition>
  </rdf:Description>
  
  <rdf:Description rdf:about="https://jenni0608.github.io/NIS2V-Vocabulary-Repository/assessment_ontology_final/MCQ108AnswerC">
    <rdf:type rdf:resource="https://jenni0608.github.io/NIS2V-Vocabulary-Repository/assessment_ontology_final/MCQ.108"/>
    <skos:prefLabel xml:lang="en">MCQ.108 - Fully Implemented</skos:prefLabel>
    <skos:altLabel xml:lang="en">(iii)</skos:altLabel>
    <skos:definition xml:lang="en">The organisation systematically determines and provides the necessary resources for the establishment, implementation, maintenance, and continual improvement of the ISMS, ensuring effective security management aligned with organisational objectives.</skos:definition>
  </rdf:Description>
  
  <rdf:Description rdf:about="https://jenni0608.github.io/NIS2V-Vocabulary-Repository/assessment_ontology_final/MCQ64AnswerB">
    <rdf:type rdf:resource="https://jenni0608.github.io/NIS2V-Vocabulary-Repository/assessment_ontology_final/MCQ.64"/>
    <skos:prefLabel xml:lang="en">MCQ.64 - Partially Implemented</skos:prefLabel>
    <skos:altLabel xml:lang="en">(ii)</skos:altLabel>
    <skos:definition xml:lang="en">There are some efforts to establish, document, implement, monitor, and review configurations, including security configurations, of hardware, software, services, and networks, but the process may be inconsistent or incomplete.</skos:definition>
  </rdf:Description>
  
  <rdf:Description rdf:about="https://jenni0608.github.io/NIS2V-Vocabulary-Repository/assessment_ontology_final/MCQ56.3AnswerB">
    <rdf:type rdf:resource="https://jenni0608.github.io/NIS2V-Vocabulary-Repository/assessment_ontology_final/MCQ.56.3"/>
    <skos:prefLabel xml:lang="en">MCQ.56.3 - Partially Implemented</skos:prefLabel>
    <skos:altLabel xml:lang="en">(ii)</skos:altLabel>
    <skos:definition xml:lang="en">The organisation identifies some stakeholders with whom to communicate regarding the information security management system, but there may be gaps in coverage.</skos:definition>
  </rdf:Description>
  
  <rdf:Description rdf:about="https://jenni0608.github.io/NIS2V-Vocabulary-Repository/assessment_ontology_final/MCQ55.2AnswerC">
    <rdf:type rdf:resource="https://jenni0608.github.io/NIS2V-Vocabulary-Repository/assessment_ontology_final/MCQ.55.2"/>
    <skos:prefLabel xml:lang="en">MCQ.55.2 - Fully Implemented</skos:prefLabel>
    <skos:altLabel xml:lang="en">(iii)</skos:altLabel>
    <skos:definition xml:lang="en">Persons doing work under the organisation’s control are fully aware of their contribution to the effectiveness of the information security management system and the benefits of improved information security performance, fostering a culture of security awareness and proactive engagement in security initiatives.</skos:definition>
  </rdf:Description>
  
  <rdf:Description rdf:about="https://jenni0608.github.io/NIS2V-Vocabulary-Repository/assessment_ontology_final/MCQ56.4AnswerB">
    <rdf:type rdf:resource="https://jenni0608.github.io/NIS2V-Vocabulary-Repository/assessment_ontology_final/MCQ.56.4"/>
    <skos:prefLabel xml:lang="en">MCQ.56.4 - Partially Implemented</skos:prefLabel>
    <skos:altLabel xml:lang="en">(ii)</skos:altLabel>
    <skos:definition xml:lang="en">There are some established methods or protocols for how to communicate information about the information security management system, but they may not be consistently applied.</skos:definition>
  </rdf:Description>
  
  <rdf:Description rdf:about="https://jenni0608.github.io/NIS2V-Vocabulary-Repository/assessment_ontology_final/MCQ105.2AnswerA">
    <rdf:type rdf:resource="https://jenni0608.github.io/NIS2V-Vocabulary-Repository/assessment_ontology_final/MCQ.105.2"/>
    <skos:prefLabel xml:lang="en">MCQ.105.2 - Not Implemented</skos:prefLabel>
    <skos:altLabel xml:lang="en">(i)</skos:altLabel>
    <skos:definition xml:lang="en">Senior management does not ensure the integration of ISMS requirements into organisational processes, resulting in a lack of alignment between security measures and organisational activities.</skos:definition>
  </rdf:Description>
  
  <rdf:Description rdf:about="https://jenni0608.github.io/NIS2V-Vocabulary-Repository/assessment_ontology_final/MCQ128AnswerC">
    <rdf:type rdf:resource="https://jenni0608.github.io/NIS2V-Vocabulary-Repository/assessment_ontology_final/MCQ.128"/>
    <skos:prefLabel xml:lang="en">MCQ.128 - Fully Implemented</skos:prefLabel>
    <skos:altLabel xml:lang="en">(iii)</skos:altLabel>
    <skos:definition xml:lang="en">The organisation has established and consistently applies processes to direct, monitor, and review all activities related to outsourced system development. This ensures that outsourced projects are managed effectively and meet security and quality standards.</skos:definition>
  </rdf:Description>
  
  <rdf:Description rdf:about="https://jenni0608.github.io/NIS2V-Vocabulary-Repository/assessment_ontology_final/MCQ104AnswerA">
    <rdf:type rdf:resource="https://jenni0608.github.io/NIS2V-Vocabulary-Repository/assessment_ontology_final/MCQ.104"/>
    <skos:prefLabel xml:lang="en">MCQ.104 - Not Implemented</skos:prefLabel>
    <skos:altLabel xml:lang="en">(i)</skos:altLabel>
    <skos:definition xml:lang="en">The organisation lacks a structured approach to establish, implement, maintain, and improve its ISMS, resulting in inconsistent or ad-hoc security measures that do not meet the requirements outlined in ISO27001.</skos:definition>
  </rdf:Description>
  
  <rdf:Description rdf:about="https://jenni0608.github.io/NIS2V-Vocabulary-Repository/assessment_ontology_final/MCQ24AnswerB">
    <rdf:type rdf:resource="https://jenni0608.github.io/NIS2V-Vocabulary-Repository/assessment_ontology_final/MCQ.24"/>
    <skos:prefLabel xml:lang="en">MCQ.24 - Partially Implemented</skos:prefLabel>
    <skos:altLabel xml:lang="en">(ii)</skos:altLabel>
    <skos:definition xml:lang="en">There are some efforts to produce, store, protect, and analyse logs that record activities, exceptions, faults, and other relevant events, but the process may be inconsistent or incomplete.</skos:definition>
  </rdf:Description>
  
  <rdf:Description rdf:about="https://jenni0608.github.io/NIS2V-Vocabulary-Repository/assessment_ontology_final/MCQ38AnswerC">
    <rdf:type rdf:resource="https://jenni0608.github.io/NIS2V-Vocabulary-Repository/assessment_ontology_final/MCQ.38"/>
    <skos:prefLabel xml:lang="en">MCQ.38 - Fully Implemented</skos:prefLabel>
    <skos:altLabel xml:lang="en">(iii)</skos:altLabel>
    <skos:definition xml:lang="en">Processes and procedures are fully defined and implemented to manage information security risks associated with the ICT products and services supply chain, ensuring thorough assessment and mitigation of potential risks throughout the supply chain.</skos:definition>
  </rdf:Description>
  
  <rdf:Description rdf:about="https://jenni0608.github.io/NIS2V-Vocabulary-Repository/assessment_ontology_final/MCQ102AnswerB">
    <rdf:type rdf:resource="https://jenni0608.github.io/NIS2V-Vocabulary-Repository/assessment_ontology_final/MCQ.102"/>
    <skos:prefLabel xml:lang="en">MCQ.102 - Partially Implemented</skos:prefLabel>
    <skos:altLabel xml:lang="en">(ii)</skos:altLabel>
    <skos:definition xml:lang="en">Some interested parties and their requirements are identified, but there is inconsistency in the process, resulting in gaps in understanding and addressing all relevant requirements.</skos:definition>
  </rdf:Description>
  
  <rdf:Description rdf:about="https://jenni0608.github.io/NIS2V-Vocabulary-Repository/assessment_ontology_final/MCQ101AnswerC">
    <rdf:type rdf:resource="https://jenni0608.github.io/NIS2V-Vocabulary-Repository/assessment_ontology_final/MCQ.101"/>
    <skos:prefLabel xml:lang="en">MCQ.101 - Fully Implemented</skos:prefLabel>
    <skos:altLabel xml:lang="en">(iii)</skos:altLabel>
    <skos:definition xml:lang="en">The organisation actively identifies and assesses both external and internal issues relevant to its information security management system, ensuring a comprehensive understanding of potential impacts and enabling proactive measures to be taken to address them effectively.</skos:definition>
  </rdf:Description>
  
  <rdf:Description rdf:about="https://jenni0608.github.io/NIS2V-Vocabulary-Repository/assessment_ontology_final/MCQ103AnswerB">
    <rdf:type rdf:resource="https://jenni0608.github.io/NIS2V-Vocabulary-Repository/assessment_ontology_final/MCQ.103"/>
    <skos:prefLabel xml:lang="en">MCQ.103 - Partially Implemented</skos:prefLabel>
    <skos:altLabel xml:lang="en">(ii)</skos:altLabel>
    <skos:definition xml:lang="en">Some consideration is given to external and internal issues, requirements, and interfaces with other organisations, but there are inconsistencies or gaps in determining the scope, leading to potential ambiguity.</skos:definition>
  </rdf:Description>
  
  <rdf:Description rdf:about="https://jenni0608.github.io/NIS2V-Vocabulary-Repository/assessment_ontology_final/MCQ81AnswerB">
    <rdf:type rdf:resource="https://jenni0608.github.io/NIS2V-Vocabulary-Repository/assessment_ontology_final/MCQ.81"/>
    <skos:prefLabel xml:lang="en">MCQ.81 - Partially Implemented</skos:prefLabel>
    <skos:altLabel xml:lang="en">(ii)</skos:altLabel>
    <skos:definition xml:lang="en">There are some efforts to manage the complete life cycle of identities, including activities such as creation, authentication, and periodic reviews, but the process may be inconsistent or lacking in certain areas.</skos:definition>
  </rdf:Description>
  
  <rdf:Description rdf:about="https://jenni0608.github.io/NIS2V-Vocabulary-Repository/assessment_ontology_final/MCQ49.1AnswerC">
    <rdf:type rdf:resource="https://jenni0608.github.io/NIS2V-Vocabulary-Repository/assessment_ontology_final/MCQ.49.1"/>
    <skos:prefLabel xml:lang="en">MCQ.49.1 - Fully Implemented</skos:prefLabel>
    <skos:altLabel xml:lang="en">(iii)</skos:altLabel>
    <skos:definition xml:lang="en">The organisation fully determines what needs to be monitored and measured, including information security processes and controls, ensuring comprehensive oversight and assessment of security measures' effectiveness.</skos:definition>
  </rdf:Description>
  
  <rdf:Description rdf:about="https://jenni0608.github.io/NIS2V-Vocabulary-Repository/assessment_ontology_final/MCQ64AnswerC">
    <rdf:type rdf:resource="https://jenni0608.github.io/NIS2V-Vocabulary-Repository/assessment_ontology_final/MCQ.64"/>
    <skos:prefLabel xml:lang="en">MCQ.64 - Fully Implemented</skos:prefLabel>
    <skos:altLabel xml:lang="en">(iii)</skos:altLabel>
    <skos:definition xml:lang="en">Configurations, including security configurations, of hardware, software, services, and networks are fully established, documented, implemented, monitored, and reviewed, ensuring consistent and effective security measures across the organisation.</skos:definition>
  </rdf:Description>
  
  <rdf:Description rdf:about="https://jenni0608.github.io/NIS2V-Vocabulary-Repository/assessment_ontology_final/MCQ129AnswerA">
    <rdf:type rdf:resource="https://jenni0608.github.io/NIS2V-Vocabulary-Repository/assessment_ontology_final/MCQ.129"/>
    <skos:prefLabel xml:lang="en">MCQ.129 - Not Implemented</skos:prefLabel>
    <skos:altLabel xml:lang="en">(i)</skos:altLabel>
    <skos:definition xml:lang="en">The organisation lacks defined security perimeters to protect areas containing information and associated assets, leading to potential unauthorized access and security breaches.</skos:definition>
  </rdf:Description>
  
  <rdf:Description rdf:about="https://jenni0608.github.io/NIS2V-Vocabulary-Repository/assessment_ontology_final/MCQ105.1AnswerA">
    <rdf:type rdf:resource="https://jenni0608.github.io/NIS2V-Vocabulary-Repository/assessment_ontology_final/MCQ.105.1"/>
    <skos:prefLabel xml:lang="en">MCQ.105.1 - Not Implemented</skos:prefLabel>
    <skos:altLabel xml:lang="en">(i)</skos:altLabel>
    <skos:definition xml:lang="en">Senior management does not ensure the alignment of information security policy and objectives with the organisation's strategic direction, leading to disjointed goals.</skos:definition>
  </rdf:Description>
  
  <rdf:Description rdf:about="https://jenni0608.github.io/NIS2V-Vocabulary-Repository/assessment_ontology_final/MCQ3.2AnswerA">
    <rdf:type rdf:resource="https://jenni0608.github.io/NIS2V-Vocabulary-Repository/assessment_ontology_final/MCQ.3.2"/>
    <skos:prefLabel xml:lang="en">MCQ.3.2 - Not Implemented</skos:prefLabel>
    <skos:altLabel xml:lang="en">(i)</skos:altLabel>
    <skos:definition xml:lang="en">Top management does not assign the responsibility and authority for ensuring that the information security management system conforms to its requirements, nor does it designate responsibility for reporting on the performance of the information security management system to top management, potentially leading to a lack of oversight and accountability</skos:definition>
  </rdf:Description>
  
  <rdf:Description rdf:about="https://jenni0608.github.io/NIS2V-Vocabulary-Repository/assessment_ontology_final/MCQ122AnswerB">
    <rdf:type rdf:resource="https://jenni0608.github.io/NIS2V-Vocabulary-Repository/assessment_ontology_final/MCQ.122"/>
    <skos:prefLabel xml:lang="en">MCQ.122 - Partially Implemented</skos:prefLabel>
    <skos:altLabel xml:lang="en">(ii)</skos:altLabel>
    <skos:definition xml:lang="en">Some efforts are made to plan and agree upon audit tests and assurance activities, but there are inconsistencies or gaps in the process, resulting in limited effectiveness in assessing operational systems.</skos:definition>
  </rdf:Description>
  
  <rdf:Description rdf:about="https://jenni0608.github.io/NIS2V-Vocabulary-Repository/assessment_ontology_final/MCQ34.2AnswerA">
    <rdf:type rdf:resource="https://jenni0608.github.io/NIS2V-Vocabulary-Repository/assessment_ontology_final/MCQ.34.2"/>
    <skos:prefLabel xml:lang="en">MCQ.34.2 - Not Implemented</skos:prefLabel>
    <skos:altLabel xml:lang="en">(i)</skos:altLabel>
    <skos:definition xml:lang="en">Documented information necessary to have confidence that the processes have been carried out as planned is not available, potentially leading to uncertainty and lack of assurance in process implementation.</skos:definition>
  </rdf:Description>
  
  <rdf:Description rdf:about="https://jenni0608.github.io/NIS2V-Vocabulary-Repository/assessment_ontology_final/MCQ31AnswerC">
    <rdf:type rdf:resource="https://jenni0608.github.io/NIS2V-Vocabulary-Repository/assessment_ontology_final/MCQ.31"/>
    <skos:prefLabel xml:lang="en">MCQ.31 - Fully Implemented</skos:prefLabel>
    <skos:altLabel xml:lang="en">(iii)</skos:altLabel>
    <skos:definition xml:lang="en">The use of resources is consistently monitored, and adjustments are promptly made in line with current and expected capacity requirements, ensuring optimal resource utilization and performance.</skos:definition>
  </rdf:Description>
  
  <rdf:Description rdf:about="https://jenni0608.github.io/NIS2V-Vocabulary-Repository/assessment_ontology_final/MCQ1AnswerC">
    <rdf:type rdf:resource="https://jenni0608.github.io/NIS2V-Vocabulary-Repository/assessment_ontology_final/MCQ.1"/>
    <skos:prefLabel xml:lang="en">MCQ.1 - Fully Implemented</skos:prefLabel>
    <skos:altLabel xml:lang="en">(iii)</skos:altLabel>
    <skos:definition xml:lang="en">The organisation thoroughly identifies external and internal issues relevant to its purpose and assesses their impact on achieving the intended outcomes of its information security management system, ensuring proactive management and alignment with organisational goals.</skos:definition>
  </rdf:Description>
  
  <rdf:Description rdf:about="https://jenni0608.github.io/NIS2V-Vocabulary-Repository/assessment_ontology_final/MCQ67AnswerC">
    <rdf:type rdf:resource="https://jenni0608.github.io/NIS2V-Vocabulary-Repository/assessment_ontology_final/MCQ.67"/>
    <skos:prefLabel xml:lang="en">MCQ.67 - Fully Implemented</skos:prefLabel>
    <skos:altLabel xml:lang="en">(iii)</skos:altLabel>
    <skos:definition xml:lang="en">Segregation is fully implemented for groups of information services, users, and information systems within the organisation's networks, ensuring controlled access and minimizing the impact of security incidents.</skos:definition>
  </rdf:Description>
  
  <rdf:Description rdf:about="https://jenni0608.github.io/NIS2V-Vocabulary-Repository/assessment_ontology_final/MCQ57.2AnswerC">
    <rdf:type rdf:resource="https://jenni0608.github.io/NIS2V-Vocabulary-Repository/assessment_ontology_final/MCQ.57.2"/>
    <skos:prefLabel xml:lang="en">MCQ.57.2 - Fully Implemented</skos:prefLabel>
    <skos:altLabel xml:lang="en">(iii)</skos:altLabel>
    <skos:definition xml:lang="en">Regular updates of the organisation's information security policy, topic-specific policies, and procedures relevant to their job function are fully provided to personnel of the organisation and relevant interested parties, ensuring they are well-informed and compliant with current security standards.</skos:definition>
  </rdf:Description>
  
  <rdf:Description rdf:about="https://jenni0608.github.io/NIS2V-Vocabulary-Repository/assessment_ontology_final/MCQ5.2AnswerB">
    <rdf:type rdf:resource="https://jenni0608.github.io/NIS2V-Vocabulary-Repository/assessment_ontology_final/MCQ.5.2"/>
    <skos:prefLabel xml:lang="en">MCQ.5.2 - Partially Implemented</skos:prefLabel>
    <skos:altLabel xml:lang="en">(ii)</skos:altLabel>
    <skos:definition xml:lang="en">There are some efforts by the organisation to determine all necessary controls to implement the chosen information security risk treatment option(s), whether by designing them as required or identifying them from any source. However, the determination process may be inconsistent or not fully comprehensive.</skos:definition>
  </rdf:Description>
  
  <rdf:Description rdf:about="https://jenni0608.github.io/NIS2V-Vocabulary-Repository/assessment_ontology_final/MCQ85AnswerC">
    <rdf:type rdf:resource="https://jenni0608.github.io/NIS2V-Vocabulary-Repository/assessment_ontology_final/MCQ.85"/>
    <skos:prefLabel xml:lang="en">MCQ.85 - Fully Implemented</skos:prefLabel>
    <skos:altLabel xml:lang="en">(iii)</skos:altLabel>
    <skos:definition xml:lang="en">Access to information and other associated assets is fully restricted in accordance with the established topic-specific policy on access control, ensuring that access is granted only to authorized individuals based on defined criteria and requirements.</skos:definition>
  </rdf:Description>
  
  <rdf:Description rdf:about="https://jenni0608.github.io/NIS2V-Vocabulary-Repository/assessment_ontology_final/MCQ34.3AnswerA">
    <rdf:type rdf:resource="https://jenni0608.github.io/NIS2V-Vocabulary-Repository/assessment_ontology_final/MCQ.34.3"/>
    <skos:prefLabel xml:lang="en">MCQ.34.3 - Not Implemented</skos:prefLabel>
    <skos:altLabel xml:lang="en">(i)</skos:altLabel>
    <skos:definition xml:lang="en">The organisation does not control planned changes and review the consequences of unintended changes, potentially leading to unmanaged risks and adverse effects on operations.</skos:definition>
  </rdf:Description>
  
  <rdf:Description rdf:about="https://jenni0608.github.io/NIS2V-Vocabulary-Repository/assessment_ontology_final/MCQ12AnswerC">
    <rdf:type rdf:resource="https://jenni0608.github.io/NIS2V-Vocabulary-Repository/assessment_ontology_final/MCQ.12"/>
    <skos:prefLabel xml:lang="en">MCQ.12 - Fully Implemented</skos:prefLabel>
    <skos:altLabel xml:lang="en">(iii)</skos:altLabel>
    <skos:definition xml:lang="en">Management consistently requires all personnel to apply information security in strict accordance with the established information security policy, topic-specific policies, and procedures of the organisation, ensuring uniform adherence throughout the organisation.</skos:definition>
  </rdf:Description>
  
  <rdf:Description rdf:about="https://jenni0608.github.io/NIS2V-Vocabulary-Repository/assessment_ontology_final/MCQ5.6AnswerB">
    <rdf:type rdf:resource="https://jenni0608.github.io/NIS2V-Vocabulary-Repository/assessment_ontology_final/MCQ.5.6"/>
    <skos:prefLabel xml:lang="en">MCQ.5.6 - Partially Implemented</skos:prefLabel>
    <skos:altLabel xml:lang="en">(ii)</skos:altLabel>
    <skos:definition xml:lang="en">There are some efforts by the organisation to retain documented information about the information security risk treatment process. However, documentation may be incomplete or inconsistently maintained.</skos:definition>
  </rdf:Description>
  
  <rdf:Description rdf:about="https://jenni0608.github.io/NIS2V-Vocabulary-Repository/assessment_ontology_final/MCQ92AnswerA">
    <rdf:type rdf:resource="https://jenni0608.github.io/NIS2V-Vocabulary-Repository/assessment_ontology_final/MCQ.92"/>
    <skos:prefLabel xml:lang="en">MCQ.92 - Not Implemented</skos:prefLabel>
    <skos:altLabel xml:lang="en">(i)</skos:altLabel>
    <skos:definition xml:lang="en">Storage media are not managed through their life cycle of acquisition, use, transportation, and disposal in accordance with the organisation’s classification scheme and handling requirements, potentially leading to mishandling of sensitive data and security breaches.</skos:definition>
  </rdf:Description>
  
  <rdf:Description rdf:about="https://jenni0608.github.io/NIS2V-Vocabulary-Repository/assessment_ontology_final/MCQ37AnswerB">
    <rdf:type rdf:resource="https://jenni0608.github.io/NIS2V-Vocabulary-Repository/assessment_ontology_final/MCQ.37"/>
    <skos:prefLabel xml:lang="en">MCQ.37 - Partially Implemented</skos:prefLabel>
    <skos:altLabel xml:lang="en">(ii)</skos:altLabel>
    <skos:definition xml:lang="en">There are some efforts to establish and agree upon relevant information security requirements with suppliers based on the type of supplier relationship, but the process may be inconsistent or not fully comprehensive.</skos:definition>
  </rdf:Description>
  
  <rdf:Description rdf:about="https://jenni0608.github.io/NIS2V-Vocabulary-Repository/assessment_ontology_final/MCQ82AnswerA">
    <rdf:type rdf:resource="https://jenni0608.github.io/NIS2V-Vocabulary-Repository/assessment_ontology_final/MCQ.82"/>
    <skos:prefLabel xml:lang="en">MCQ.82 - Not Implemented</skos:prefLabel>
    <skos:altLabel xml:lang="en">(i)</skos:altLabel>
    <skos:definition xml:lang="en">There is no management process in place to control the allocation and management of authentication information, and personnel are not advised on the appropriate handling of authentication information.</skos:definition>
  </rdf:Description>
  
  <rdf:Description rdf:about="https://jenni0608.github.io/NIS2V-Vocabulary-Repository/assessment_ontology_final/MCQ67AnswerA">
    <rdf:type rdf:resource="https://jenni0608.github.io/NIS2V-Vocabulary-Repository/assessment_ontology_final/MCQ.67"/>
    <skos:prefLabel xml:lang="en">MCQ.67 - Not Implemented</skos:prefLabel>
    <skos:altLabel xml:lang="en">(i)</skos:altLabel>
    <skos:definition xml:lang="en">Segregation is not implemented for groups of information services, users, and information systems within the organisation's networks, leading to potential risks of unauthorized access and data breaches.</skos:definition>
  </rdf:Description>
  
  <rdf:Description rdf:about="https://jenni0608.github.io/NIS2V-Vocabulary-Repository/assessment_ontology_final/MCQ39AnswerA">
    <rdf:type rdf:resource="https://jenni0608.github.io/NIS2V-Vocabulary-Repository/assessment_ontology_final/MCQ.39"/>
    <skos:prefLabel xml:lang="en">MCQ.39 - Not Implemented</skos:prefLabel>
    <skos:altLabel xml:lang="en">(i)</skos:altLabel>
    <skos:definition xml:lang="en">The organisation does not regularly monitor, review, evaluate, or manage change in supplier information security practices and service delivery, potentially exposing the organisation to security risks from suppliers.</skos:definition>
  </rdf:Description>
  
  <rdf:Description rdf:about="https://jenni0608.github.io/NIS2V-Vocabulary-Repository/assessment_ontology_final/MCQ49.2AnswerB">
    <rdf:type rdf:resource="https://jenni0608.github.io/NIS2V-Vocabulary-Repository/assessment_ontology_final/MCQ.49.2"/>
    <skos:prefLabel xml:lang="en">MCQ.49.2 - Partially Implemented</skos:prefLabel>
    <skos:altLabel xml:lang="en">(ii)</skos:altLabel>
    <skos:definition xml:lang="en">There are some efforts by the organisation to establish methods for monitoring, measurement, analysis, and evaluation to ensure valid results. However, these methods may not be consistently applied or capable of producing fully comparable and reproducible results.</skos:definition>
  </rdf:Description>
  
  <rdf:Description rdf:about="https://jenni0608.github.io/NIS2V-Vocabulary-Repository/assessment_ontology_final/MCQ25AnswerB">
    <rdf:type rdf:resource="https://jenni0608.github.io/NIS2V-Vocabulary-Repository/assessment_ontology_final/MCQ.25"/>
    <skos:prefLabel xml:lang="en">MCQ.25 - Partially Implemented</skos:prefLabel>
    <skos:altLabel xml:lang="en">(ii)</skos:altLabel>
    <skos:definition xml:lang="en">There are some efforts to monitor networks, systems, and applications for anomalous behaviour, but the process of evaluating potential information security incidents may be inconsistent or incomplete.</skos:definition>
  </rdf:Description>
  
  <rdf:Description rdf:about="https://jenni0608.github.io/NIS2V-Vocabulary-Repository/assessment_ontology_final/MCQ90AnswerA">
    <rdf:type rdf:resource="https://jenni0608.github.io/NIS2V-Vocabulary-Repository/assessment_ontology_final/MCQ.90"/>
    <skos:prefLabel xml:lang="en">MCQ.90 - Not Implemented</skos:prefLabel>
    <skos:altLabel xml:lang="en">(i)</skos:altLabel>
    <skos:definition xml:lang="en">Rules for acceptable use and procedures for handling information and other associated assets are not identified, documented, or implemented, potentially leading to misuse or mishandling of sensitive information.</skos:definition>
  </rdf:Description>
	
	
	

